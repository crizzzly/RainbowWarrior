            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Helligkeit der LEDs steuern"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Bestimmt ist bereits aufgefallen, wie hell die LEDs leuchten können. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Diese Helligkeit kann man aber verändern! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Relativ einfach kann man mit Neopixel.brightness(brightness_value) bestimmen, wie hell die LEDs leuchten sollen. brightness_value kann hier einen Wert zwischen 0 und 255 annehmen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Gerne kann dies wieder in Repl ausprobiert werden! Allerdings empfehle ich, dies bei unteschiedlichen Lichtverhältnissen zu testen, um ein Gefühl dafür zu bekommen, wann welcher Wert passt."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; import machine, animations"/>
                <para PARENT="code"/>
                <ITEXT CH="&gt;&gt;&gt; animations.running_dot()"/>
                <para PARENT="code"/>
                <ITEXT CH="&gt;&gt;&gt; animations.strip.brightness(1) "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Helligkeit der LEDs steuern" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
