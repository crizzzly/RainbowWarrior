            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Der Fotowiderstand "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Selbstverständlich kann man diese Werte manuell definieren. Aber viel besser wäre es doch, wenn dies automatisch, abhängig von der Umgebungshelligkeit, geschehen würde - so wie es auch bei Smartphones passiert!"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hierzu nutzt man einen Photowiderstand. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Ein Photowiderstand ist ein Halbleiter, dessen Widerstand abhängig von der Umgebungshelligkeit ist. Er wird auch LDR (=Light Dependent Resistor) genannt. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Er wird oft als Beleuchtungsstärkemesser, Flammenwächter, Dämmerungsschalter und als Sensor in Lichtschranken verwendet. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Fotowiderstand" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="180" YPOS="24769.4910253998" OwnPage="55" ItemID="1692896297" PTYPE="4" WIDTH="475.28" HEIGHT="243.428974600189" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 243.429 L0 243.429 L0 0 Z" copath="M0 0 L475.28 0 L475.28 243.429 L0 243.429 L0 0 Z" gXpos="180" gYpos="17714.3710253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Helligkeit der LEDs steuern"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Bestimmt ist bereits aufgefallen, wie hell die LEDs leuchten können. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Diese Helligkeit kann man aber verändern! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Relativ einfach kann man mit Neopixel.brightness(brightness_value) bestimmen, wie hell die LEDs leuchten sollen. brightness_value kann hier einen Wert zwischen 0 und 255 annehmen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Gerne kann dies wieder in Repl ausprobiert werden! Allerdings empfehle ich, dies bei unteschiedlichen Lichtverhältnissen zu testen, um ein Gefühl dafür zu bekommen, wann welcher Wert passt."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; import machine, animations"/>
                <para PARENT="code"/>
                <ITEXT CH="&gt;&gt;&gt; animations.running_dot()"/>
                <para PARENT="code"/>
                <ITEXT CH="&gt;&gt;&gt; animations.strip.brightness(1) "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Helligkeit der LEDs steuern" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.28" YPOS="24769.4910253998" OwnPage="56" ItemID="1692482729" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800565" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="735.28" gYpos="24769.4910253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1695334201" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Analog Digital Converter - das ADC Modul"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um den Photowiderstand am ESP einzusetzen muss man wissen, dass der ESP grundsätzlich eigentlich nur digitale Ein- und Ausgänge hat. In der Regel können diese nur Binärcode ausgeben und entgegennehmen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Allerdings besitzt der ESP auch einen Analog-Digital-Wandler, auch ADC (=Analog Digital Converter) genannt. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Den ADC findet man - wie alles, was mit Pins zu tun hat - im machine Modul:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="https://github.com/loboris/MicroPython_ESP32_psRAM_LoBo/wiki/adc"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die Modulbeschreibung sieht relativ komplex aus für Menschen, die mit Elektrotechnik nicht viel zu tun haben. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Aber wir setzen hier nur die nötigsten Bausteinett ein, um die Umgebungshelligkeit zu messen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu beachten ist allerdings, dass es zwei Arten von ADC gibt. ADC1 und ADC2. Jeweils auf verschiedenen Pins. ADC2 kann man allerdings nur nutzen, wenn WLAN nicht aktiv ist. Da wir die WLAN Funktion später noch brauchen werden, nutzen wir ADC1."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dieser ist auf den Pins 32 - 38. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="32, 33, 34, 35, 36, 37, 38, 38"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Eine Instanz des ADC bilden wir mit "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="ldr = machine.LDR(pin[, unit])"/>
                <ITEXT CH=", wobei unit standardgemäß 1 (für ADC1) ist. Daher muss dies nicht angegeben werden."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="ldr_val = lrd.read()"/>
                <ITEXT CH=" liest die Spannung (in mV), die am Sensor anliegt und speichert sie in ldr_val. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Aus der Modulbeschreibung (machine.ADC.vref) geht hervor, dass die Spannung einen Wert zwischen 0 und 1100 mV annehmen kann, wobei 0 als sehr dunkel und 1100 als sehr hell zu interpretieren ist."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Diesen Wert können wir also nicht einfach 1:1 nutzen, um die Helligkeit der LED zu setzen, da diese nur Werte zwischen 0 und 255 akzeptiert. Für den Anfang reicht es daher, den Wert des Sensors auf den Helligkeitswert zu &quot;mappen&quot;. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies ist ganz einfache Mathematik:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="brightness_val = ldr_val / 1100 * 255. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Würde der LDR nun einen Wert von 700 haben, würde als Ergebnis 162,272727273 herauskommen. Das Problem dabei: Neopixel.brightness kann nur mit ganzen Zahlen, sogenannten Integer umgehen! "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies ist aber leicht behoben. "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="int(brightness_val) "/>
                <ITEXT CH="sorgt dafür, dass aus der Gleitkommazahl 162,272727273 die Ganzzahl 162 wird. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hierbei wird allerdings nicht gerundet! der Wert wird einfach nach dem Komma abgetrennt, sodass wir eine ganze Zahl erhalten. Da wir keine sensiblen Berechnungen durchführen ist dies vernachlässigbar. Um Gleitkommazahlen zu runden, gibt es Funktionen im Modul math. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="brightness_val können wir nun einfach einer Funktion in animations.py übergeben, die dann die Helligkeit des Strips setzt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die Funktion, um die Helligkeit in animations.py zu setzen sieht ganz einfach aus: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def set_brightness(brightness_val):"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="strip.brightness(brightness_val)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Aufgerufen wird sie in main.py - am besten in der Endlos-Schleife: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="while True: "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="ldr_val = ldr.read()"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="brightness_val = int(ldr_val / 1100 * 255)"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="animations.set_brightness(brightness_val) "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Diese drei Zeilen lassen sich auch auf eine komprimieren!"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="while True: "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="animation.set_brightness(int(ldr.read() / 1100 * 255)t)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Analog Digital Converter" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
