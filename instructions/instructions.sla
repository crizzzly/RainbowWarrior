        <PAGEOBJECT XPOS="170.8" YPOS="71897.78" OwnPage="81" ItemID="1972169377" PTYPE="2" WIDTH="132.8" HEIGHT="231.2" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="0.266666666666667" LOCALSCY="0.266666666666667" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" Pagenumber="0" PFILE="../../RainbowWarrior_schaltung/piezo_Steckplatine.png" IRENDER="0" EMBEDDED="0" path="M0 0 L132.8 0 L132.8 231.2 L0 231.2 L0 0 Z" copath="M0 0 L132.8 0 L132.8 231.2 L0 231.2 L0 0 Z" gXpos="0" gYpos="124.976923800568" gWidth="475.28" gHeight="414.176923800565" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="299.68" YPOS="71897.38" OwnPage="81" ItemID="1972174817" PTYPE="2" WIDTH="324.8" HEIGHT="289.6" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="0.266666666666667" LOCALSCY="0.266666666666667" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" Pagenumber="0" PFILE="../../RainbowWarrior_schaltung/esp32_led_ldr_piezo_Steckplatine.png" IRENDER="0" EMBEDDED="0" path="M0 0 L324.8 0 L324.8 289.6 L0 289.6 L0 0 Z" copath="M0 0 L324.8 0 L324.8 289.6 L0 289.6 L0 0 Z" gXpos="128.88" gYpos="124.576923800567" gWidth="475.28" gHeight="414.176923800565" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="170.8" YPOS="72405.78" OwnPage="82" ItemID="1972165409" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="72405.78" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Das RTTTL-Modul"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="RTTTL steht für Ring Tone Text Transfer Language und ist ein verbreitetes Format für Klingeltöne auf Mobiltelefonen. Hier ein Beispiel für den Song &quot;One More Time&quot;:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="'OneMoreT:d=16,o=5,b=125:4e,4e,4e,4e,4e,4e,8p,4d#.,4e,4e,4e,4e,4e,4e,8p,4d#.,4e,4e,4e,4e,4e,4e,8p,4d#.,4f#,4f#,4f#,4f#,4f#,4f#,8f#,4d#.,4e,4e,4e,4e,4e,4e,8p,4d#.,4e,4e,4e,4e,4e,4e,8p,4d#.,1f#,2f#'"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Der Klingelton muss drei Teile beinhalten, um vom Klingeltonprogramm (oder hier des RTTTL-Moduls) erkannt zu werden: "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH=" "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="1. Name des Klingeltons (hier: 'OneMoreT')"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="2. Standardvorgaben (d=16,o=5,b=125), wobei &quot;d=&quot; die Standardnotenlänge ist. &quot;4&quot; bedeutet, dass jede Note ohne Längenangabe eine Viertelnote ist. &quot;8&quot; würde eine Achtelnote bedeuten usw.  &quot;o=&quot; die Standardoktave. Es gibt vier Oktaven im RTTTL-Format. &quot;b=&quot; steht für die Abspielgeschwindigkeit in Schlägen pro Minute."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="3. Die Tonangaben. Sie sind durch Komma voneinander getrennt und beinhalten:"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="1. (optional) Tonlänge"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="2. Notenangabe (c, d e, f, g, a, b, oder p = Pause). Eine Raute hinter der Tonangabe erhöht um einen Halbton. "/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="3. (optional) Oktave"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Das RTTTL-Modul für MicroPython kann hier heruntergeladen werden, wobei wir lediglich Datei &quot;rtttl.py&quot; brauchen werden : "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="https://github.com/dhylands/upy-rtttl"/>
                <para PARENT="links"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dieser sollte am besten im Projektordner, wo auch alle bisher erstellten *.py-Files liegen, gespeichert werden."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wie wir beim Betrachten des Dateisystems fesgtestellt haben, ist der Speicherort für weitere Module auf dem ESP der Ordner &quot;lib&quot;. Also müssen wir einen Ordner namens &quot;lib&quot; erstellen und die Datei rtttl.py in diesen kopieren. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="mkdir /flash/lib "/>
                <para PARENT="code"/>
                <ITEXT CH="cp rtttl.py /flash/lib"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Weitere Beispielsongs findet man hier: "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="http://www.picaxe.com/RTTTL-Ringtones-for-Tune-Command/"/>
                <para PARENT="links"/>
                <ITEXT CH="http://mines.lumpylumpy.com/Electronics/Computers/Software/Cpp/MFC/RingTones.RTTTL"/>
                <para PARENT="links"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Songs mit dem Piezo spielen - das RTTTL-Modul" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="73287.67" OwnPage="83" ItemID="1972849697" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="73287.67" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1975462881" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Songs mit dem PWM abspielen"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para/>
                <ITEXT CH="Um die Songs zu speichern und abzuspielen, erstellen wir eine neue Datei namens songs.py. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wir müssen nicht immer das komplette machine-Modul einbinden, wenn wir lediglich Pin und PWM daraus nutzen werden. Ebenso benötigen wir aus rtttl lediglich die Klasse RTTTL. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Außerdem wird noch das _thread-Modul importiert, da auch das Abspielen der Songs in einem Thread laufen wird. Außerdem können wir gleich den piezo_pin (22) definieren und PWM kurz initialisieren, um ihn sofort wieder zu deinitialisieren. Dies geschieht aus dem Grund, dass unsere PWM-Instanz auch von beiden Funktionen aus verändert werden können. Zudem benötigen wir noch eine Variable, die auf True gesetzt wird, sobald eine Benachrichtigung zum Abbruch des Threads kommt: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="from machine import Pin, PWM"/>
                <para PARENT="code"/>
                <ITEXT CH="from rtttl import RTTTL"/>
                <para PARENT="code"/>
                <ITEXT CH="import _thread"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="piezo_pin = 22 "/>
                <para PARENT="code"/>
                <ITEXT CH="piezo = PWM(piezo_pin)"/>
                <para PARENT="code"/>
                <ITEXT CH="piezo.deinit()"/>
                <para PARENT="code"/>
                <ITEXT CH="abort_playback = False"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Unsere Songs speichern wir in einer Liste namens SONGS. Wie bereits bekannt müssen Elemente einer Liste durch Komma getrennt werden. Aufgrund der Länge der verwendeten Strings und der Tatsache, dass sie viel zu lang sind, um auf dieser Seite einzeilig dargestellt zu werden, kann dies gerne übersehen werden: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="SONGS = ["/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="'OneMoreT:d=16,o=5,b=125:4e,4e,4e,4e,4e,4e,8p,4d#.,4e,4e,4e,4e,4e,4e,"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="8p,4d#.,4e,4e,4e,4e,4e,4e,8p,4d#.,4f#,4f#,4f#,4f#,4f#,4f#,8f#,4d#.,"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="4e,4e,4e,4e,4e,4e,8p,4d#.,4e,4e,4e,4e,4e,4e,8p,4d#.,1f#,2f#',"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="'MarioTitle:d=4,o=5,b=125:8d7,8d7,8d7,8d6,8d7,8d7,8d7,8d6,2d#7,8d7,p,"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="32p,d6,8b6,8b6,8b6,8d6,8b6,8b6,8b6,8d6,8b6,8b6,8b6,16b6,16c7,b6,8a6,"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="8d6,8a6,8a6,8a6,8d6,8a6,8a6,8a6,8d6,8a6,8a6,8a6,16a6,16b6,a6,8g6,8d6,"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="8b6,8b6,8b6,8d6,8b6,8b6,8b6,8d6,8b6,8b6,8b6,16a6,16b6,c7,e7,8d7,8d7,"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="8d7,8d6,8c7,8c7,8c7,8f#6,2g6',"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="'Tetris:d=4,o=5,b=160:e6,8b,8c6,8d6,16e6,16d6,8c6,8b,a8a,c6,e6,8d6, "/>
                <tab/>
                <ITEXT CH="8c6,b,8b,8c6,d6,e6,c6,a,2a,8p,d6,8f6,a6,8g6,8f6,e6,8e6,8c6,e6,8d6, "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="8c6,b,8b,8c6,d6,e6,c6,a,a'"/>
                <para PARENT="code"/>
                <ITEXT CH="]"/>
                <para PARENT="code"/>
                <tab/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wir werden insgesamt drei Funktionen benötigen, wovon nur eine vom MainThread aus aufgerufen wird. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die vom MainThread aufzurufende Funktion wird aus SONGS den angeforderten Song wählen und in einer separaten Variable speichern. Anschließend wird mit dieser Variable als Argument eine Instanz von RTTTL erstellt. Nun wird für jede Note, die in diesem Song vorhanden ist, die nächste Funktion play_tone aufgerufen, die die Note dann abspielt. In dieser Schleife sollte auch geprüft werden, ob der Thread abgebrochen werden soll:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def find_song(name):"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="&quot;&quot;&quot; find_song(name)"/>
                <para PARENT="Kommentare - Block"/>
                <ITEXT CH="   "/>
                <tab/>
                <ITEXT CH="searches in SONGS list for song name and plays"/>
                <para PARENT="Kommentare - Block"/>
                <ITEXT CH="    "/>
                <tab/>
                <ITEXT CH="its tones with the play_tone function"/>
                <para PARENT="Kommentare - Block"/>
                <ITEXT CH="    "/>
                <tab/>
                <ITEXT CH="name: name of the song to search for"/>
                <para PARENT="Kommentare - Block"/>
                <ITEXT CH="    "/>
                <tab/>
                <ITEXT CH="&quot;&quot;&quot;"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="global abort_playback"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="abort_playback = False"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="for song in SONGS:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="song_name = song.split(&quot;:&quot;)[0]"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="if song_name == name:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="tune = RTTTL(song)"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="for freq, msec in tune.notes():"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="play_tone(freq, msec)"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="if abort_playback:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="return"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="piezo.deinit()"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zwei Ausdrücke, die wir - zumindest so - noch nicht besprochen haben: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH="song_name = song.split(&quot;:&quot;)[0]"/>
                <ITEXT CH=" - splittet den String an der Stelle, an der der Doppelpunkt vorkommt. Dies funktioniert natürlich ebenso, wenn mehr als ein Doppelpunkt im String vorhanden ist. [0] beinhaltet dann den Namen, [1] die Standardvorgaben und [2] die Tonangaben. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH="for freq, msec in tune.notes()"/>
                <ITEXT CH=" deutet darauf hin, dass von der Funktion tune.notes() zwei Objekte zurückgegeben werden: freq für die Frequenz und msec für die Dauer in ms. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="somit kann man mit dieser for-Schleife zwei Werte parallel verarbeiten. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="abort_playback ist - wie der Name schon sagt, eine Variable, in der festgehalten wird, ob die Wiedergabe abgebrochen werden soll oder nicht. Dies wird in play_tone geprüft"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="play_tone(freq, ms) muss vor der Funktion find_song() platziert werden. Hier wird für jede einzelne Note ein Ton mittels PWM erzeugt. Anstelle von utime.sleep_ms(ms) können wir - analog zu animations.py - waitForExitNotification(ms) einsetzen, die im darauffolgenden Schritt erstellt wird: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def play_tone(freq, msec): "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="&quot;&quot;&quot;play_tone(freq, msec)"/>
                <para PARENT="Kommentare - Block"/>
                <ITEXT CH="    "/>
                <tab/>
                <ITEXT CH="plays a single tone on piezo buzzer"/>
                <para PARENT="Kommentare - Block"/>
                <ITEXT CH="   "/>
                <tab/>
                <ITEXT CH="freq: frequency of the tone"/>
                <para PARENT="Kommentare - Block"/>
                <ITEXT CH="   "/>
                <tab/>
                <ITEXT CH="msec: duration in millis"/>
                <para PARENT="Kommentare - Block"/>
                <ITEXT CH="  "/>
                <tab/>
                <ITEXT CH="&quot;&quot;&quot;"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="global piezo "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="global abort_playback"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="print('freq = {:6.1f} msec = {:6.1f}'.format(freq, msec))"/>
                <tab/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="if freq &gt; 0: "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="piezo.freq(int(freq))"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="piezo.duty(50)"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="ntf0 =waitForExitNotification(int(msec *0.9))"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="piezo.duty(0)"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="ntf1 = waitForExitNotification(int(msec * 0.1))"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="if (ntf0 or ntf1):"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="abort_playback = True"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="piezo.deinit()"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="Hier wird der Wert von ms gesplittet: zuerst wird gewartet, bis 90% des ms-Wertes vergangen sind, um dann duty auf 0 zu setzen. Anschließend wird die letzten 10% von ms abgewartet. Um nicht versehentlich eine Benachrichtigung zu überschreiben, werden diese in zwei verschiedene Variablen gespeichert."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="ntf0 or ntf1 ist eine logische Operation. Sie gibt genau dann True zurück, wenn mindestens eine der beden Operatoren (ntf0 oder ntf1) True ist. Somit wird auch keine Abbruchbedingung übersehen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zuletzt benötigen wir noch eine Funktion, die die Abbruchbedingung prüft:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def waitForExitNotification(timeout):"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="&quot;&quot;&quot; waitForExitNotification(timeout)"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="uses _thread.wait(timeout) to sleep for amount of ms saved in timeout,  "/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="checks notfication for _thread.EXIT notification. Returns True if "/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="_thread.EXIT notification is recieved, else returns False"/>
                <para PARENT="Kommentare - Block"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="timeout: (ms) time in ms used for _thread.wait()"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="return: (boolean) True if _thread.EXIT is recieved, else False."/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="&quot;&quot;&quot;"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="ntf = _thread.wait(timeout)"/>
                <para PARENT="code"/>
                <tab/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="if ntf == _thread.EXIT:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="return True"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="return False"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="return False muss hier nicht zwingend in einem else-Block stehen. Das return-Statement im if-Blockbewirkt, dass diese Funktion mit dieser Zeile verlassen wird. Wenn also die Bedingung (ntf == _thread.EXIT) zutrifft, wird return False gar nicht mehr ausgeführt."/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Songs mit PWM abspielen" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="74169.56" OwnPage="84" ItemID="1975462881" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="74169.56" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1975467697" BACKITEM="1972849697"/>
        <PAGEOBJECT XPOS="170.8" YPOS="75051.45" OwnPage="85" ItemID="1975467697" PTYPE="4" WIDTH="453.68" HEIGHT="485.550000000032" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 485.55 L0 485.55 L0 0 Z" copath="M0 0 L453.68 0 L453.68 485.55 L0 485.55 L0 0 Z" gXpos="170.8" gYpos="75051.45" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1975462881"/>
        <PAGEOBJECT XPOS="170.8" YPOS="75560.1110253998" OwnPage="85" ItemID="1975551665" PTYPE="4" WIDTH="453.68" HEIGHT="205.728974600162" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 205.729 L0 205.729 L0 0 Z" copath="M0 0 L453.68 0 L453.68 205.729 L0 205.729 L0 0 Z" gXpos="170.8" gYpos="75560.1110253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="2081213537" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Dropdown Menü für Songs in die Webseite einfügen"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um den Song auf der Webseite auswählen zu können verfahren wir analog wie mit den LED Animationen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um uns arbeit zu sparen, können wir den gesamten Bereich innerhalb der form-Tags kopieren und unterhalb des kopierten Bereichs wieder einfügen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um etwas mehr Abstand zwischen den beiden Menüs zu schaffen, sollte man "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="&lt;/br&gt;"/>
                <ITEXT CH="-Tags einfügen. Diese bewirken einen Zeilenumsprung. Hier darf gerne getestet werden, was optisch akzeptabel ist. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="h3"/>
                <ITEXT CH=" sollten wir nun selbstverständlich anpassen: Song ausw&amp;auml;hlen passt hier besser! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="als nächstes muss im "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="select"/>
                <ITEXT CH="-Tag der "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="name"/>
                <ITEXT CH=" angepasst werden: "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="name=&quot;song&quot;"/>
                <ITEXT CH="  "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun müssen wir noch unsere Songs ins Dropdown Menü einfügen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hier sollten wir - wie auch bei den LED Animationen - darauf achten, dass unter "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="value"/>
                <ITEXT CH=" der Name steht, der in der SONGS-Liste in songs.py zu finden ist. Dies spart uns unnötige Abfragen. Der Name, der auf der Seite gezeigt werden soll, ist dafür immernoch frei wählbar."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH=" Hier nun der neu eingefügte Part in index.html:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&lt;/br&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="&lt;/br&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="&lt;h3 class=&quot;mb-3&quot; id=&quot;id1&quot;&gt;Song ausw&amp;auml;hlen:&lt;/h3&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="    &lt;div class=&quot;input-group&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="      &lt;select class=&quot;custom-select&quot; id=&quot;inputGroupSelect04&quot; name=&quot;sound&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="        &lt;option selected&gt;Song ausw&amp;auml;hlen...&lt;/option&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="        &lt;option value=&quot;OneMoreT&quot;&gt;One More Time&lt;/option&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="        &lt;option value=&quot;MarioTitle&quot;&gt;Super Mario Titelsong&lt;/option&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="        &lt;option value=&quot;Tetris&quot;&gt;Tetris Theme&lt;/option&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="      &lt;/select&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="      &lt;div class=&quot;input-group-append&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="       &lt;button class=&quot;btn btn-outline-secondary&quot; type=&quot;button&quot;&gt;Button&lt;/button&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="     &lt;/div&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="    &lt;/div&gt;"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Übrigens: man muss die HTML-Seite nicht zwingend zuerst auf den ESP kopieren um sie anschauen zu können. Da sie sich gemeinsam mit ihren zugehörigen CSS-Seiten im Ordner www befindet, kann man diese einfach mit einem Doppelklick im Browser öffnen."/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Dropdownmenü für Songs in die Webseite einfügen" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="76364.0010253998" OwnPage="86" ItemID="1980542673" PTYPE="4" WIDTH="453.68" HEIGHT="283.728974600162" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 283.729 L0 283.729 L0 0 Z" copath="M0 0 L453.68 0 L453.68 283.729 L0 283.729 L0 0 Z" gXpos="170.8" gYpos="76364.0010253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1981942977" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Verarbeitung der Daten auf dem Webserver"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Auch hier kann man analog zu den Animationen verfahren: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="in webSrv.py sollte nun nicht nur nach &quot;light&quot; geprüft werden, sondern auch nach &quot;song&quot;."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Oder aber man verzichtet auf diesen Schritt und überlässt diese Unterscheidung dem MainThread!"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dazu kann die Zeile:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="if &quot;light&quot; in formData:"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="ersetzt werden durch:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="if formData: "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Das bedeutet, dass die Thread-Message nur gesendet wird, wenn Daten empfangen wurden. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Thread-Message sollte entweder aus einem String, oder einem Integer-Wert bestehen. Ein Dictionary ist allerdings weder das eine, noch das andere!"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dafür kann man einen Dictionary sehr einfach mit str(dict) in einen String verwandeln! und einen String (sofern er den Aufbau eines Dictionarys hat) ebenso einfach mit eval(str) wieder in einen Dictionary! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="if formData:"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="message = str(formData)"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="_thread.sendmsg(_thread.getReplID(), message)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="konvertiert nun den vom webServer empfangenen Dictionary in einen String und sendet diesen an den MainThread. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="In main.py lassen wir uns nun ausgeben, was der Server uns geschickt hat: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="msg = _thread.getmsg("/>
                <para PARENT="code"/>
                <ITEXT CH="if msg[1] == 2:"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="print(msg[2])"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; {'light': 'crossing_dots', 'sound': 'Song+auswählen+...'}"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Oh! "/>
                <breakline/>
                <ITEXT CH="Dies bedeutet, dass nun - unabhängig davon, welcher Button von beiden gedrückt wurde, beide Werte übergeben werden!"/>
                <breakline/>
                <ITEXT CH="Dies liegt wohl daran, dass sich beide innerhalb einer Form befinden!"/>
                <breakline/>
                <breakline/>
                <ITEXT CH="Man kann nun entweder eine separate Form für jedes Menü erstellen - oder aber wir versehen die Buttons mit name und value, wodurch der angeklickte Button diese Werte ebens übergibt. Anhand diesen wir entscheiden, wie mit diesen Daten weiter verfahren wird. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Für beide Buttons setzen wir also "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="name=&quot;button&quot;"/>
                <ITEXT CH=" ein. so können wir später einfach den Wert prüfen, der an der Stelle &quot;button&quot; ist."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Als "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="value"/>
                <ITEXT CH=" setzen wir &quot;"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="light"/>
                <ITEXT CH="&quot; bei den Animationen und &quot;"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="sound"/>
                <ITEXT CH="&quot; bei den songs.  Dann sieht der Output so aus: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; {'light': 'crossing_dots', 'sound': 'Song+auswählen+...', 'button': 'light'}"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Verarbeitung der Daten auf dem Webserver" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="76815.23" OwnPage="87" ItemID="1981942977" PTYPE="4" WIDTH="453.68" HEIGHT="574.199999999997" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 574.2 L0 574.2 L0 0 Z" copath="M0 0 L453.68 0 L453.68 574.2 L0 574.2 L0 0 Z" gXpos="170.8" gYpos="76815.23" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1980542673"/>
        <PAGEOBJECT XPOS="170.8" YPOS="77391" OwnPage="87" ItemID="1981944209" PTYPE="4" WIDTH="453.68" HEIGHT="138.619999999966" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 138.62 L0 138.62 L0 0 Z" copath="M0 0 L453.68 0 L453.68 138.62 L0 138.62 L0 0 Z" gXpos="170.8" gYpos="77391" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="2003403553" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Verarbeitung der Daten in main"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun haben wir uns eine Routine gebaut, die sehr Variabel mit allen Möglichen Inputs umgehen kann. Es fehlt lediglicj noch die Verarbeitung in main.py."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="_thread.getmsg()[2] liefert uns einen Dictionary als String, den wir nun wieder in einen Dictionary umwandeln müssen. Anschließend müssen wir prüfen, ob der Button mit dem value &quot;light&quot; gedrückt wurde. dann können wir handleAnimationThread mit dem Wert, der in &quot;light&quot; steht starten:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="# read threadMessage from microWebSrv"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="msg = _thread.getmsg()"/>
                <para PARENT="code"/>
                <ITEXT CH="# check if message is String"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="if msg[0] == 2:"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="# convert String in dictionary"/>
                <para PARENT="Kommentare - Zeilen"/>
                <tab/>
                <ITEXT CH="values_dict = eval(msg[2])"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="# call function according to value of &quot;button&quot;"/>
                <para PARENT="Kommentare - Zeilen"/>
                <tab/>
                <ITEXT CH="if values_dict[&quot;button&quot;] == &quot;light:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="handleAnimationThread(values_dict[&quot;light&quot;])"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Analog verfahren wir mit &quot;sound&quot;:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="elif values_dict[&quot;button&quot;] == &quot;sound&quot;:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="handleSoundThread(values_dict[&quot;sound&quot;])"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Ohne die Funktion handleSoundThread(song) zu definieren, können wir allerdings nicht fortfahren:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="sound_thread = 100"/>
                <para PARENT="code"/>
                <ITEXT CH="def handleSoundThread(song):"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="&quot;&quot;&quot; stopps running sound thread and starts a new thread with value saved "/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="in 'song'"/>
                <para PARENT="Kommentare - Block"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="song: (String) name of the song to start "/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH=" &quot;&quot;&quot;"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="global sound_thread"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="_thread.notify(sound_thread, _thread.EXIT)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="sound_thread = _thread.start_new_thread(&quot;sound&quot;, songs.find_song, (song, ))"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Fertig für den Test! "/>
                <para/>
                <ITEXT CH="Wenn alles fehlerfrei eingefügt und auf den ESP kopiert wurde, kannn man nun nach einem Neustart den Song von der Webseite aus abspielen lassen."/>
                <para/>
                <para/>
                <ITEXT CH="Leider aber stößt der ESP an seine Grenzen, wenn man rechenintensive Animationen zeitgleich mit einem Song laufen lässt. In diesem Fall ist es möglich, dass Song und Animation merkbar langsamer abgespielt werden."/>
                <trail/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Verarbeitung der Daten in main" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="78579.01" OwnPage="89" ItemID="1979151073" PTYPE="4" WIDTH="453.68" HEIGHT="171.740000000034" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 171.74 L0 171.74 L0 0 Z" copath="M0 0 L453.68 0 L453.68 171.74 L0 171.74 L0 0 Z" gXpos="170.8" gYpos="78579.01" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="RainbowWarrior als Wecker nutzen "/>
                <para PARENT="Überschrift Mittel"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun haben wir gelernt, wie man den LED Strip ansteuert sowie mit dem Piezo Speaker Songs abspielt. Selbstverständlich lassen sich diese beidem Elemente auch kombinieren um sie als Wecker einzusetzen."/>
                <para PARENT="Default Paragraph Style" ALIGN="0"/>
                <para PARENT="Default Paragraph Style" ALIGN="0"/>
                <ITEXT CH="FreeRTOS ist ein Real Time Operating System. Das bedeutet, man kann mit dem ESP auch zeitgesteuerte Operationen ausführen. Die Klasse machine.RTC stellt uns eine Real Time Clock zur Verfügung, mit der wir Datum- und Uhrzeit einstellen können."/>
                <para PARENT="Default Paragraph Style" ALIGN="0"/>
                <ITEXT CH="Sowie uns das Modul utime Zeit-abhängige Funktionen zur Verfügung stellt"/>
                <para PARENT="Default Paragraph Style" ALIGN="0"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="RainbowWarrior als Wecker nutzen" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="78757.55" OwnPage="89" ItemID="1983540465" PTYPE="4" WIDTH="453.68" HEIGHT="535.849999999962" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 535.85 L0 535.85 L0 0 Z" copath="M0 0 L453.68 0 L453.68 535.85 L0 535.85 L0 0 Z" gXpos="170.8" gYpos="78757.55" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Real Time Clock - das RTC Modul "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Mit dem RTC-Modul können wir die Systemzeit des ESP einstellen. Zudem haben wir die Möglichkeit - sofern wir im Station-Mode mit dem Internet verbunden sind - die Uhrzeit im Internet zu synchronisieren (so, wie es auch unsere Mobiltelefone machen)."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Außerdem können wir den ESP in den sogenannten &quot;deepsleep&quot;-Modus versetzen. Das bedeutet, dass viele Funktionen heruntergefahren werden und somit weniger Strom verbraucht wird. Selbstverständlich stellt es auch Möglichkeiten zur Verfügung, den ESP wieder aus dem Tiefschlaf zu wecken! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die Modulbeschreibung ist hier zu finden: "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="https://github.com/loboris/MicroPython_ESP32_psRAM_LoBo/wiki/rtc"/>
                <para PARENT="links"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die wichtigsten Funktionen, um den ESP als Wecker zu nutzen, sind: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="rtc = machine.RTC()"/>
                <ITEXT CH=" erstellt eine Instanz der RTC"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="rtc.init(date)"/>
                <ITEXT CH=" lässt uns die Systemzeit einstellen, wobei für Date ein Tupel erwartet wird: (year, month, day [,hour [,minute [, second ]]])"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="rtc.now()"/>
                <ITEXT CH=" gibt uns die aktuelle Zeit als Tupel zurück: (year, month, day, hour, minute, second)"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH=" Um die Zeit im Internet zu synchronisieren sollte vorab eine Prüfung stattfinden, ob der ESP im Station-Mode verbunden ist. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="rtc.ntp_sync(server [,update_period] [,tz])"/>
                <ITEXT CH=" wobei als Server &quot;de.pool.ntp.org&quot; genutzt werden kann."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Real Time Clock - das RTC Modul" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="79460.9" OwnPage="90" ItemID="1984407297" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="79460.9" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Zeitabhängige Funktionen - das utime-Modul"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Das utime-Modul stellt ebenso Funktionen zur Verfügung, mit denen man die aktuelle Zeit ausgeben kann. Außerdem Zeitintervalle messen und Delays ausführen."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="https://docs.micropython.org/en/latest/pyboard/library/utime.html"/>
                <para PARENT="links"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="utime.localtime([secs]) "/>
                <ITEXT CH="konvertiert die Zeit, angegeben in Sekunden (secs) in ein Tupel aus 8 Elementen  (year, month, mday, hour, minute, second, weekday, yearday). Wenn kein Argument übergeben wird, wird die aktuelle Systemzeit genutzt. "/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="year - als Jahrtausendangabe (z.B. 2014)."/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="month (1-12)"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="mday (1-31)"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="hour (0-23)"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="minute (0-59)"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="second (0-59)"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="weekday (0-6 für Mo-So)"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="yearday (1-366)"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="utime.mktime(time_tuple)"/>
                <ITEXT CH=" gibt - Gegenteilig zu localtime -  für ein vollständiges Tupel aus 8 Elementen (year, month, mday, hour, minute, second, weekday, yearday) die Anzahl der Sekunden zwischen dem 01.01.1970 und der dateTime, die im Tupel üergeben wurde, zurück. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="utime.sleep_ms"/>
                <ITEXT CH="(milliseconds) führt ein Delay für die Angegebene Zeit in Millisekunden aus"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="utime.sleep_us(microseconds) "/>
                <ITEXT CH=" führt ein Delay für die Angegebene Zeit in Mikrosekunden aus"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Condensed Bold" CH="utime.ticks_ms()"/>
                <ITEXT CH=" gibt eine steigende Zahl an Millisekunden zurück - der Referenzpunkt ist allerdings willkürlich gesetzt. Diese Funktion wird zum Beispiel eingesetzt, um ein Delay zu erstellen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="utime.ticks_add(ticks, delta) führt eine Addition mit Ticks aus, wobei ticks vorher mit ticks_ms(, ticks_us oder ticks_cpu - in Modulbeschreibung zu finden) erstellt werden muss. delta kann eine positive oder negative Integer sein) "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="utime.ticks_diff(ticks1, ticks2)"/>
                <ITEXT CH=" gibt die Zeitdifferenz zwischen zwei ticks-Werten zurück. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Normale Addition bzw Subtraktion von ticks-Werten sollten nicht ausgeführt werden, da dies zu inkorrekten Ergebnissen führen kann."/>
                <para PARENT="Default Paragraph Style"/>
                <para/>
                <ITEXT FONT="DejaVu Sans Bold" CH="utime.time()"/>
                <ITEXT CH=" gibt die Anzahl der Sekunden zurück, die zwischen dem 01.01.1970 und der eingestellten Systemzeit vergangen sind. "/>
                <trail/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Zeitabhängige Funktinen - das utime-Modul" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="80342.79" OwnPage="91" ItemID="1979411729" PTYPE="4" WIDTH="453.68" HEIGHT="287.460000000036" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 287.46 L0 287.46 L0 0 Z" copath="M0 0 L453.68 0 L453.68 287.46 L0 287.46 L0 0 Z" gXpos="170.8" gYpos="80342.79" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Systemzeit auf der Webseite anzeigen"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um lediglich die aktuelle Zeit anzuzeigen reicht uns das utime-Modul. Nachdem dieses in webSrv.py eingebunden wurde, kann die aktuelle Systemzeit als Tupel in der Variable datetime gespeichert werden: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="import utime"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="datetime = utime.localtime()"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Damit bei jedem Aufruf der Webseite die aktuelle Systemzeit genutzt wird, muss datetime = utime.localtime()  innerhalb der Route-Handler Funktionen stehen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zur Erinnerung, das Tupel besteht aus 8 Elementen: (year, month, mday, hour, minute, second, weekday, yearday)"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wollen wir nun aus diesem Tupel z.B. eine Ausgabe am Terminal machen, die nur die Uhrzeit (Stunden und Minuten)  getrennt durch einen Doppelpunkt - anzeigt, müssen wir uns zuerst etwas mit dem Thema String Formatierung beschäftigen:  "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Systemzeit auf der Webseite anzeigen" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="80631.4510253998" OwnPage="91" ItemID="1986078321" PTYPE="4" WIDTH="453.68" HEIGHT="425.728974600162" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 425.729 L0 425.729 L0 0 Z" copath="M0 0 L453.68 0 L453.68 425.729 L0 425.729 L0 0 Z" gXpos="170.8" gYpos="80631.4510253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1985685553" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" CH="String Formatierung "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Bisher haben wir die print-Ausgabe nur genutzt, um entweder Integer-Variablen oder Texte auszugeben."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Man kann diese beiden Ausgaben aber auch kombinieren! Hier ein Beispiel für die Ausgabe mit Text und Variable anhand der Print-Ausgabe. Zuerst werden wir das Alter des Nutzers in Repl einlesen, anschließend einen Text inklusive dem eingegebenen Alter ausgeben. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="&gt;&gt;&gt; age = input(&quot;Alter?  &quot;)"/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Dadurch erfolgt in Repl die Ausgabe 'Alter? ', bei der man dann das Alter (z.B. 22) eingeben kann. Dieses wird nach der Bestätigung mit Enter in der Variable age gespeichert:"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Alter? 22"/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Nun wollen wir das Alter in einen Text einfügen und ausgeben:"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="&gt;&gt;&gt; print(&quot;Alter: {} Jahre&quot;.format(age))"/>
                <para PARENT="code"/>
                <ITEXT CH="Alter ist 22 Jahre"/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="String.format(var) konvertiert dabei die Variable var in einen String und fügt sie in den Platzhalter {} ein."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Wollen wir nun wissen, um welchen Datentyp es sich bei &quot;age&quot; handelt, schreiben wir: "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="&gt;&gt;&gt; type(age)"/>
                <para PARENT="code"/>
                <ITEXT CH="&lt;class 'str'&gt;"/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Was bedeutet, dass es sich um einen String handelt. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Um mit dieser Eingabe berechnungen ausführen zu können, bräuchten wir allerdings einen Integer-Wert. mit int(age) lässt sich der String in age in einen Integer-Wert konvertieren. Dies geht ebenso direkt mit der Eingabe:"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="&gt;&gt;&gt; age = int( input(&quot;Alter?  &quot;))"/>
                <para PARENT="code"/>
                <ITEXT CH="Alter? 22 "/>
                <para PARENT="code"/>
                <ITEXT CH="&gt;&gt;&gt; type(age)"/>
                <para PARENT="code"/>
                <ITEXT CH="&lt;class 'int'"/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Arbeiten mit mehereren Platzhaltern"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Nehmen wir an, wir haben Geschwister im Alter von 12, 14 und 18 Jahren (Alter gespeichert in age1, age2, age3) und wollen diese in einen Text einfügen, gehen wir folgendermaßen vor: "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="&gt;&gt;&gt; print(&quot;Meine Geschwister sind {}, {} und {} Jahre alt&quot;.format(age1, age2, age3)"/>
                <para PARENT="code"/>
                <ITEXT CH="Meine Geschwister sind 12, 14 und 18 Jahre alt"/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Wobei in die Platzhalter der Reihe nach die Werte in format() eingetragen werden. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Allerdings können wir auch die Reihenfolge der Ausgabe anpassen, ohne die Reihenfolge in format() zu verändern:"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="&gt;&gt;&gt; print(&quot;Meine grosse Schwester ist {2} Jahre alt, mein kleiner Bruder {0} und meine kleine Schwester {1}&quot;.format(age1, age2, age3))"/>
                <para PARENT="code"/>
                <ITEXT CH="Meine grosse Schwester ist 18 Jahre alt, mein kleiner Bruder 12 und meine kleine Schwester 14"/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Nehmen wir nun an, wir haben eine Variable time, in der die Zeit als Tupel (h, m) gespeichert ist. Derzeit ist es kurz nach 9.  Die Ausgabe der Uhrzeit in einem String würde dann folgendermaßen aussehen:"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="&gt;&gt;&gt; print(&quot;Zeit: {}:{} Uhr&quot;.format(time[0], time[1])"/>
                <para PARENT="code"/>
                <ITEXT CH="Zeit: 9:6 Uhr"/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Leider sieht dies schrecklich aus, da wir gewohnt sind, dass die Uhrzeit in dieser Darstellung immer zweistellig für Stunde und Minute ist. Diese Darstellung erreichen wir folgendermaßen:"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="&gt;&gt;&gt; print(&quot;Uhrzeit: {0:02d}:{1:02d} Uhr&quot;.format(time[0], time[1]))"/>
                <para PARENT="code"/>
                <ITEXT CH="Uhrzeit: 09:06 Uhr"/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Wobei 02 bedeutet, dass die Ausgabe mindestens zweistellig sein muss, bei kleineren Werten soll eine 0 vorangestellt werden. Das d bedeutet, dass es sich bei diesem Wert um eine Dezimalzahl handelt."/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Hier ist die Position (0: oder 1:) zwingend anzugeben! "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Man kann bei der Ausgabe ebenso mit Schlüsselwortparametern arbeiten: "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="&gt;&gt;&gt;print(&quot;die Hauptstadt von {province} ist {capital}.&quot;.format(&quot;province&quot; : &quot;Hessen&quot;, &quot;capital&quot; :  &quot;Wiesbaden&quot;))"/>
                <para PARENT="code"/>
                <ITEXT CH="die Hauptstadt von Hessen ist Wiesbaden."/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Diese Angaben gelten nicht nur für die print-Ausgabe, sondern sind ebenso auch auf Strings anwendbar: "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="&gt;&gt;&gt; month = &quot;April&quot;"/>
                <para PARENT="code"/>
                <ITEXT CH="&gt;&gt;&gt; text = &quot;Mein Geburtsmonat ist {}&quot;"/>
                <para PARENT="code"/>
                <ITEXT CH="&gt;&gt;&gt; text.format(month) "/>
                <para PARENT="code"/>
                <ITEXT CH="&gt;&gt;&gt; print(text)"/>
                <para PARENT="code"/>
                <ITEXT CH="Mein Geburtsmonat ist April"/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Weitere Anwendungsbeispiele findet man im Python-Forum:"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Oblique" CH="https://www.python-kurs.eu/python3_formatierte_ausgabe.php"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="E: String Formaterung" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="81224.68" OwnPage="92" ItemID="1985685553" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="81224.68" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="2062951185" BACKITEM="1986078321"/>
        <PAGEOBJECT XPOS="170.8" YPOS="82354.3410253998" OwnPage="93" ItemID="1985688177" PTYPE="4" WIDTH="453.68" HEIGHT="466.618974600162" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 466.619 L0 466.619 L0 0 Z" copath="M0 0 L453.68 0 L453.68 466.619 L0 466.619 L0 0 Z" gXpos="170.8" gYpos="82354.3410253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1988509713" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Um also die Systemzeit auf der Webseite anzeigen zu können, müssen wir auf dieser erst einmal Platzhalter einfügen. Anschließend müssen wir dafür sorgen, dass uns die HTML-Datei als String in webSrv.py vorliegt, da wir format() nur auf einen String anwenden können. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zum Schluss müssen wir noch httpResponse unseres RouteHandlers anpassen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die Zeitausgabe auf der Webseite soll folgendermaßen formatiert werden: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="22.09.2018 15:05 Uhr"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Unser Tupel besteht aus year (0), month (1), mday (2), hour (3), minute (4), second (5), weekday (6), yearday (7)"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zur besseren Übersicht können wir nun die Zeit und das Datum in unserer gewohnten Reihenfolge in einen Dictionary in webSrv.py speichern:"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH=" "/>
                <para PARENT="code"/>
                <ITEXT CH="datetime_dict = { &quot;year&quot; : datetime[0], &quot;month&quot; : datetime[1], "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="&quot;mday&quot; : datetime[2], &quot;hour&quot;: datetime[3], &quot;minute&quot; : datetime[4], "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="&quot;second&quot; : datetime[5], &quot;weekday&quot; : datetime[6], &quot;yearday&quot; : datetime[7]}"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun fügen wir in index.html die entsprechenden Platzhalter ein. Dies erreichen wir, indem wir einen neuen &quot;paragrafen&quot; mit &lt;p&gt; (text) &lt;/p&gt; einfügen. Nur Text innerhalb eines Tags wird auch auf der Webseite dargestellt!"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Noch vor dem ersten &lt;form&gt;-Tag fügen wir also zuerst einen öffnenden &lt;p&gt;-Tag, dann unsere Platzhalter inklusive Text, danach den schließenden Tag &lt;/p&gt; ein: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&lt;p&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="   {mday:02d}.{month:02d}.{year:04d} {hour:02d}:{minute:02d} Uhr"/>
                <para PARENT="code"/>
                <ITEXT CH="&lt;/p&gt;"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="&lt;form ..."/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="bevor wir die korrekte Darstellung testen können, müssen wir in webSrv.html dafür sorgen, dass uns index.html als String vorliegt:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="file = open(&quot;www/index.html&quot;)"/>
                <para PARENT="code"/>
                <ITEXT CH="htmlSite = file.read()"/>
                <para PARENT="code"/>
                <ITEXT CH="file.close()"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Mit file = open wird die Seite, die mit dem Pfad (als String) übergeben wird, in file gespeichert. Dies geschieht zum Beispiel auch, wenn wir mit einem Texteditor eine Datei öffnen. Wie im Texteditor muss auch hier die Datei mit file.close() nach der verarbeitung geschlossen werden. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="file.read() gibt den Inhalt der Seite als String zurück."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun sorgen wir dafür, dass beim Aufruf der Webseite die aktuelle Systemzeit in datetime gespeichert wird und erstellen daraus einen Dictionary. Dazu fügen wir in unsere RouteHandler-Funktion (_httpHandlerPost) folgende Zeilen ein. : "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="datetime = utime.localtime()"/>
                <para PARENT="code"/>
                <ITEXT CH="datetime_dict = { &quot;year&quot; : datetime[0], &quot;month&quot; : datetime[1], &quot;mday&quot; : datetime[2], &quot;hour&quot;: datetime[3], &quot;minute&quot; : datetime[4], &quot;second&quot; : datetime[5], &quot;weekday&quot; : datetime[6], &quot;yearday&quot; : datetime[7]}"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun können wir dafür sorgen, dass die Werte in die Platzhalter mit String.format() eingefügt werden können: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="htmlSite.format(**datetime_dict)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Das doppelte Sternchen (**) vor datetime_dict sorgt dafür, dass datetime_dict in die Form 'year'= value, 'month'= value, ... umgewandelt wird."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun müssen wir noch httpResponse anpassen. Also das, was bei einem Aufruf (=einer Anfrage) des Clients an den Server vom Server zurückgegeben wird. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Bisher wurde mit httpRespons.writeResponseFile( .... path = &quot;www/index.html&quot;) die Datei &quot;www/index.html&quot; zurückgegeben. Dies müssen wir nun ändern, da wir keine Datei, sondern den String mit dem HTML-Code der Datei als Antword senden wollen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hierfür nutzt man writeResponseOk und fügt anstatt 'path= ... ' content = htmlFile ein: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="httpResponse.WriteResponseOk(headers = ({'Cache-Control': 'no-cache'}), contentType = 'text/html', contentCharset  = 'UTF-8', content =htmlFile) "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Insgesamt sieht webSrv.py nun so aus: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="from microWebSrv import MicroWebSrv"/>
                <para PARENT="code"/>
                <ITEXT CH="import _thread, utime"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="srv_run_in_thread = True"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="# open file &quot;www/index.html&quot; and save content as String in htmlSite and "/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="# close file"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="file = open(&quot;www/index.html&quot;)"/>
                <para PARENT="code"/>
                <ITEXT CH="htmlSite = file.read()"/>
                <para PARENT="code"/>
                <ITEXT CH="file.close()"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="# route handler for index.html with method GET"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="@MicroWebSrv.route('/')"/>
                <para PARENT="code"/>
                <ITEXT CH="def _httpHandlerPost(httpClient, httpResponse) :"/>
                <para PARENT="code"/>
                <ITEXT CH="    # get system time and save it to datetime_dict"/>
                <para PARENT="code"/>
                <ITEXT CH="    datetime = utime.localtime()"/>
                <para PARENT="code"/>
                <ITEXT CH="    datetime_dict = { &quot;year&quot; : datetime[0], &quot;month&quot; : datetime[1], "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="&quot;mday&quot; : datetime[2], &quot;hour&quot;: datetime[3], "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="&quot;minute&quot; : datetime[4], &quot;second&quot; : datetime[5],"/>
                <para PARENT="code"/>
                <ITEXT CH="                  &quot;weekday&quot; : datetime[6], &quot;yearday&quot; : datetime[7]}"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="    # insert datetime_dict values in placeholders in htmlSite"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="    htmlSite = htmlSite.format(**datetime_dict)"/>
                <para PARENT="code"/>
                <ITEXT CH="    # send htmlSite as response to client"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="    httpResponse.WriteResponseOk(headers = ({'Cache-Control': 'no-cache'}),"/>
                <para PARENT="code"/>
                <ITEXT CH="                                    contentType     = 'text/html',"/>
                <para PARENT="code"/>
                <ITEXT CH="                                    contentCharset  = 'UTF-8',"/>
                <para PARENT="code"/>
                <ITEXT CH="                                    content = htmlSite)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="# route handler for index.html with method POST"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="@MicroWebSrv.route('/', 'POST')"/>
                <para PARENT="code"/>
                <ITEXT CH="def _httpHandlerPost(httpClient, httpResponse) :"/>
                <para PARENT="code"/>
                <ITEXT CH="    # read posted form data. If data (dictionary) received, convert to string"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="    # and send threadMessage to mainThread"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="    formData = httpClient.ReadRequestPostedFormData()"/>
                <para PARENT="code"/>
                <ITEXT CH="    print(formData)"/>
                <para PARENT="code"/>
                <ITEXT CH="    if formData:"/>
                <para PARENT="code"/>
                <ITEXT CH="        message = str(formData)"/>
                <para PARENT="code"/>
                <ITEXT CH="        _thread.sendmsg(_thread.getReplID(), message)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="    # get system time and save it to datetime_dict"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="    datetime = utime.localtime()"/>
                <para PARENT="code"/>
                <ITEXT CH="    datetime_dict = { &quot;year&quot; : datetime[0], &quot;month&quot; : datetime[1], "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="&quot;mday&quot; : datetime[2], &quot;hour&quot;: datetime[3], "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="&quot;minute&quot; : datetime[4], &quot;second&quot; : datetime[5],"/>
                <para PARENT="code"/>
                <ITEXT CH="                  &quot;weekday&quot; : datetime[6], &quot;yearday&quot; : datetime[7]}"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="    # insert datetime_dict values in placeholders in htmlSite"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="    htmlSite = htmlSite.format(**datetime_dict)"/>
                <para PARENT="code"/>
                <ITEXT CH="    # send htmlSite as response to client"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="    httpResponse.WriteResponseOk(headers = ({'Cache-Control': 'no-cache'}),"/>
                <para PARENT="code"/>
                <ITEXT CH="                                    contentType     = 'text/html',"/>
                <para PARENT="code"/>
                <ITEXT CH="                                    contentCharset  = 'UTF-8',"/>
                <para PARENT="code"/>
                <ITEXT CH="                                    content = htmlSite)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="# create instance of MicroWebSrv and start server"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="srv = MicroWebSrv(webPath = 'www')"/>
                <para PARENT="code"/>
                <ITEXT CH="srv.Start(threaded = srv_run_in_thread, stackSize= 8192)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nachdem alles auf dem ESP aktualisiert und neugestartet wurde, sollte nun auf der Webseite die Systemzeit angezeigt werden."/>
