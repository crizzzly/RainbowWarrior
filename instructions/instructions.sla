        <PAGEOBJECT XPOS="170.8" YPOS="28311.28" OwnPage="32" ItemID="1913195473" PTYPE="4" WIDTH="453.68" HEIGHT="394.970000000012" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 394.97 L0 394.97 L0 0 Z" copath="M0 0 L453.68 0 L453.68 394.97 L0 394.97 L0 0 Z" gXpos="170.8" gYpos="28311.28" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1910905665"/>
        <PAGEOBJECT XPOS="170.8" YPOS="30303.3908063432" OwnPage="34" ItemID="1913198817" PTYPE="4" WIDTH="453.68" HEIGHT="486.059193656787" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 486.059 L0 486.059 L0 0 Z" copath="M0 0 L453.68 0 L453.68 486.059 L0 486.059 L0 0 Z" gXpos="170.8" gYpos="30303.3908063432" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1915810273" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Funktionen"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu diesem Zweck gibt es Funktionen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Bevor man eine Funktion implementieren möchte, muss man sich darüber im Klaren sein:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="1. Welche Anweisungen in dieser Funktion gemacht werden - "/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <tab/>
                <ITEXT CH="Funktionsname daher möglichst beschreibend wählen"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="2. ob oder welche Werte dazu nötig sind, die man ggf. verändern kann"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="3. ob die Funktion irgend etwas zurückgeben muss. "/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <tab/>
                <ITEXT CH="wie True/False bei is_dark()"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Funktionen werden mit dem Schlüsselwort &quot;"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="def"/>
                <ITEXT CH="&quot; (=define) eingeleitet. Der Aufbau sieht folgendermaßen aus:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def funktions_name(parameterliste):"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="&quot;&quot;&quot; funktion_name(parameterlist)"/>
                <para PARENT="Kommentare - Block"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="an example function to show structure of functions. "/>
                <para PARENT="Kommentare - Block"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="parameterlist:  (type of parameter: Text or number) short description "/>
                <tab/>
                <ITEXT CH="what parameter(s) is / are needed"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="returns: (type of returned value) if a value is returned, describe it "/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="shortly"/>
                <para PARENT="Kommentare - Block"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="&quot;&quot;&quot;"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="Anweisung(en)"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="(return)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="funktios_name"/>
                <ITEXT CH=": Wie in Punkt 1 beschrieben sollte der Funktionsname stets möglichst treffend beschreiben, was mit dieser Funktion bezweckt werden soll, bzw. was der Code für Auswirkungen hat. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Parameterliste"/>
                <ITEXT CH=" kann aus beliebig vielen Parametern bestehen, die benötigt werden, um die in der Funktion stehenden Anweisungen auszuführen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Bei dem in grün dargestellten Text handelt es sich um einen &quot;docstring&quot;. Diese werden genutzt, um Funktionen und Module zu beschreiben. Mehr dazu im Kapitel &quot;Kommentare in Python&quot;"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Das "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="return"/>
                <ITEXT CH=" Statement ermöglicht es einem, Ergebnisse von Berechnungen, oder eben Werte eines ausgelesenen Sensors, wieder zurück zu geben. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Diesen Wert kann man also mit dem Funktionsaufruf in eine Variable speichern. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Weitere Infos findet man wie Immer im Netz. Zum Beispiel auf:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="http://python4kids.net/how2think/kap05.htm "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="https://www.python-kurs.eu/python3_funktionen.php"/>
                <para PARENT="links"/>
                <ITEXT CH="https://www.python-kurs.eu/python3_parameter.php"/>
                <para PARENT="links"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wollten wir jetzt, als sehr einfaches Beispiel, eine Funktion schreiben, in der zwei Werte (meist Parameter genannt) miteinander addiert werden sollen, würden wir wie folgt vorgehen: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu 1. Wir wollen eine Funtion, in der zwei Zahlen miteinander addiert werden. "/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="Nennen wir sie also einfach &quot;addieren&quot;. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu 2. Zwei Werte sind dazu nötig. Der erste und der zweite Summand."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu 3. Die Summe der Addition soll zurückgegeben werden. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def addieren(a, b): "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="e = a + b"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="return e"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="ergebnis = addieren(a, b)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Übertragen auf die Blink-Funktion würde es also folgendermaßen aussehen: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu 1. Wir wollen den LED-Strip blinken lassen. Daher nennen wir die Funktion "/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="blink()"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu 2. Wir möchten die Farbe und die Zeiten (an + aus) verändern. Brauchen "/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="dazu also drei Werte. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu 3. Da wir lediglich eine Ausgabe an einen Pin machen, brauchen wir "/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="keinen Rückgabewert"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="In der bisherigen Blink-Funktion wurden festen Werten für die Farbe und die Zeiten verwendet. Diese müssen nun durch Variablen ersetzen, dessen Wert erst bei Aufruf der Funktion bestimmt, bzw übergeben wird. "/>
                <para PARENT="Default Paragraph Style"/>
                <breakline/>
                <ITEXT CH="Auch bei Variablen gilt stets darauf zu achten, treffende Namen zu wählen, um die Lesbarkeit zu gewährleisten und  - ganz wichtig - um auch nach einigen Wochen oder Monaten noch nachvollziehen zu können, was hier genau geschieht."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Für die Farbe bietet es sich daher an, die Variable &quot;color&quot; zu nennen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Für die Zeit, die die LEDs leuchten sollen,  &quot;time_on&quot;"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Für die Zeit, die die LEDs nicht leuchten sollen, &quot;time_off&quot;"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; def blink(color, time_on, time_off): "/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="strip.set(0, color, num=num_pixels"/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(time_on)"/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="strip.set(0, 0x00, num=num_pixels)"/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(time_off)"/>
                <tab/>
                <para PARENT="code"/>
                <para/>
                <ITEXT CH="Nun ist es ganz einfach möglich, mit den Farbwerten und Blink-Intervallszeiten herumzuspielen. "/>
                <para/>
                <ITEXT CH="Nachdem die Funktion in REPL eingetragen und fehlerfrei gespechert wurde, kann man sie mit ..."/>
                <para/>
                <para/>
                <ITEXT CH="&gt;&gt;&gt; blink(strip.RED, 200, 10"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="... aufrufen. "/>
                <para/>
                <ITEXT CH="Gerne kann anstatt des Standardwerts für color auch mit Farbwerten experimentiert werden. Im Netz findet sich unter dem englischen Suchbegriff &quot;hex color picker&quot; (= hexadezimaler Farbwähler) eine einfache Möglichkeit, die gewünschte Farbe als Hexadezimalwert zu erhalten. Keine Sorge, falls hier öfter Fehler passieren. Dies passiert immer wieder und soll nur beim Lernen unterstützen!"/>
                <trail/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Funktionen" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="30956.95" OwnPage="35" ItemID="1915810273" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="30956.95" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1915802177" BACKITEM="1913198817"/>
        <PAGEOBJECT XPOS="170.8" YPOS="31838.84" OwnPage="36" ItemID="1915802177" PTYPE="4" WIDTH="453.68" HEIGHT="379.200000000001" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 379.2 L0 379.2 L0 0 Z" copath="M0 0 L453.68 0 L453.68 379.2 L0 379.2 L0 0 Z" gXpos="170.8" gYpos="31838.84" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1915810273"/>
        <PAGEOBJECT XPOS="170.8" YPOS="33266.9971428571" OwnPage="37" ItemID="1907314001" PTYPE="4" WIDTH="453.68" HEIGHT="120.752857142899" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 120.753 L0 120.753 L0 0 Z" copath="M0 0 L453.68 0 L453.68 120.753 L0 120.753 L0 0 Z" gXpos="170.8" gYpos="33266.9971428571" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Aufgabe: Blinken in zwei Farben "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Schaffst du es auch, den Strip anstatt ein- und auszuschalten zwischen zwei Farben wechseln zu lassen? "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Oder abwechselnd in zwei Farben blinken zu lassen?"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Aufgabe - Blinken in zwei Farben" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="32229.06" OwnPage="36" ItemID="1916246513" PTYPE="4" WIDTH="453.68" HEIGHT="324.169999999987" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 324.17 L0 324.17 L0 0 Z" copath="M0 0 L453.68 0 L453.68 324.17 L0 324.17 L0 0 Z" gXpos="170.8" gYpos="32229.06" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1917432737" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Funktionen mit optionalen Parametern "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Sicherlich ist bereits aufgefallen, dass bei der Funktion strip.set(), im Falle, dass alle LEDs gleichzeitig geschalten werden sollen, die Anzahl der zu schaltenden LEDs als Parameter &quot;num= ..&quot; übergeben wird. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hierbei handelt es sich um einen optionalen Parameter, auch Default-Parameter genannt."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies ist sehr praktisch, da wir hierfür einen Standardwert (Default-Wert) nutzen können, der bei Bedarf verändert werden kann. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wenn im Vorfeld bereits ansprechende Zeit-Werte durch Versuche ermittelt wurden, kannst man diese ebenso als optionale Parameter angeben: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; def blink(color, time_on = 200, time_off = 100): "/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="strip.set(0, color, num=num_pixels"/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(time_on)"/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="strip.set(0, 0x00, num=num_pixels)"/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(time_off)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu beachten ist hier lediglich, dass die Default-Parameter grundsätzlich zuletzt genannt werden, da alle Variablen davor, hier lediglich &quot;color&quot;, sogenannte "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Positionsparameter"/>
                <ITEXT CH=" sind, sprich anhand ihrer Position identifiziert werden. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die ursprüngliche Fassung der Blink-Funktion bestand nur aus Positionsparametern. color, time_on, time_off wurden beim Funktionsaufruf in dieser Reihenfolge eingegeben und auch so zugewiesen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Da wir nun time_on und time_off als optionale Parameter definiert haben, dürfen diese genau aus diesem Grund nicht am Anfang stehen. Ansonsten würde die Gefahr bestehen,dass die Werte falsch zugeordet werden! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Würden wir nun alle drei Werte nennen, müssten wir die Schlüsselwörter (time_on / time_off) nicht zwingend mit angeben."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Falls wir aber nur den Wert für time_off ändern wollten, den für time_on aber auf default lassen würden, können wir time_on also einfach auslassen und die Funktion mit ..."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; blink(0x3F03F, time_off=50)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="... aufrufen."/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Funktionen mit optionalen Parametern" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="32720.73" OwnPage="37" ItemID="1917432737" PTYPE="4" WIDTH="453.68" HEIGHT="322.020000000015" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 322.02 L0 322.02 L0 0 Z" copath="M0 0 L453.68 0 L453.68 322.02 L0 322.02 L0 0 Z" gXpos="170.8" gYpos="32720.73" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1916246513"/>
        <PAGEOBJECT XPOS="170.8" YPOS="34484.51" OwnPage="39" ItemID="1917446913" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="34484.51" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1916081297" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Möchte man innerhalb des neu erstellten Projektordners eine neue Datei anlegen, kann man dies, indem man mit der rechten Maustaste das Kontextmenü des Projektordners öffnet und &quot;new File&quot; wählt."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Alternativ kann man bei markiertem Projektordner die Taste A drücken."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="In beiden Fällen öffnet sich ein Eingabefenster, in dem der Name der Datei - Inklusive Dateiendung (animations.py) eingegeben werden muss."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Im rechten Bereich befindet sich dann der eigentliche Texteditor, hier wird der Inhalt der geöffneten (Projekt-)Dateien (in allen gängigen Sprachen) angezeigt. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Außerdem wird hier nun, nach und nach, beginnend mit dem import-Statement, alles bisher gemachte übertragen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Sehr schnell erkennt man hier die Vorteile: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Durch farbige Hervorhebungen wirkt der Text auf Anhieb viel strukturierter. "/>
                <breakline/>
                <ITEXT CH="So werden zum Beispiel alle Schlüsselwörter (import, def, ...) lila gekennzeichnet. Alle Funktionen, Konstruktoren sind cyan, sowie Werte orange gekennzeichnet sind."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Auch die Zeilenzahlen am linken Rand werden noch von Vorteil sein, wenn es zu den ersten Syntaxfehlern (oder manchmal einfach Tippfehlern) kommt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nachdem nun alles in den Editor übertragen und gespeichert wurde, müssen wir aber doch wieder das Terminal nutzen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Atom.io beinhaltet ebenso ein Terminal, das praktischerweise beim Start sofort den Projektordner als Arbetsverzeichnis öffnet. Diesen erreicht man über das kleine '+' - Zeichen am linken unteren Rand."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH=" "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Von hier aus können wir direkt via rshell eine Verbindung zum ESP herstellen: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="$ rshell --buffer-size=30 -p /dev/ttyUSB0 -a -e nano"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="bevor wir aber REPL starten, kopieren wir das erstellte Skript auf den ESP: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="$ cp animations.py /flash"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Anschließend müssen wir den ESP neu starten. Hierzu einfach den Reset-Button (rst) auf dem Controller dücken."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun können wir den REPL-Mode starten. Also wieder einfach &quot;repl&quot; im Terminal eingeben und mit Enter bestätigen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um nun die Funktion blink zu starten, müssen wir das Skript zuerst importieren. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; import animations"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wie wie unschwer zu verkennen, funktioniert dies auf die selbe Weise wie auch vorhandene Module eingefügt werden. Dies bedeutet, dass mit diesen wenigen Codezeilen bereits ein Mini-Modul erstellt wurde! Es besteht zwar momentan nur aus einer Funktion, aber die wird nicht sehr lange alleine bleiben! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Oooops! Schon hat sich ein kleiner 'Fehlerteufel' eingeschlichen. Wer findet den Fehler? "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Aber so wird gleich klar, wozu die Zeilenanzeige im Editor gut ist! "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Bei dieser Fehlermeldung sind für uns erst einmal die letzten beiden Zeilen relevant. In der letzten steht die Art des Fehlers, der gefunden wurde - also ein Syntax-Fehler. Die Zeile darüber sagt mir, welche Datei betroffen ist und in welcher Zeile es zu diesem Fehler gekommen ist - also Zeile 11. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="utime.sleep_ms(time_on) "/>
                <ITEXT FONT="DejaVu Sans Book" CH="in Zeile 11 sieht allerdings richtig aus. Kein Syntax-Fehler zu finden! In diesem Fall sollte man sich auch die darüberliegende Zeile genauer ansehen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="strip.set(0, color, num=num_pixels "/>
                <ITEXT FONT="DejaVu Sans Book" CH="in Zeile 10 - hier stimmt definitiv etwas nicht! Eine Klammer wurde geöffnet, aber nicht wieder geschlossen! So hat der Interpreter verstanden, dass die darauf folgende Zeile - Zeile 11 - also noch Bestandteil dessen ist, was in die Klammer gehört. Da dies aber so keinen Sinn macht, gab es nen Syntax-Fehler. "/>
                <ITEXT CH="Und da dieser erst in Zeile 11 als Fehler registriert wurde, wurde auch Zeile 11 angegeben! "/>
                <breakline/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Oft kommt es vor, dass selbst dies nicht hilft, um die Fehlerquelle zu finden. Dann lohnt sich ein Blick auf die Zeilen der Terminalausgabe, die sich über der letzten beiden befinden. Hier wird dann der Backtrace aufgelistet. Sprich jede Funktion, jeder übergebene Parameter, der mit diesem Fehler zusammenhängen könnte, wird gelistet. So kann man Schritt für Schritt verfolgen, was passiert ist und somit ermitteln, was zu diesem Fehler geführt hat. Debugging nennt man diesen Prozess, und die hier gezeigte Methode ist relativ simpel - allerdings auch nur geeignet, um kleine, überschaubare Programme zu debuggen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Ein kleiner Scherz am Rande: Debugging is like being the detective in a crime movie where you are also the murderer. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Es kann sehr nervenaufreibend sein, und jede(r) kann immer wieder an den Punkt kommen, an dem sie / er alles in Zweifel stellt. "/>
                <breakline/>
                <ITEXT CH="Dies ist also relativ normal! Und: Es geht vorbei! Irgendwann findet man den - oft zu banalen - Fehler und prompt geht es weiter!"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Also. Im Editor Klammer einfügen. Skript speichern. Im Terminal mit STRG+X REPL beenden. Skript erneut kopieren und REPL wieder starten. ESP neu starten. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Tipp für &quot;faule&quot;:  Man kann diese Befehle auch auf eine Zeile komprimieren! In Linux ist die Systemsprache C. Sprich Linux wurde komplett in C geschrieben. In fast allen Programmiersprachen (außer Python) werden die Zeilenenden mit einem &quot;;&quot; versehen, um dem Compiler oder dem Interpreter zu signalisieren, dass der Befehl hier nun zuende ist. Selbes kann man sich auch im Terminal zunutze machen, indem der Befehl zum kopieren und der zum REPL starten auf eine Zeile - getrennt durch einen ';' - geschrieben wird:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="$ cp animations.py; repl"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Auch wenn in Python keine Semikolons nötig sind um die Zeile (oder den Befehl) zu beenden, funktioniert selbes auch im REPL-Mode!"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Da wir wieder den Controller neu starten müssen, können wir - anstatt den Button zu drücken - hier einfach eingeben: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; import machine; machine.reset()"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun sollte das Importieren problemlos funktionieren. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Übrigens: Wenn der ESP nicht gerade außer Betrieb (= ohne Strom) war, muss man eventuell das machine-Modul nicht erneut einbinden."/>
                <para/>
                <para/>
                <ITEXT CH="Dies lässt sich testen, indem man &quot;ma&quot; in REPL eingibt und anschließend die TAB-Taste drückt. Sollte das Modul bereits verfügbar sein, wird die Autovervollständigung den Rest des Wortes &quot;machine&quot; einfügen."/>
                <trail/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="34705.9525" OwnPage="39" ItemID="1916089553" PTYPE="2" WIDTH="453.68" HEIGHT="176.4375" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" TEXTFLOWMODE="2" LOCALSCX="0.75" LOCALSCY="0.75" LOCALX="0" LOCALY="-15.75" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" Pagenumber="0" PFILE="../../../../Schreibtisch/pics-projekt/atom- 01 blinkCode.png" IRENDER="0" EMBEDDED="0" path="M0 0 L453.68 0 L453.68 176.438 L0 176.438 L0 0 Z" copath="M0 0 L453.68 0 L453.68 176.438 L0 176.438 L0 0 Z" ImageRes="0" gXpos="170.8" gYpos="34705.9525" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="170.8" YPOS="35366.4" OwnPage="40" ItemID="1916081297" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="35366.4" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1921260737" BACKITEM="1917446913"/>
        <PAGEOBJECT XPOS="170.8" YPOS="36248.29" OwnPage="41" ItemID="1921260737" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="-424.48" gYpos="36248.29" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1916081297"/>
        <PAGEOBJECT XPOS="170.8" YPOS="38012.07" OwnPage="43" ItemID="1921279185" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="-9.19999999999999" gYpos="3151.78897460018" gWidth="475.28" gHeight="672.176923800565" LAYER="0" NEXTITEM="1924573121" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="12" CH="Dateisystem auf dem ESP"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Schauen wir uns nun mal die vorhandenen Dateien auf dem ESP an. Dazu müssen wir zuerst REPL mit STRG+X wieder verlassen um anschließend mit "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH=" ls /flash"/>
                <ITEXT FONTSIZE="12" CH=" den Inhalt des flash-Speichers auf dem ESP32 ausgeben zu lassen: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="10" CH="&gt;&gt;&gt; ls /flash"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="gibt uns nun den aktuellen Inhalt des Flash-Speichers auf dem ESP aus. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Neben unserer &quot;animations.py&quot; befindet sich nur eine Datei auf dem Controller: &quot;boot.py&quot;. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Dies ist die erste Datei, die beim Boot-Vorgang (oder einfach beim Starten) des ESP ausgeführt wird. Hier sollte lediglich Code stehen, der finale Einstellungen vornimmt, um den Boot-Prozess abzuschließen. Sehr viel passiert hier nicht - und wir sollten die Datei auch zum jetzigen Zeitpunkt nicht verändern, jedoch darf gerne mal einen Blick darauf geworfen werden. Damit die Datei auch im Projektordner landet und nicht unter Umständen irgendwo im lokalen Dateisystem &quot;verloren&quot; geht, muss hier als zweites Argument (Ziel) der vollständige Pfad zum Projektordner eingegeben werden:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="10" CH="&gt;&gt;&gt; cp /flash/boot.py [/path/to/your/projectDirectory]/[projectDir]"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="&gt;&gt;&gt; cp /flash/boot.py /home/chrissi/coding/microPy/rainbowWarror"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="kopiert die Datei vom Controller in dein lokales Filesystem, sodass diese mit jedem beliebigen Texteditor gelesen werden kann. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Hier wird das Modul &quot;"/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="sys"/>
                <ITEXT FONTSIZE="12" CH="&quot; (Abkürzung für System) eingebunden, und der Liste sys.path (eine Variable des Moduls sys) ein Pfad zu einem Ordner namens 'lib' auf dem Flash-Speicher als Text zugewiesen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Um herauszufinden, was hier genau geschieht, lohnt es sich, einen Blick in die Dokumentation der Micropython-Implementierung zu werfen:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="https://docs.micropython.org/en/latest/pyboard/library/sys.html"/>
                <para PARENT="links"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="sys.path"/>
                <ITEXT FONTSIZE="12" CH=": A mutable list of directories to search for imported modules."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Bedeutet, dass hier eine veränderbare Liste an Ordnern in der Variablen gespeichert wird, in denen nach eingefügten Modulen gesucht wird. Sprich falls wir je weitere (externe) Module benötigen, die nicht in der Micropython-Implementierung vorhanden sind, müssen diese im Ordner &quot;flash/lib&quot; gespeichert werden, andererseits werden sie wahrscheinlich nicht gefunden."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Nachdem "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="&quot;boot.py"/>
                <ITEXT FONTSIZE="12" CH="&quot; ausgeführt wurde, wird standardgemäß nach einer File namens &quot;"/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="main.py"/>
                <ITEXT FONTSIZE="12" CH="&quot; gesucht. Falls diese vorhanden ist, wird sie ausgeführt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Nur, was macht main.py? "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Dies kommt sehr darauf an, wofür der ESP eingesetzt werden soll! So können hier weitere Module eingebunden und ausgeführt werden, wie unsere Blink-Funktion."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Daher können wir nun alles in main.py übertragen, was wir nach dem letzten Neustart in REPL eingegeben haben, sodass die Funktion &quot;blink&quot; automatisch nach dem Boot-Prozess startet. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Im Editor erstellen wir also eine neue Datei namens &quot;main.py&quot;, die im Projektordner gespeichert wird und fügen die zuletzt in REPL ausgeführten Zeilen ein. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="In Atom können nicht nur mehrere Dateien gleichzeitig geöffnet sein (Zugriff über die Reiter), es gibt auch einen sogenannten Split-Mode, mit dem wir zwei Dateien nebeneinander setzen können. Dazu klicken wir auf den Reiter &quot;animations.py&quot;, halten die Taste gedrückt und ziehen sie in den rechten Bereich des Fensters. So kann man beide Dateien nebeneinander betrachten, was gerade beim Schreiben von Funktionsaufrufen praktisch sein kann."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="In der Zwischenzeit wurden die gestellten Aufgaben sicherlich bereits erledigt? Für den weiteren Verlauf des Projekts ist es gut, wenn wir der Blink-Funktion mitteilen können, wie oft sie blinken kann. Dazu ist ein weiterer optionaler Parameter notwendig. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Vorsicht bei der Namensgebung für diesen Parameter! Hier würde sich &quot;range&quot; als treffender Name anbieten. Jedoch kann dies zu Konflikten führen. Ebenso darf man keine Schlüsselwörter als Variablennamen einsetzen. Daher nennen wir diese Variable lieber &quot;count&quot;"/>
                <para PARENT="Default Paragraph Style"/>
                <para/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Dateisystem auf dem ESP " Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="38452.6075" OwnPage="43" ItemID="1924336977" PTYPE="2" WIDTH="453.68" HEIGHT="62.8125" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" TEXTFLOWMODE="2" LOCALSCX="0.75" LOCALSCY="0.75" LOCALX="-17.0666666666667" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" Pagenumber="0" PFILE="../../../../Schreibtisch/pics-projekt/groß/boot-py.png" IRENDER="0" EMBEDDED="0" path="M0 0 L453.68 0 L453.68 62.8125 L0 62.8125 L0 0 Z" copath="M0 0 L453.68 0 L453.68 62.8125 L0 62.8125 L0 0 Z" ImageRes="0" gXpos="-9.19999999999999" gYpos="3592.3264746002" gWidth="475.28" gHeight="672.176923800565" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="170.8" YPOS="38893.96" OwnPage="44" ItemID="1924573121" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="-9.19999999999999" gYpos="2659.89978094339" gWidth="475.28" gHeight="672.176923800565" LAYER="0" NEXTITEM="-1" BACKITEM="1921279185"/>
        <PAGEOBJECT XPOS="170.8" YPOS="39110.3716936568" OwnPage="44" ItemID="1924365137" PTYPE="2" WIDTH="453.68" HEIGHT="205.378306343198" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" TEXTFLOWMODE="2" LOCALSCX="0.75" LOCALSCY="0.75" LOCALX="-11" LOCALY="-0.333333333333333" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" Pagenumber="0" PFILE="../../../../Schreibtisch/pics-projekt/atom-split.png" IRENDER="0" EMBEDDED="0" path="M0 0 L453.68 0 L453.68 205.378 L0 205.378 L0 0 Z" copath="M0 0 L453.68 0 L453.68 205.378 L0 205.378 L0 0 Z" ImageRes="0" gXpos="-9.19999999999999" gYpos="2876.31147460019" gWidth="475.28" gHeight="672.176923800565" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="170.8" YPOS="39775.85" OwnPage="45" ItemID="1924490113" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="38012.07" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1937359745" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Animationen Teil 2: running dots"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Eine Möglichkeit, mehr Bewegung in die Animation einzubringen, ist es, wenn zum Beispiel nur eine LED zur gleichen Zeit - jedoch nach und nach um eine Position verschoben - leuchtet. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nur - wie erreichen wir das? "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="schalte LED an Pos 0 ein - warte - schalte LED an Pos 0 aus "/>
                <para PARENT="Default Paragraph Style" ALIGN="1"/>
                <ITEXT CH=" schalte LED an Pos 1 ein - warte - schalte LED an Pos 1 aus"/>
                <para PARENT="Default Paragraph Style" ALIGN="1"/>
                <ITEXT CH=" ....... "/>
                <para PARENT="Default Paragraph Style" ALIGN="1"/>
                <ITEXT CH="schalte LED an letzter Pos ein - warte - schalte LED an letzter Pos aus"/>
                <para PARENT="Default Paragraph Style" ALIGN="1"/>
                <ITEXT CH=" "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hierfür wird also wieder eine for-schleife benötigt, wobei die obere Grenze der range-Funkton gleich der Anzahl der LED (also num_led) ist. Anders geschrieben lautet dieser Pseudocode also:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="für jedes LED in der range (0, num_led):"/>
                <para PARENT="Default Paragraph Style" ALIGN="1"/>
                <ITEXT CH="schalte LED ein - warte - schalte LED aus"/>
                <para PARENT="Default Paragraph Style" ALIGN="1"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Diese Funktion  wird einfach in animations.py eingefügt "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def running_dot(color = 0x00FFFF, time_on=50):"/>
                <para PARENT="code"/>
                <ITEXT CH="    while True:"/>
                <para PARENT="code"/>
                <ITEXT CH="        for i in range(num_pixels):"/>
                <para PARENT="code"/>
                <ITEXT CH="            strip.set(i +1, color)"/>
                <para PARENT="code"/>
                <ITEXT CH="            utime.sleep_ms(time_on)"/>
                <para PARENT="code"/>
                <ITEXT CH="            strip.set(i +1, 0x00)"/>
                <para PARENT="code"/>
                <ITEXT CH="            utime.sleep_ms(1)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun soll wieder erreicht werden, dass die Farbe und die Zeit, die die LED leuchten sollen, bestimmt werden können, daher werden color und time_on als optionale Parameter angegeben. color = 0x00FFFF ergibt die Farbe cyan, mit time_on = 50 ms läuft die Animation relativ zügig durch."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hier ist am Ende der Schleife ein kurzes Delay von min 1ms notwendig, da es sonst zu fehlerhaften Zuweisungen auf dem Strip kommt. Würden wir dieses Delay auslassen, würden die LEDs in allen möglichen Farben kreuz und quer leuchten!"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hier muss ich auf eine kleine &quot;Besonderheit&quot; in dieser MicroPython Implementierung hinweisen, die leider nicht den gängigen Programmierkonventionen entspricht! "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Normalerweise fängt der Informatiker bei 0 an zu zählen. Von 0 bis 9 hat man genau 10 Ziffern. Und zwar genau die 10 Ziffern, die notwendig sind, um alle Zahlen im 10er Zahlensystem darzustellen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies bedeutet, dass - normalerweise - das Element, das sich an erster Stelle befindet, mit dem Index 0 angesprochen wird. Ebenso wird das Element, das sich an Stelle n befindet, mit dem Index n-1 angesprochen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Auf den Strip übertragen würde dies bedeuten, die LED an 1. Stelle würde mit pos = 0 angesprochen werden, die LED an der letzten Stelle mit pos = (num_pixels -1)"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies ist hier aber nicht der Fall! Die erste LED kann man sowohl mit pos = 0 als auch mit pos = 1 ansprechen. Dagegen wird mit pos = num_pixels-1 die vorletzte LED leuchten, nicht etwa die letzte. Die letzte LED wird mit num_pixels angesprochen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Daher müssen wir in strip.set() als Index grundsätzlich i+1 übergeben! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Gestartet wird die Animation wieder in &quot;main.py&quot;. Da alle Parameter optional sind, muss man auch beim Funktionsaufruf nicht zwingend Parameter übergeben! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="animations.running_dots() "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="... führt die Animation mit Default-Werten aus. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Animationen Teil 2: running dots" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="35701.5775" OwnPage="40" ItemID="1927329201" PTYPE="2" WIDTH="453.68" HEIGHT="66.5625" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" TEXTFLOWMODE="2" LOCALSCX="0.75" LOCALSCY="0.75" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" Pagenumber="0" PFILE="../../../../Schreibtisch/pics-projekt/groß/animationen-terminal-fehler.png" IRENDER="0" EMBEDDED="0" path="M0 0 L453.68 0 L453.68 66.5625 L0 66.5625 L0 0 Z" copath="M0 0 L453.68 0 L453.68 66.5625 L0 66.5625 L0 0 Z" gXpos="170.8" gYpos="35701.5775" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="170.8" YPOS="37130.18" OwnPage="42" ItemID="1927327553" PTYPE="4" WIDTH="453.68" HEIGHT="164.199999999997" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 164.2 L0 164.2 L0 0 Z" copath="M0 0 L453.68 0 L453.68 164.2 L0 164.2 L0 0 Z" gXpos="170.8" gYpos="37130.18" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Skript auf dem ESP ausführen "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Eine Funktion in einem Modul wird aufgerufen, indem man zuerst den Modulnamen schreibt, anschließend, durch einen Punkt getrennt, der Funktionsnamen inkl. Parameter in der Klammer: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; import animations"/>
                <para PARENT="code"/>
                <ITEXT CH="&gt;&gt;&gt; animations.blink(0x00ff00)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun haben wir zwar eine Blink-Funktion, diese lässt die LED allerdings wieder nur 1x blinken! "/>
                <para/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Skripte auf dem ESP ausführen" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="37702.7975" OwnPage="42" ItemID="1927608897" PTYPE="4" WIDTH="453.68" HEIGHT="141.772499999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 141.772 L0 141.772 L0 0 Z" copath="M0 0 L453.68 0 L453.68 141.772 L0 141.772 L0 0 Z" gXpos="170.8" gYpos="37702.7975" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Aufgabe: Schleifen erstellen "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Erstelle nun anhand der vorherigen Beispiele eine Blink-Funktion, die "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="1. 10 x blinkt (einmal mit for- und einmal mit while-Schleife) "/>
                <para PARENT="numerierte Liste"/>
                <ITEXT CH="2. für immer blinken wird. "/>
                <para PARENT="numerierte Liste"/>
                <ITEXT CH="3. (Special) erweitere die Funktion (Parameterliste und Funktionsrumpf) so, dass beim Funktionsaufruf definiert werden kann, wie oft die Schleife durchlaufen werden soll. "/>
                <para PARENT="numerierte Liste"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Aufgabe: Schleifen erstellen" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="34172.1516936567" OwnPage="38" ItemID="1927835633" PTYPE="2" WIDTH="453.68" HEIGHT="144.858306343245" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="0.75" LOCALSCY="0.75" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" Pagenumber="0" PFILE="../../../../Schreibtisch/pics-projekt/groß/atom-start.png" IRENDER="0" EMBEDDED="0" path="M0 0 L453.68 0 L453.68 144.858 L0 144.858 L0 0 Z" copath="M0 0 L453.68 0 L453.68 144.858 L0 144.858 L0 0 Z" ImageRes="0" gXpos="170.8" gYpos="34172.1516936567" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="170.8" YPOS="43303.41" OwnPage="49" ItemID="1928019569" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="43303.41" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="running dots mit Schweif - white level"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wollen wir nun erreichen, dass nicht nur eine, sondern 3 aneinandergereihte LEDs leuchten, diese aber mit jeder LED an Farbintensität abnimmt, können wir die vierte Farbe der SK2812 LEDs nutzen: Weiß! Diesen Farbwert steuert man in strip.set() über den Parameter 'white'. Dieser kann Werte zwischen 0 und 255 annehmen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die zweite LED, die also etwas weniger farbintensiv sein soll, können wir ganz einfach in der Schleife mit pos=i-1 ansprechen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="strip.set(i-1, color, white=100) "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="... würde also die LED ansprechen, die sich direkt hinter der aktuell leuchtenden befindet."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="strip.set(i-2, color, white = 150) "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="... würde dann die LED ansprechen, die sich zwei Positionen dahinter befindet! "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Aber: im Fall i=1 wäre i-2 = -1. Ein negativer Index kann aber zu einer Fehlermeldung führen (Index Error - List Index out of range)."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Daher müssen wir garantieren, dass der Wert für pos in strip.set nie kleiner als 1 und auch nie größer als num_pixels ist. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um zu garantieren, dass der Wert für i-1, i-2 nie kleiner als 0 und nie größer oder gleich als num_pixels ist, können wir den Modulo-Operator, also Rest-Division einsetzen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="pos1 = (i - 1) % "/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH="num_pixels"/>
                <ITEXT CH=" "/>
                <para PARENT="code"/>
                <ITEXT CH="pos2 = (i - 2) % num_pixels"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Da bei der Modulodivison - genau wie bei der normalen Division - die Regel &quot;Punkt vor Strich&quot; gilt, muss der Ausdruck (i - 1) zwingend in Klammern. Somit haben wir garantiert, dass pos1 immer kleiner bleibt als "/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH="num_pixels"/>
                <ITEXT CH=", da zum Beispiel 6 % 5 =1 (1 Rest 1).  "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Aber 5 % 5 = 1 Rest 0. Wenn wir Pixel 0 in cyan leuchten lassen, im nächsten Schritt aber Pixel 1, dann würde jeweils die selbe LED leuchten. Dafür würde aber die letzte LED (an Stelle "/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH="num_pixels"/>
                <ITEXT CH=") ausbleiben. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um den &quot;Fehler&quot; in der MicroPython Implementierung wieder aufzufangen,  muss bei strip.set für pos wieder i+1 (, pos1+1, pos2+1, ..) übergeben werden:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="strip.set(i+1, color, white=white_level, update = False)"/>
                <para PARENT="code"/>
                <ITEXT CH="strip.set(pos1+1, color, white=white_level, update = False)"/>
                <para PARENT="code"/>
                <ITEXT CH="strip.set(pos2+1, color, white=white_level, update = False)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Micropython Animationen: running dots mit Schweif - white level" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="41539.63" OwnPage="47" ItemID="1930388257" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="41539.63" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="2003418641" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Kommentare in (Micro)Python "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Nun haben wir eine Situation, in der wir ein Stück Code geschrieben haben, das wir zeitweise &quot;ausblenden&quot; wollen. Eine Möglichkeit wäre, diese einfach zu entfernen. Sobald es aber um mehr als nur eine Zeile geht, läuft man in Gefahr, dass dieser Codeblock in der Zwischenablage &quot;vergessen&quot; und somit schnell gelöscht oder überschrieben werden kann. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Eine andere Möglichkeit bieten Kommentare."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Üblicherweise werden Kommentare eingesetzt um zu beschreiben, was im nachfolgenden Codeblock geschieht. Aber man kann diese während der Entwicklung ebenso einsetzen, um Codezeilen vorübergehend vom Interpreter 'überlesen' zu lassen. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Aus Performancegründen sollten diese Art Kommentare allerdings gelöscht werden, sobald sie nicht mehr gebraucht werden. Spätestens aber wenn das Programm 'fertig' ist."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Man unterscheidet zwischen zwei verschiedenen Arten von Kommentaren: dem Zeilenkommentar und dem Blockkommentar. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Zeilenkommentare"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Zeilenkommentare gehen - wie der Name schon sagt - nur über eine Zeile und werden gekennzeichnet durch eine # zu Beginn der Zeile / des Kommentars. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Man kann Zeilenkommentare auf die komplette Zeile anwenden, oder eben nur auf einen Teil der Zeile. Alles, was sich hinter der # befindet, wird also vom Interpreter übersprungen und nicht ausgeführt. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="# Dies ist ein Zeilenkommentar. "/>
                <para PARENT="Kommentare - Zeilen"/>
                <para PARENT="code"/>
                <ITEXT CH="i = x % 5 "/>
                <ITEXT FCOLOR="middle grey" FSHADE="100" CH="# hier kann man kurz beschreiben, was passiert"/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Zeilenkommentare werden nicht nur genutzt, um Codezeilen vom Interpeter &quot;überlesen&quot; zu lassen, sie werden hauptsächlich eingesetzt, um Codefragmente, auch Routinen genannt, zu beschreiben. Dies dient dazu, den Code lesbarer zu machen. Nicht nur für andere, auch für einen selbst. Nach einiger Zeit weiß man sonst oft nicht mehr, was im entsprechenden Teil geschieht."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Blockkommentare / Docstrings"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Blockkommentare können über mehrere Zeilen gehen. Dazu müssen zu Beginn und am Ende des Blockkommentars drei Anführungszeichen in Folge stehen. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="&quot;&quot;&quot; Dies ist ein Blockkommentar. "/>
                <para PARENT="Kommentare - Block"/>
                <ITEXT CH="Dieser darf auch mehrzeilig sein und wird auch häufig eingesetzt, um Module, bzw. Funktionen zu beschreiben. "/>
                <para PARENT="Kommentare - Block"/>
                <ITEXT CH="&quot;&quot;&quot;"/>
                <para PARENT="Kommentare - Block"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Docstrings - oder auch Documentation-Strings werden meist für die Dokumentation genutzt. Hier wird möglichst prägnant beschrieben, was in der Funktion geschieht. Diese beschreibung sollte so ausfallen, dass andere, die den Code dazu nicht kennen, diesen nachbauen können. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Es gibt zusätzliche Tools, die aus diesen Docstrings eine Dokumentation des Projekts erstellen, die im besten Fall sogar von Menschen verstanden werden, die nicht programmieren können. Außerdem kann man in der Python Shell via my_function.__doc__ oder mit print(my_function.__doc__) diese Beschreibung anzeigen lassen"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Damit aus den Docstrings eine Dokumentation erstellt werden kann, müssen sie ein gewisses Format haben: "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Zuerst kommt eine kurze Beschreibung des Moduls / der Funktion."/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Anschließend sollten die zu übergebenden Parameter und ggf. Return-Value sowie deren Datentyp beschrieben werden. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="&quot;&quot;&quot; running_dot(color = 0x00FFFF, time_on = 50) "/>
                <para PARENT="Kommentare - Block"/>
                <para PARENT="Kommentare - Block"/>
                <ITEXT CH="Neopixel-Animation, every single LED will light up in the color specified by color for a time, specified in time_on. after time has passed, LED is turned off and next LED will light up. "/>
                <para PARENT="Kommentare - Block"/>
                <para PARENT="Kommentare - Block"/>
                <ITEXT CH="color: color (hex) of the LEDs "/>
                <para PARENT="Kommentare - Block"/>
                <ITEXT CH="time_on: time (ms) the LED will light up"/>
                <para PARENT="Kommentare - Block"/>
                <ITEXT CH="&quot;&quot;&quot;&quot;"/>
                <para PARENT="Kommentare - Block"/>
                <para PARENT="erklärungen"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Kommentare in (Micro)Python" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="46258.97" OwnPage="52" ItemID="1932697905" PTYPE="4" WIDTH="453.68" HEIGHT="404.5" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 404.5 L0 404.5 L0 0 Z" copath="M0 0 L453.68 0 L453.68 404.5 L0 404.5 L0 0 Z" gXpos="170.8" gYpos="46258.97" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1939532401" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Bedingte Anweisungen / Verzweigungen"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="In der Programmierung versteht man unter einer bedingten Anwesung (oder Verzweigung) Codeteile, die nur unter bestimmten Bedingungen ausgeführt werden. Liegt diese Bedingung nicht vor, werden diese nicht ausgeführt. Alternativ kann (aber muss nicht) statt dessen ein anderer Codeteil ausgeführt werden. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Anders ausgedrückt: Eine Verzweigung legt fest, welcher von zwei (oder auch mehr) Programmteilen (Alternativen) in Abhängigkeit von einer (oder mehreren) Bedingungen ausgeführt wird. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="So könnte man ganz einfach sagen: "/>
                <para PARENT="erklärungen"/>
                <tab/>
                <ITEXT CH="Wenn dies: mache das. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Dazu könnte man noch definieren: "/>
                <tab/>
                <para PARENT="erklärungen"/>
                <tab/>
                <ITEXT CH="Wenn dies nicht: mache was anderes. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Oder wenn man meherere Bedingungen in Frage kommen könnten:"/>
                <para PARENT="erklärungen"/>
                <tab/>
                <ITEXT CH="Wenn dies:   mache das"/>
                <para PARENT="erklärungen"/>
                <tab/>
                <ITEXT CH="Wenn dies nicht sondern das:  mache jenes"/>
                <para PARENT="erklärungen"/>
                <tab/>
                <ITEXT CH="wenn dies und das nicht:  mache was anderes. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Hierzu nutzt man die if-Anweisung. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Nehmen wir an, wir haben den derzeitigen Wochentag in der Variable &quot;weekday&quot; gespeichert. Wollen wir zum Beispiel erreichen, dass nur am Montag eine Anzeige im Terminal erscheint, die einen an diesen (meist ungeliebten) Tag erinnert schreiben wir: "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="if weekday == &quot;monday&quot;: "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="print(&quot;today is monday. &quot;) "/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="der Operator &quot;==&quot; ist ein sogenannte Gleichheitsoperator. Dieser prüft, ob die beinden Werte (davor und danach) gleich sind. Ist diese Bedingung erfüllt, wird True zurück geliefert, die Bedingung wird somit wahr und der nachfolgende Block darf ausgeführt werden. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Wollen wir, dass die Ausgabe nur erfolgt, wenn der Wochentag nicht der Montag ist, so können wir auch das Gegenteilige mit einem Gleichheitsoperator prüfen. Also ob eine Bedingung nicht stimmt. Hierzu nutzt man &quot;!=&quot; (=ungleich):"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="if weekday != &quot;monday&quot;:"/>
                <para PARENT="erklärungen"/>
                <tab/>
                <ITEXT CH="print(&quot;today's not monday&quot;)"/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Falls wir nur den Samstag oder Sonntag als Tag ausgeben, die restlichen Tage unter der Woche aber nur, dass kein Wochenende ist:"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="if weekday == &quot;saturday&quot;: "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="print(&quot;today is saturday&quot;) "/>
                <para PARENT="code"/>
                <ITEXT CH="elif weekday == &quot;sunday&quot;: "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="print(&quot;today is sunday&quot;)"/>
                <para PARENT="code"/>
                <ITEXT CH="else: "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="print(&quot;no weekend today&quot;) "/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="elif (Abkürzung von else if) wird nur ausgeführt, wenn die erste Bedingung (if) nicht zutrifft."/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Else wird nur ausgeführt, wenn sowohl if als auch elif nicht zutreffen. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Man kann beliebig oft verzweigen."/>
                <para PARENT="erklärungen"/>
                <ITEXT CH=" "/>
                <para PARENT="erklärungen"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Bedingte Anweisungen / Verzweigungen" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="44355.8825367012" OwnPage="50" ItemID="1932177425" PTYPE="4" WIDTH="453.68" HEIGHT="237.617463298797" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 237.617 L0 237.617 L0 0 Z" copath="M0 0 L453.68 0 L453.68 237.617 L0 237.617 L0 0 Z" gXpos="-9.19999999999999" gYpos="27523.4015113014" gWidth="475.28" gHeight="672.176923800575" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="12" CH="Der Touch Sensor"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Unser ESP32 hat praktischerweise einige Sensoren an Board, wie zum Beispiel den Touch Sensor (oder Berührungssensor). "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Genauer gesagt haben wir insgesamt 10 Touch Sensoren. Diese befinden sich an den Pins  0, 2, 4, 13, 12, 14, 15, 27, 33 und 32. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Beim Touch-Sensor handelt es sich um einen Kapazitive Näherungssensoren. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Die Funktion dieses Sensors beruht auf der Änderung des elektrischen Feldes in der Umgebung vor seiner Sensorelektrode (aktive Zone)."/>
                <ITEXT CH=" Diese aktive Zone ist bei dem verbauten Sensor allerdings sehr klein. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Im Prinzip handelt es sich hierbei um eine Art Kondensator. Da sich die Kapazität eines Kondensators mit dem Abstand seiner Elektroden verändert, kann diese messbare Größe zur Erkennung von Berührung eingesetzt werden. "/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Der Touch Sensor" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="44185.3" OwnPage="50" ItemID="1934519537" PTYPE="4" WIDTH="453.68" HEIGHT="192.950000000019" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 192.95 L0 192.95 L0 0 Z" copath="M0 0 L453.68 0 L453.68 192.95 L0 192.95 L0 0 Z" gXpos="-9.19999999999999" gYpos="27352.8189746002" gWidth="475.28" gHeight="672.176923800575" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="20" CH="Animationen steuern"/>
                <para PARENT="Überschrift Mittel"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Nun haben wir einige Animationen gebastelt, zwischen denen wir im Idealfall auch wechseln wollen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Natürlich könnten wir jetzt sagen, wir lassen diese einfach nacheinander abspielen, sodass zum Beispiel Animation Nr. 1 für einige Zeit läuft, anschließend Animation Nr. 2"/>
                <ITEXT CH=", usw. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Besser wäre es doch, wenn wir selbst bestimmen könnten, wann die nächste Animation starten soll. "/>
                <trail PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Animationen steuern " Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="44598.37" OwnPage="50" ItemID="1934896337" PTYPE="4" WIDTH="453.68" HEIGHT="301.319999999978" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 301.32 L0 301.32 L0 0 Z" copath="M0 0 L453.68 0 L453.68 301.32 L0 301.32 L0 0 Z" gXpos="-9.19999999999999" gYpos="27765.8889746002" gWidth="475.28" gHeight="672.176923800575" LAYER="0" NEXTITEM="1937067857" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="12" CH="Wir wollen uns momentan nur auf den Touch Sensor konzentrieren und lassen den Teil mit den Animationen vorerst aus. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Alle Arten von Pins am ESP werden über das machine-Modul eingebunden. Wollen wir also den Touch Sensor an Pin 27 nutzen, schreiben wir: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Mono Book" FONTSIZE="10" CH="import machine"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT FONT="DejaVu Sans Mono Book" FONTSIZE="10" CH="# touch related values"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT FONT="DejaVu Sans Mono Book" FONTSIZE="10" CH="touchPin = 27"/>
                <para PARENT="code"/>
                <ITEXT FONT="DejaVu Sans Mono Book" FONTSIZE="10" CH="touch = machine.TouchPad(machine.Pin(touchPin))"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Somit haben wir eine Instanz des Touchpads in der Variable touch gespeichert."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Wenn wir nun den Wert des Touch Pins auslesen wollen, schreiben wir: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="10" CH="touch.read() "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Da es hin und wieder beim Auslesen zu Fehlern (ValueError) kommen kann und diese dazu führen, dass die Ausführung des kompletten Programms anhält (was nur durch Neustart behoben werden kann), müssen wir diesen Fehler abfangen."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Dieser Fehler tauchte bei meinen Tests anfangs überhaupt nicht auf, später dafür immer wieder. Falls dieser Fehler zu häufig auftreten sollte, empfiehlt es sich, einen anderen Pin als Touch-Sensor zu nutzen, weshalb ich die Pin-Nummer auch in eine separate Variable gespeichert habe. Dies wird, sobald das Programm eine gewisse Größe erreicht hat, zur Übersicht  und besseren Wartbarkeit beitragen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Um den Fehler abzufangen müssen wir dem Programm sagen, dass es versuchen soll, diesen Pin auszulesen. Falls dies nicht möglich sein sollte, kann (muss aber nicht) eine Ausgabe im Terminal geschehen."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Dies regelt man mit einer Aushanhmebehandlung (exception handling). "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Der Code, der das Risiko für eine Ausnahme beherbergt, wird in ein "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="try"/>
                <ITEXT FONTSIZE="12" CH="-Block eingebettet. Abgefangen werden diese Ausnahmen im "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="except"/>
                <ITEXT FONTSIZE="12" CH="-Block."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Wie bereits erwähnt wird die Ausführung des Programms linear vorgenommen. Sprich es wird Zeile für Zeile ausgeführt. Wenn eine Funktion (auch aus anderen Modulen) aufgerufen wird, geht die Ausführung des Programms - wieder Zeile für Zeile - in der aufgerufenen Funktion weiter, bis diese komplett ausgeführt wurde. Anschließend geht die Ausführung weiter in der Zeile nach der aufgerufenen Funktion. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Da wir den Wert des Sensors mehr als nur einmal auslesen wollen um die Werte zu vergleichen, müssen wir den Codeblock wieder in eine Endlosschleife packen. Es reicht, den Wert 5 mal pro Sekunde auszulesen, also nehmen wir einen sleep-Wert von 200 ms."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="10" CH="import machine, utime"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT FONT="DejaVu Sans Condensed" FONTSIZE="10" CH="# touch related values"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT FONTSIZE="10" CH="touch_pin = 27"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="touch = machine.TouchPad(machine.Pin(touch_pin))"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="while True: "/>
                <para PARENT="code"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="# try to read touch pin"/>
                <para PARENT="Kommentare - Zeilen"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="try:"/>
                <para PARENT="code"/>
                <tab FONTSIZE="10"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="touch_val = touch.read()"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="except ValueError:"/>
                <para PARENT="code"/>
                <tab FONTSIZE="10"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="print(&quot;ValueError while reading touch_pin&quot;)"/>
                <para PARENT="code"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="print(touch_val)"/>
                <para PARENT="code"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="utime.sleep_ms(200)"/>
                <tab FONTSIZE="10"/>
                <para PARENT="code"/>
                <para/>
                <ITEXT FONTSIZE="12" CH="Bevor wir aber den Touch Sensor testen können, müssen wir noch ein Kabel an den Touch Pin (IO27) stecken. Nehmt dazu am besten ein JumperWire (male - female), also eins, das man mit der einen Seite über den Pin stülpen kann und bei dem dann auf der anderen Seite eine Spitze herausragt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Main.py Speichern, Datei auf den ESP kopieren, Repl starten und Controller neustarten. Schon sehen wir im Terminal, welche Werte der Sensor hat. Ohne Berührung bewegt sich der Wert zwischen 400 und 500, mit Berührung grob zwischen 30 und 150. "/>
                <ITEXT CH="Diese Werte können allerdings je nach Umgebungsverhältnissen variieren"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Setzen wir also den Schwellwert, ab dem der Sensor als &quot;berührt&quot; gelten soll, auf 180. Falls es im weiteren Verlauf zu Schwierigkeiten kommen sollte, wenn zum Beispiel eine Berührung registriert wurde obwohl keine stattgefunden hat, oder keine Reaktion erfolgen sollte obwhol der Sensor berührt wurde, sollte man diesen Schritt wiederholen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
        </PAGEOBJECT>
