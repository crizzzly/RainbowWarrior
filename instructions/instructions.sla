        <PAGEOBJECT XPOS="180" YPOS="15950.5910253998" OwnPage="35" ItemID="1680750409" PTYPE="4" WIDTH="475.28" HEIGHT="235.688974600191" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 235.689 L0 235.689 L0 0 Z" copath="M0 0 L475.28 0 L475.28 235.689 L0 235.689 L0 0 Z" gXpos="180" gYpos="15950.5910253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1668644793"/>
        <PAGEOBJECT XPOS="735.28" YPOS="5367.91102539981" OwnPage="12" ItemID="1680742713" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800564" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="735.28" gYpos="5367.91102539981" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1639380473"/>
        <PAGEOBJECT XPOS="180" YPOS="6249.80102539981" OwnPage="13" ItemID="1680478681" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800564" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="180" gYpos="6249.80102539981" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Compiler"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Überschrift - normal-fett"/>
                <ITEXT CH="Compiler sind spezielle Übersetzer, die Programmcode aus problemorientierten Programmiersprachen, sogenannten Hochsprachen, in"/>
                <para PARENT="Überschrift - normal-fett"/>
                <ITEXT CH="ausführbaren Maschinencode, also die Sprache, die die Prozessoren verstehen, übersetzen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wenn du dich einmal in der Ordnerstruktur des Micropython Frameworks umsiehst, findest du zum Beispiel im Ordner MicroPython_ESP32_psRAM_LoBoNeu/MicroPython_BUILD/components/micropython/esp32 einige Dateien mit der Endung *.c oder *.h. Dies sind Module, geschrieben in der Programmiersprache C, und die zugehörigen Header-Files."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Diese sind für uns Menschen noch einigermaßen lesbar, der Prozessor, der dies allerdings ausführen muss, kann noch gar nichts damit anfangen. Dieser &quot;versteht&quot; nämlich nur den sogenannten Maschinencode, bestehend aus einer Folge von Bytes, die sowohl Befehle als auch Daten repräsentieren. Da dieser Code für den Menschen schwer lesbar ist, werden in der Assemblersprache (die Sprache, die der Maschiensprache noch am nächsten kommt) die Befehle durch besser verständliche Abkürzungen, sogenannte Mnemonics, dargestellt."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Der Compiler scannt also während des kompiliervorgangs den Code und übersetzt ihn - so ressourcenschonend es geht - in Maschinensprache. Die dadurch entstandene binary lässt sich dann vom Computer ausführen. Nachfolgend ein kleines Beispiel, wie es für den Prozessor aussehen kann, wenn man der Variablen &quot;a&quot; einen Wert zuweist oder wenn man zwei Werte, die in Variablen gespeichert sind, miteinander addiert. Wichtig zu wissen ist hierbei, die Variablen werden in sogenannten Registern gespeichert. Dies sind Speicherbereiche innerhalb des Prozessors, die direkt mit dem Rechenkern verbunden sind."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Falls du mehr darüber erfahren möchtest, hier einige Links:"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="https://de.wikipedia.org/wiki/Compiler"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="https://de.wikipedia.org/wiki/Maschinensprache"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="https://de.wikipedia.org/wiki/Register_(Computer)"/>
                <trail PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Compiler" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.28" YPOS="6634.77897460019" OwnPage="14" ItemID="1677125497" PTYPE="4" WIDTH="475.28" HEIGHT="287.198974600187" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 287.199 L0 287.199 L0 0 Z" copath="M0 0 L475.28 0 L475.28 287.199 L0 287.199 L0 0 Z" gXpos="735.28" gYpos="6634.77897460019" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Interpreter"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Im gegesatz zu einem Compiler, der vor Ausführung des Programms den vorhandenen Code in Maschinensprache übersetzt, macht dies ein sogenannter Interpreter erst währed der Laufzeit."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Vorteil hiervon ist, dass ein zum Beispiel in Python geschriebenes Programm auf verschiedenen Systemen mit verschiedenen  Prozessorarchitekturen ausführbar ist. Leider muss man aber im Gegensatz zum Compiler mit Performanceeinbußen rechnen."/>
                <trail PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Interpreter" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="180" YPOS="19478.1510253998" OwnPage="43" ItemID="1682335945" PTYPE="4" WIDTH="475.28" HEIGHT="288" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 288 L0 288 L0 0 Z" copath="M0 0 L475.28 0 L475.28 288 L0 288 L0 0 Z" gXpos="180" gYpos="19478.1510253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1675507641"/>
        <PAGEOBJECT XPOS="735.28" YPOS="19478.1510253998" OwnPage="44" ItemID="1682341657" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800565" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="735.28" gYpos="19478.1510253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="In unseren Code eingefügt sieht das nun so aus: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="touch_threshold = 150 "/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="while True:"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="try: "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="touch_val = touch.read()"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="expect ValueError:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="print(&quot;TouchPad Error&quot;)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="if touch_val &lt; touch_threshold: "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="animations.next()"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(200)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Jetzt haben wir aber mit animations.next() eine Funktion next() erfunden, die es in animations.py noch gar nicht gibt! "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Diese müssen wir noch in die File animations.py basteln."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wie der Name schon sagt, soll diese Funktion einfach die nächste Animation aufrufen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Am einfachsten erreicht man dies, wenn man alle Funktionen in eine Liste - besser noch in einen Dictionary - schreibt, bei der man ganz einfach sagen kann: starte die Nächste! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="180" YPOS="20360.0410253998" OwnPage="45" ItemID="1682782233" PTYPE="4" WIDTH="475.28" HEIGHT="519.848974600194" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 519.849 L0 519.849 L0 0 Z" copath="M0 0 L475.28 0 L475.28 519.849 L0 519.849 L0 0 Z" gXpos="180" gYpos="20360.0410253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Datentypen in Python Teil 1"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Bisher haben wir nur ganz allgemein über Variable gesprochen."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Variablen werden genutzt, um veränderbare Objekte zu speichern. Man nicht nur einzelne Zahlenwerte, die zum Beispiel als ganze Zahl (Integer) oder Gleitkommazahl (float oder double) vorliegen können, oder sogenannte Strings (einfache Zeichenfolgen, sprich Text - welcher innerhalb doppelter Anführungszeichen stehen muss) in Variablen speichern. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Man kann sogar Listen (also eine Liste an Werten oder Variablen), sogenannte Tupel (bestehend aus mehreren Werten, eingeklammert und durch Komma getrent), Dictionarys (ähnlich den Listen, nur dass jedes Objekt noch einen Bezeichner enthält. Diese werden in geschweiften Klammern angegeben) und sogar ganze Funktionen in Variablen speichern."/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Genauso kann man Funktionen in Listen eintragen. Diese Eigenschaft machen wir uns nun zunutze. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Listen"/>
                <ITEXT CH=" werden erstellt, indem man einer Variable mehrere Werte zuweist, die in eckigen Klammern und durch Komma getrennt werden: "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="my_list = [list_item1, list_item2, list_item3, ... ]"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Analog verfährt man mit den "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Tupeln"/>
                <ITEXT CH=": "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="my_tupel = (item_1, item_2, item_3, ... )"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Dictionaries"/>
                <ITEXT CH=" haben zusätzlich zum Objekt einen Bezeichner, über den man das Objekt in der Liste aufrufen kann:"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="capital_dict = {&quot;Baden-Wuerttemberg&quot;:&quot;Stuttgart&quot;,  &quot;Bayern&quot;:&quot;Muenchen&quot;, ... }"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Mehr Informationen zu den bisher gezeigten Datentypen kann man auf folgenden Links abrufen: "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="https://www.python-kurs.eu/python3_variablen.php"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="https://www.python-kurs.eu/python3_listen.php"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="https://www.python-kurs.eu/python3_dictionaries.php"/>
                <para PARENT="erklärungen"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="E: Datentypen in Python" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.28" YPOS="20360.0410253998" OwnPage="46" ItemID="1680810585" PTYPE="4" WIDTH="475.28" HEIGHT="607.598974600201" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 607.599 L0 607.599 L0 0 Z" copath="M0 0 L475.28 0 L475.28 607.599 L0 607.599 L0 0 Z" gXpos="735.28" gYpos="20360.0410253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Für den jetzigen Verwendungszweck würde eine Liste ausreichen. Aber da wir das ganze später noch ausweiten wollen, werden wir hier einen Dictionary verwenden. Spätestens wenn wir die Webseite erstellen, um bestimmte Animationen auszuwählen, werden wir nicht um einen Dictionary kommen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="In der File animations.py erstellen wir also zuerst einen Dictionary, mit der wir arbeiten können. In dieser Liste werden alle vorhandenen Animationen gespeichert. Wichtig ist, dass diese Variable erst nach den ganzen Funktionen deklariert wird. Beim Import der File animations.py werden - von oben nach unten - alle Variablen und Funktionen angelegt. Würden wir diese zu Beginn der Datei anlegen, würde der Interpreter die Funktionen noch gar nicht kennen und somit einen Fehler melden. Daher wird die Variable und die zugehörige next()-Funktion ans Ende der File gestellt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="animation_dict = [&quot;blink&quot; : blink, &quot;running_dot&quot; : running_dot, &quot;crossing_dot&quot; : crossing_dots, &quot;v_shape&quot; : v_shape ]"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wie man sehen kann, werden hier nur die Bezeichner der Funktionen gespeichert. Würden wir &quot;blink()&quot; anstatt &quot;blink&quot; schreiben, würde das zur sofortigen Ausführung der Funktion führen, was wir aber nicht wollen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Anschließend überlegen wir uns, was genau in der Funktion next() geschehen soll. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Innerhalb dieser Funktion möchten wir, dass, wenn bisher keine Funktion aufgerufen wird, die erste in der Liste gestartet wird. Falls aber im Vorfeld bereits eine gestartet wurde, soll die jeweils nächste gestartet werden."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Eine einfache, aber komplizierte Lösung wäre, dass wir zum Beispiel sagen, dass blink() die Animation Nr 0 ist, running_dot() Animation Nr 1 (...), während ein Zähler die Touch-Eingaben hochzählt."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Einfacher geht es mit einem Iterator"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.28" YPOS="21241.9310253998" OwnPage="48" ItemID="1683894217" PTYPE="4" WIDTH="475.28" HEIGHT="243.6720507996" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 243.672 L0 243.672 L0 0 Z" copath="M0 0 L475.28 0 L475.28 243.672 L0 243.672 L0 0 Z" gXpos="735.28" gYpos="21241.9310253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Lokale und globale Variablen in Python"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Jede Variable, die man in Python in einer Funktion definiert, ist automatisch eine lokale Variable. Sprich sie existiert nur innerhalb dieser Funktion."/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Man kann zwar innerhalb der Funktion auf eine Variable, die außerhalb der Funktion definiert wurde, zugreifen, diese auch verändern. Jedoch hat diese Veränderung keinen Einfluss auf die Variable außerhalb."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Sprich wenn eine Variable x außerhalb der Funktion den Wert 10 hat, wir diesen innerhalb der Funktion durch 2 teilen, hat die Variable außerhalb der Funktion immernoch den Wert 10, während die Variable x in der Funktion den Wert 5 hat."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Weitere Infos sowie einige Beispiele sind unter anderem im Python-Forum zu finden:"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="https://www.python-kurs.eu/python3_global_lokal.php"/>
                <para PARENT="erklärungen"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="E: Lokale und Globale Variablen in Pythin" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="180" YPOS="21241.9310253998" OwnPage="47" ItemID="1685159913" PTYPE="4" WIDTH="475.28" HEIGHT="393.747949200377" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 393.748 L0 393.748 L0 0 Z" copath="M0 0 L475.28 0 L475.28 393.748 L0 393.748 L0 0 Z" gXpos="180" gYpos="21241.9310253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Iterator"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Ein Iterator ist einfach ein Zeiger, der auf das Element einer Liste (oder ähnlichem) zeigt, das gerade an der Reihe ist. Er funktioniert ähnlich wie eine for-Schleife."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="for i in range(3):"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="print(i)"/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="könnte man auch folgendermaßen schreiben: "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="numbers = [0,1,2]"/>
                <para PARENT="code"/>
                <ITEXT CH="num_iterator = iter(numbers)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="while True: "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="try:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="num = num_iterator.__next__()"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="except StopIteration:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="break"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="print(num)"/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Im ersten Moment scheint dies zwar komplizierter, aber oftmals - wie in unserem Fall - definitiv die bessere Wahl."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Zu beachten ist hier auch der try / except - Bereich. Würden wir diesen auslassen, würde es zu einer Fehlermeldung (StopIteration) und folglich zu einer Unterbrechung des Programms kommen, sobald wir das Ende erreicht, bzw. überschritten haben."/>
                <para PARENT="erklärungen"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="E: Iterator" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="180" YPOS="21641.9310253998" OwnPage="47" ItemID="1685961401" PTYPE="4" WIDTH="475.28" HEIGHT="144.958974600195" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 144.959 L0 144.959 L0 0 Z" copath="M0 0 L475.28 0 L475.28 144.959 L0 144.959 L0 0 Z" gXpos="180" gYpos="21641.9310253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Wir möchten aber nicht, dass die Iteration abbricht, nachdem wir die letzte Animation erreicht haben. Wir wollen, dass, wenn das letzte Element erreicht wurde, die Iteration wieder von vorn beginnt."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Also behelfen wir uns eines kleinen Tricks:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wir legen einfach eine neue Iteration an! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies führt uns aber zu einem weiteren Thema, das hier behandelt werden muss: Lokale und globale Variablen."/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.28" YPOS="21559.89" OwnPage="48" ItemID="1682944377" PTYPE="4" WIDTH="475.28" HEIGHT="354.217949200371" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 354.218 L0 354.218 L0 0 Z" copath="M0 0 L475.28 0 L475.28 354.218 L0 354.218 L0 0 Z" gXpos="735.28" gYpos="21559.89" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1688059881" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Die Variable animation_iterator wird außerhalb der Funktion next() deklariert (=angelegt). Würden wir sie erst beim Aufruf der Funktion deklarieren, würde das bedeuten, dass, bei jedem Aufruf die Iteration bei 0 beginnen würde. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Da wir sie aber - um nach Ende der Iteration wieder bei 0 zu beginnen - im except-Block neu anlegen und somit verändern müssen, würde dies zu einer Fehlermeldung führen (Variable referenziert bevor sie deklariert wurde), da wir die Variable zuerst verwenden und anschließend verändern. Gerne darf dieser Effekt ausprobiert werden! "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Daher müssen wir in der Funktion angeben, dass es sich hier um eine globale Variable handelt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Anders verhält es sich bei animation_dict. Diese wird in der Funktion lediglich aufgerufen, nicht aber verändert. Daher muss diese nicht global sein."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="animation_dict = [&quot;blink&quot; : blink, &quot;running_dot&quot; : running_dot, &quot;crossing_dots&quot; : crossing_dots, &quot;v_shape&quot; : v_shape ]"/>
                <para PARENT="code"/>
                <ITEXT CH="animation_iterator = iter(animation_dict)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="def next(): "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="global animation_iterator "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="try: "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="running_function = animation_iterator.__next__()"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="running_function()"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="except StopIteration: "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="animation_iterator = iter(animation_dict)"/>
                <para PARENT="code"/>
                <tab/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="running_function = animation_iterator.__next__() bewirkt, dass die nächste Funktion in die Variable running_function gespeichert wird. Mit running_function() wird diese dann ausgeführt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Fertig für den Test!"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Also, Datei speichern, zum Terminal wechseln. Datei auf den ESP kopieren, Repl starten, ESP neu starten."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun müsstest du in der Lage sein, die Blink-Animation mit dem Touch Sensor zu starten. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Bei erneuter Berührung sollte running_dot starten. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nur. Wenn man jetzt die nächste Animation starten möchte steht man vor einem Problem:"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Der Sensor reagiert nicht mehr auf Eingaben! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Das legt ganz einfach daran, dass der Code ja linear abgearbeitet wird."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Sprich während der Ausführung der Main-Funktion wird im Endlos-Loop die Animation gestartet. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Running_dot ist allerdings eine Funktion, die ebenso in einer Endlos-Schleife steckt. Somit ist es nicht mehr möglich, in der Main den Sensor auszulesen! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Natürlich könnten wir jetzt einfach sagen, wir begrenzen die Ausführung der Animationen, sodass die Schleife nur noch 20 Mal ausgeführt werden kann. Aber was ist, wenn wir die Animation früher wechseln möchten?  "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um mehrere Prozesse quasi Parallel laufen lassen zu können, behilft man sich mit dem Prinzip des Multithreading."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <para/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="180" YPOS="22123.8210253998" OwnPage="49" ItemID="1688059881" PTYPE="4" WIDTH="475.28" HEIGHT="541.818974600195" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 541.819 L0 541.819 L0 0 Z" copath="M0 0 L475.28 0 L475.28 541.819 L0 541.819 L0 0 Z" gXpos="180" gYpos="22123.8210253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1682944377"/>
        <PAGEOBJECT XPOS="735.28" YPOS="22123.8210253998" OwnPage="50" ItemID="1688044089" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800565" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="735.28" gYpos="22123.8210253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1688015529" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Multithreading"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Multithreading wird wahrscheinlich den wenigsten ein Begriff sein. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Dafür dürfte Multitasking bekannt sein!"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Jeder PC beherrscht Multitasking. So werden mehrere Programme quasi gleichzeitig ausgeführt."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Quasi gleichzeitig bedeutet, sie laufen nicht wirklich gleichzeitig. Es bedeutet lediglich, dass das Betriebssystem dafür sorgt, dass jedem Programm eine gewisse CPU-Zeit zugesprochen wird. Sprich ein Zeitfenster, indem es die CPU (also den Prozessor) beanspruchen darf. In Wirklichkeit laufen diese Prozesse gestückelt nacheinander - aber in solch engen Zeitfenstern, dass es der User nicht warhnimmt. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Multithreading verhält sich ähnlich, nur dass hier nicht zwei Programme, sondern ein Programm in verschiedene Threads, also verschiedene Aufgabenbereiche gegliedert wird, die parallel abzuarbeiten sind. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Bisher läuft unser Programm in einem Thread, dem main-Thread. Weitere Threads können wir in unserem Code hinzufügen. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Hierfür nutzt man das Micropython Modul _thread. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Im Python-Forum wird nochmal das Verhalten vom Threads erklärt:"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="https://www.python-kurs.eu/threads.php"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Allerdings können wir diese Beispiele nicht anwenden, da Micropython lediglich eine Abgespeckte Version von Python ist. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Die Modul-Beschreibung zu _thread inklusive Beispielen findet man hier: "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="https://github.com/loboris/MicroPython_ESP32_psRAM_LoBo/wiki/thread"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Um eine Funktion in einem Thread zu starten, nutzt man"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="thread = _thread.start_new_thread(th_name, th_func, args [, kwargs])"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="th_name"/>
                <ITEXT CH=" ist der Name des Threads, der als String (also in doppelten Anführungszeichen) anzugeben ist."/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="th_func"/>
                <ITEXT CH=" ist die Funktion, die gestartet werden soll. Hier soll man nur den Bezeichner angeben, ohne Klammer und Argumente"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="mit "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="args"/>
                <ITEXT CH=" werden dann, falls vorhanden, die Argumente als Tupel übergeben. Falls keine Argumente übergeben werden, muss man eine leere Klammer einfügen. Falls nur ein Argument übergeben wird, muss vor der schließenden Klammer noch ein Komma stehen. Dies symbolisiert ein Tupel mit nur einem Wert. Zum Beispiel (arg1, )"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="kwargs"/>
                <ITEXT CH=" sind optional. Hier werden die optionalen Parameter mit ihren Schlüsselworten übergeben. Diese müssen als Dictionary eingetragen werden. Bsp: {&quot;arg1&quot;: value1, &quot;arg2&quot;: value2}. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Die Funktion gibt beim Aufruf die ID des Threads zurück, die dann in einer Variablen (hier: thread) gespeichert werden sollte."/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Diese ID wird benötigt, um die Ausführung des Threads auszusetzen oder ihn wieder zu stoppen."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="mit "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="_thread.suspend(thread_id)"/>
                <ITEXT CH=" und"/>
                <ITEXT FONT="DejaVu Sans Bold" CH=" _thread.resume(thread_id)"/>
                <ITEXT CH=" wird die Ausführung des Threads unterbrochen und anschließend wieder fortgesetzt. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Dies funktioniert allerdings nur, wenn in der auszuführenden Funktion die Unterbrechung mit "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="_thread.allowsuspend(True) "/>
                <ITEXT CH="erlaubt wurde. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Möchte man den Thread stoppen, kann man "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="_thread.stop(thread_id)"/>
                <ITEXT CH=" nutzen. Dadurch wird dem Thread allerdings nur eine Benachrichtigung geschickt! Damit diese auch verarbeitet werden kann, muss in diesem die Funktion "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="notification = _thread.getnotification() "/>
                <ITEXT CH="ausgeführt werden. Sobald in Notification der selbe Wert steht, der auch _thread.EXIT hat (EXIT ist keine Variable sondern eine Konstante. Wenn man hier einen Namen anstatt eines Werts nutzt trägt das zur besseren Lesbarkeit bei)"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Alternativ kann man auch "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="_thread.wait(timeout) "/>
                <ITEXT CH="nutzen. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Dies ist sehr praktisch, da man diese Funktion anstelle von utime.sleep_ms(timeout) einsetzen kann. Damit wird erreicht, dass während des Timeouts trotzdem Benarichtigungen erhalten werden können und auch währenddessen ein Abbruch möglich ist. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Durch das Schlüsselwort break wird die derzeitig laufende Schleife (egal ob endlich oder endlos) wieder verlassen wird. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="E: Multithreading" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.28" YPOS="23005.7110253998" OwnPage="52" ItemID="1685617033" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800565" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="735.28" gYpos="23005.7110253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1688041705" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Implementierung von _thread"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Überschrift - normal-fett"/>
                <ITEXT CH="Wir haben nun schon einige Animationen geschrieben. Bevor wir nun in jeder einzelnen prüfen, ob eine Benachrichtigung erhalten wurde, packen wir das ganze lieber in eine Funktion."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Es gehört zu einem guten Programmierstil, dass kein gleicher Codeblock mehrmals vorkommt. In dieser Funktion wollen wir den timeout laufen lassen (der utime.sleep_ms ersetzen soll). Also muss der Funktion mitgeteilt werden, wie lange dieser timeout sein soll. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dann wollen wir prüfen, ob eine Benachrichtigung vorliegt. Wenn die Benachrichtigung _thread.EXIT ist, wollen wir zurückgeben, dass die Abbruchbedingung gegeben ist. Also brauchen wir ein return-Statement. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies können wir so erreichen:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def waitForExitNotification(timeout):"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="notification = _thread.wait(timeout) "/>
                <para PARENT="code"/>
                <tab/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="if notification == _thread.EXIT:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="return True"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="else:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="return False"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um diese Funktion in den Animationen aufzurufen und den Return-Wert zu verarbeiten können wir utime_sleep_ms(timeout) durch folgendes ersetzen:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="exit = waitForExitNotification(timeout) "/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="if exit:"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="return"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies fügen wir jetzt anstelle von utime.sleep_ms in jede Animation ein, die in einer Endlosschleife läuft. NIcht vergessen: Den Wert von timeout angeben! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Jetzt haben wir unsere Funktionen so angepasst, dass sie mit den Benachrichtigungen umgehen können. Nun müssen wir noch dafür sorgen, dass der Thread gestartet und gestoppt werden kann. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um nicht für jede einzelne Animation etwas eigenes anlegen zu müssen, können wir sagen, dass animations.next() einfach im Thread gestartet werden soll. der Aufruf der Animation geschieht dann innerhalb von next(). "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Also ersetzen wir in main.py animations.next() durch "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="animation_thread = _thread.start_new_thread(&quot;animation&quot;, animations.next, () )"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Jetzt haben wir den Start der Animation geregelt, aber noch nicht den Abbruch!"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wenn wir aber bereits eine Animation am laufen haben und diese zuerst abbrechen müssen, müssen wir die variable animation_thread einsetzen bevor sie deklariert wurde! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Variablendeklarationen sollten wenn möglch immer zu Beginn einer Funktion oder eines Moduls geschehen - es sei denn, es werden dazu Angaben benötigt, die erst später im Code vorkommen (wie im Fall des animation_dict)."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Also werden wir dies in main.py auch zu Beginn - am besten nach den Vaiablen für touch -  einsetzen und den Wert 0 zuweisen und in der Zeile bevor der Thread gestartet wird _thread.stop(animation_thread) einsetzen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hier nochmal die komplette main.py: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="import machine, utime, _thread"/>
                <para PARENT="code"/>
                <ITEXT CH="import animations"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="touch_pin = 27"/>
                <para PARENT="code"/>
                <ITEXT CH="touch = machine.TouchPad(machine.Pin(touch_pin))"/>
                <para PARENT="code"/>
                <ITEXT CH="touch_threshold = 150"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="animation_thread = 0"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="while True:"/>
                <para PARENT="code"/>
                <ITEXT CH=" "/>
                <tab/>
                <ITEXT CH="try:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="touch_val = touch.read()"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="except ValueError:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="print(&quot;ValueError while reading touch_pin&quot;)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="if touch_val &lt; touch_threshold:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="print(&quot;touched!!&quot;)"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="_thread.notify(animation_thread, _thread.EXIT)"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(50)"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="animation_thread = _thread.start_new_thread "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="(&quot;animation&quot;, animations.next, () )"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(200)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
