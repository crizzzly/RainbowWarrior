                <ITEXT FONT="DejaVu Sans Bold" CH="duty"/>
                <ITEXT CH=" (optional) ist der Arbeitszyklus in %. Default: 50%"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="timer"/>
                <ITEXT CH=" (optional) gibt an, ob ein Zeitmesser aktiviert werden soll (default: 0) "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="pwm.freq([freq])"/>
                <ITEXT CH="  setzt die Frequenz in Hz. Dies hat Auswirkungen auf alle Kanäle, die den selben Timer nutzen Wenn kein Argument übergeben wird gibt diese Funktion die Frequenz zurück, die auf diesem Kanal gespielt wird. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="pwm.deinit()"/>
                <ITEXT CH=" deinitialisiert den pwm-Pin, Ausgabe auf diesem Pin wird gestoppt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="pwm.init([ freq=f] [, duty=d] [, timer=tm])"/>
                <ITEXT CH="  reinitialisiert pwm."/>
                <para PARENT="Default Paragraph Style"/>
                <para/>
            </StoryText>
