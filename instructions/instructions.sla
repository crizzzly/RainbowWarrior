        <PAGEOBJECT XPOS="170.8" YPOS="15964.82" OwnPage="18" ItemID="1895477793" PTYPE="4" WIDTH="453.68" HEIGHT="454.200000000001" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 454.2 L0 454.2 L0 0 Z" copath="M0 0 L453.68 0 L453.68 454.2 L0 454.2 L0 0 Z" gXpos="170.8" gYpos="15964.82" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Framework &quot;flashen&quot;"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Überschrift - normal-fett"/>
                <ITEXT CH="Genug des Ausflugs in die Theorie. nun wird es Zeit, den ESP auszupacken und anzuschließen! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Also. Raus aus der Verpackung, ran ans USB-Kabel und im Computer einstecken! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Jedes USB-Gerät wird vom PC an einen speziellen Port geknüpft, mit dem es vom System angesprochen wird. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Unter Linux ist dies meist ttyUSB0 oder ttyUSB1. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Sobald der Controller eingesteckt ist, kannst du dies auch einfach prüfen: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="$ ls dev/ttyUSB* "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="ls bedeutet list, sprich es listet nun alle angeschlossenen Geräte, die mit ttyUSB anfangen. Der * signalisiert, dass nach dieser Zeichenfolge noch andere Zeichen folgen"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Bevor wir die Binary drauf spielen können, löschen wir vorsichtshalber alles: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="$ esptool.py --port /dev/ttyUSB0 erase_flash"/>
                <para PARENT="code"/>
                <ITEXT CH="oder:"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="$ ./BUILD.sh erase"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Und spielen dann die binary auf den Controller: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="$ ./BUILD.sh flash "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <para/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="9218.33919365677" OwnPage="10" ItemID="1896290465" PTYPE="4" WIDTH="453.68" HEIGHT="301.41080634323" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 301.411 L0 301.411 L0 0 Z" copath="M0 0 L453.68 0 L453.68 301.411 L0 301.411 L0 0 Z" gXpos="-9.19999999999999" gYpos="5328.33919365677" gWidth="475.28" gHeight="386.307949200376" LAYER="0" NEXTITEM="2008094673" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="12" CH="USB Port ermitteln"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para/>
                <ITEXT FONTSIZE="12" CH="Bevor mit der Einrichtung des Framework begonnen werden kann, ist es wichtig zu wissen, auf welchem USB Port der ESP angesprochen wird. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Jedes USB-Gerät wird vom PC an einen speziellen Port geknüpft, mit dem es vom System angesprochen wird. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Unter Linux, sehr wahrscheinlich auch unter MacOS, ist dies für den ESP32 meist &quot;ttyUSB0&quot; oder &quot;ttyUSB1&quot;. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Sobald der Controller eingesteckt ist, kannst du dies auch einfach prüfen: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="10" CH="$ ls /dev/ttyUSB* "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="ls"/>
                <ITEXT FONTSIZE="12" CH=" bedeutet "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="list"/>
                <ITEXT FONTSIZE="12" CH=", sprich es listet nun alle angeschlossenen Geräte (gespeichert im Ordner /dev), die mit &quot;ttyUSB&quot; anfangen. Der * signalisiert, dass nach dieser Zeichenfolge noch andere Zeichen folgen können."/>
                <para PARENT="Default Paragraph Style"/>
                <para/>
                <ITEXT FONTSIZE="12" CH="Falls mit dem Befehl ls keine Ausgabe erfolgen sollte, kann man auch die "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="Kernelmeldungen auslesen"/>
                <ITEXT FONTSIZE="12" CH=". Hierfür nutzt man - leider wieder nur unter Linux und MacOS - den Befehl"/>
                <para/>
                <para/>
                <ITEXT FONT="DejaVu Sans Mono Bold" FONTSIZE="12" CH="dmesg "/>
                <ITEXT FONTSIZE="12" CH="(=display messages) "/>
                <para/>
                <ITEXT FONTSIZE="12" CH="Um die Arbeit vom Kernel zu demonstrieren, kann man den Befehl einmal ausführen bevor die Hardware eingesteckt wurde,"/>
                <trail/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Terminalbefehle: USB-Port ermitteln" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
