        <PAGEOBJECT XPOS="735.28" YPOS="10302.2910807439" OwnPage="22" ItemID="1656227273" PTYPE="2" WIDTH="475.28" HEIGHT="132.128482142422" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" TEXTFLOWMODE="2" LOCALSCX="0.75" LOCALSCY="0.725980671112211" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" Pagenumber="0" PFILE="../../../../Schreibtisch/pics-projekt/groß/for-schleife-print-outp.png" IRENDER="0" EMBEDDED="0" path="M0 0 L475.28 0 L475.28 132.128 L0 132.128 L0 0 Z" copath="M0 0 L475.28 0 L475.28 132.128 L0 132.128 L0 0 Z" ImageRes="0" gXpos="0" gYpos="1406.82005534411" gWidth="475.28" gHeight="672.176923800567" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="735.28" YPOS="9777.36102539981" OwnPage="22" ItemID="1656430761" PTYPE="4" WIDTH="475.28" HEIGHT="103.428974600192" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 103.429 L0 103.429 L0 0 Z" copath="M0 0 L475.28 0 L475.28 103.429 L0 103.429 L0 0 Z" gXpos="0" gYpos="881.890000000001" gWidth="475.28" gHeight="672.176923800567" LAYER="0" NEXTITEM="-1" BACKITEM="1651862329"/>
        <PAGEOBJECT XPOS="180" YPOS="10659.2510253998" OwnPage="23" ItemID="1656709081" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800567" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="180" gYpos="10659.2510253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1654741849"/>
        <PAGEOBJECT XPOS="180" YPOS="10989.03" OwnPage="23" ItemID="1656088329" PTYPE="4" WIDTH="475.28" HEIGHT="342.397949200378" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 342.398 L0 342.398 L0 0 Z" copath="M0 0 L475.28 0 L475.28 342.398 L0 342.398 L0 0 Z" gXpos="180" gYpos="10989.03" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1659288889" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="While-Schleife "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die einfachste While-Schleife ist die Endlosschleife. Einmal gestartet soll sie endlos den beinhalteten Codeblock ausführen. Hierzu sind gerade mal zwei Worte notwendig: "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Mono Bold" CH="while True: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wollen wir also unsere LEDs endlos blinken lassen müsste die Anweisungsfolge lauten: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="mache für immer: schalte ein – pause – schalte aus – pause."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies würde dann folgendermaßen aussehen: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; import utime"/>
                <para PARENT="code"/>
                <ITEXT CH="&gt;&gt;&gt; while True:  "/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="strip.set(0, strip.TEAL, num=num_pixels)"/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(200)"/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="strip.set(0, 0x00, num=num_pixels)"/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(200)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wir können allerdings ebenso mit einer while-Schleife ausdrücken, dass der Codeblock nur 5x ausgeführt werden soll. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nehmen wir an, eine Variable i hat vor Schleifenaufruf den Wert 0. Dann kann man sagen while (i&lt;5), also während der Wert von i kleiner ist als 5, wird die Schleife ausgeführt. Dazu muss aber auch die Variable i innerhalb des nachfolgenden Codeblocks um 1 erhöht werden, ansonsten kann man sich auch so eine Endlosschleife generieren! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; while (i &lt; 5): "/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="i += 1"/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die Bedingung einer while-Schleife darf auch von Funktionen abhängen!"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nehmen wir einmal an, wir haben bereits eine Funtion geschrieben, die prüft, ob es draußen dunkel ist. Wenn es dunkel ist, gibt sie den Wert &quot;True&quot; zurück, ansonsten &quot;False&quot;.  Nennen wir sie mal &quot;is_dark()&quot;. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Schleifen funktionieren so, dass sie prüfen, ob die Bedingung nach dem Codewort &quot;while&quot; wahr ist. Wenn dem so ist, darf die Schleife ausgeführt, bzw. fortgeführt werden, ansonsten wird die Ausführung des Codes nach der Schleife fortgesetzt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; while is_dark(): "/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="wäre also genauso möglich."/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="While-Schleife" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.28" YPOS="12423.0310253998" OwnPage="28" ItemID="1647365865" PTYPE="4" WIDTH="475.28" HEIGHT="506.671474600189" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 506.671 L0 506.671 L0 0 Z" copath="M0 0 L475.28 0 L475.28 506.671 L0 506.671 L0 0 Z" gXpos="735.28" gYpos="12423.0310253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Ein erstes kleines Skript"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Sicherlich hast auch du dich beim Funktionen schreiben im Terminal bereits einige Male vertippt und musstest alles neu eingeben? "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies ist auf Dauer sehr mühsam."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Da unsere Funktionen nun nach und nach immer komplexer werden und es nun vorteilhaft ist, Codepassagen einfach überarbeiten zu können, steigen wir nun auf einen Texteditor um."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Editor"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hierzu könntest du für den Anfang den normalen Texteditor nutzen, der auf jedem System vorinstalliert ist. Allerdings ist es sehr von Vorteil, einen Editor zu nutzen, der auch fürs coden gedacht ist. Ich nutze für meine MicroPython Projekte den Editor &quot;Atom.io&quot;, der kostenlos für alle Systeme verfügbar ist. Dieser kann auf https://atom.io/ heruntergeladen werden."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Falls du noch keinen Ablageort für deine Skripte haben solltest, empfiehlt es sich, gleich einen anzulegen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Ich habe zum Beispiel im meinem Home-Verzeichnis ein Verzeichnis namens &quot;coding&quot; in dem ich meine Skripte - sortiert nach Programmiersprache - ablege. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Atom besteht im Wesentlichen aus zwei Bereichen: "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Im linken Bereich, &quot;Project&quot; genannt, hast du die Möglichkeit, Projektordner hinzuzufügen. Im rechten Teil werden dann die Textdateien angezeigt. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um einen Projektordner zu erstellen, bzw. auszuwählen, klickst du einfach mit der rechten Maustaste in das &quot;Project&quot;-Feld und gehst auf &quot;Add Project Folder&quot;. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Am besten ist es, du legst im selben Schritt innerhalb des Ordners für deine coding-Projekte einen eigenen Ordner für das Projekt an, das hier entsteht. Nennen wir es &quot;rainbowWarrior&quot;"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Ein erstes kleines Skript" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.28" YPOS="10659.2510253998" OwnPage="24" ItemID="1659288889" PTYPE="4" WIDTH="475.28" HEIGHT="326.63897460019" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 326.639 L0 326.639 L0 0 Z" copath="M0 0 L475.28 0 L475.28 326.639 L0 326.639 L0 0 Z" gXpos="735.28" gYpos="10659.2510253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1656088329"/>
        <PAGEOBJECT XPOS="735.28" YPOS="10991.14" OwnPage="24" ItemID="1659274761" PTYPE="4" WIDTH="475.28" HEIGHT="340.287949200378" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 340.288 L0 340.288 L0 0 Z" copath="M0 0 L475.28 0 L475.28 340.288 L0 340.288 L0 0 Z" gXpos="735.28" gYpos="10991.14" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1661407849" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Die Funktion &quot;is_dark()&quot; werden wir jetzt allerdings noch nicht implementieren. Jedoch wird es Zeit, einen Blick auf Funktionen zu werfen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Selbstverständlich kannst du auch ohne Funktionen ein wenig mit den Werten beim Blink-Algorithmus spielen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Allerdings wird dich sehr schnell die viele Schreibarbeit nerven. Dabei möchtest du doch nur die Farbe verändern, oder auch die Zeiten, wie lange die LEDs ein oder aus sein sollte. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Funktionen"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu diesem Zweck gibt es Funktionen. Bevor man eine Funktion implementieren möchte, muss man sich darüber im Klaren sein"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="1. Welche Anweisungen in dieser Funktion gemacht werden - "/>
                <para PARENT="Hg-weiß mit Rahmen"/>
                <tab/>
                <tab/>
                <ITEXT CH="Funktionsname daher möglichst beschreibend wählen"/>
                <para PARENT="Hg-weiß mit Rahmen"/>
                <tab/>
                <ITEXT CH="2. ob oder welche Werte dazu nötig sind, die man ggf. verändern kann"/>
                <para PARENT="Hg-weiß mit Rahmen"/>
                <tab/>
                <ITEXT CH="3. ob die Funktion irgend etwas zurückgeben muss. "/>
                <para PARENT="Hg-weiß mit Rahmen"/>
                <tab/>
                <tab/>
                <ITEXT CH="wie True/False bei is_dark()"/>
                <para PARENT="Hg-weiß mit Rahmen"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Funktionen werden mit dem Schlüsselwort &quot;def&quot; (=define) eingeleitet. Der Aufbau sieht folgendermaßen aus:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def funktions-name(Parameterliste):"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="Anweisung(en)"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="(return)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="das "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="return"/>
                <ITEXT CH=" Statement ermöglicht es einem, Ergebnisse von Berechnungen, oder eben Werte eines ausgelesenen Sensors, wieder zurück zu geben. Diesen Wert kann man also beim Funktionsaufruf in eine Variable speichern. Wollten wir jetzt, als sehr einfaches Beispiel, eine Funktion schreiben, in der zwei Werte (meist Parameter genannt) miteinander addiert werden sollen, würden wir wie folgt vorgehen: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu 1. Wir wollen eine Funtion, in der zwei Zahlen miteinander addiert werden. "/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="Nennen wir sie also einfach &quot;addieren&quot;. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu 2. zwei Werte sind dazu nötig. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu 3. das Ergebnis der Addition soll zurückgegeben werden. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def addieren(a, b): "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="e = a + b"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="return e"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="ergebnis = addieren(a, b)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Übertragen auf unsere Blink-Funktion würde es also folgendermaßen aussehen: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu 1. Wir wollen den LED-Strip blinken lassen. Daher nennen wir die Funktion "/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="strip_blink()"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu 2. Wir möchten die Farbe und die Zeiten verändern. Brauchen dazu also "/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="drei Werte. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu 3. Da wir lediglich eine Ausgabe an einen Pin machen, brauchen wir also "/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="keinen Wert zurückgeben"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="In unserer bisherigen Blink-Funktion haben wir mit festen Werten für die Farbe und die Zeiten gearbeitet. Diese müssen wir nun durch Variablen ersetzen, dessen Wert erst bei Aufruf der Funktion bestimmt, bzw übergeben wird. "/>
                <para PARENT="Default Paragraph Style"/>
                <breakline/>
                <ITEXT CH="Auch bei Variablen gilt stets darauf zu achten, treffende Namen zu wählen, um die Lesbarkeit zu gewährleisten und  - ganz wichtig - um auch nach einigen Wochen oder Monaten noch nachvollziehen zu können, was hier genau geschieht."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Für die Farbe bietet es sich daher an, die Variable &quot;color&quot; zu nennen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Für die Zeit, die die LEDs leuchten sollen, nehmen wir &quot;time_on&quot;"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Für die Zeit, die die LEDs nicht leuchten sollen, nehmen wir &quot;time_off&quot;"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; def blink(color, time_on, time_off): "/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="strip.set(0, color, num=num_pixels"/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(time_on)"/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="strip.set(0, 0x00, num=num_pixels)"/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(time_off)"/>
                <tab/>
                <para PARENT="code"/>
                <para/>
                <ITEXT CH="Nun kannst du ganz einfach mit Farbe und Blink-Intervallszeiten herumspielen. "/>
                <para/>
                <ITEXT CH="Gebe dazu einfach folgendes im Terminal ein und ersetze die Werte nach Belieben: "/>
                <para/>
                <para/>
                <ITEXT CH="&gt;&gt;&gt; blink(strip.RED, 200, 10"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Gerne kannst du anstatt den vordefinierten Werten auch selbst mit Farbwerten herumspielen. Im Netz findet sich unter dem englischen Suchbegriff &quot;hex color picker&quot; (= hexadezimaler Farbwähler) eine einfache Möglichkeit, die gewünschte Farbe als Hexadezimalwert zu erhalten. "/>
                <trail/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Funktionen" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="180" YPOS="11541.1410253998" OwnPage="25" ItemID="1661407849" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800567" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="180" gYpos="11541.1410253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1661415625" BACKITEM="1659274761"/>
        <PAGEOBJECT XPOS="735.28" YPOS="11541.1410253998" OwnPage="26" ItemID="1661415625" PTYPE="4" WIDTH="475.28" HEIGHT="260.748974600188" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 260.749 L0 260.749 L0 0 Z" copath="M0 0 L475.28 0 L475.28 260.749 L0 260.749 L0 0 Z" gXpos="735.28" gYpos="11541.1410253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1661407849"/>
        <PAGEOBJECT XPOS="180" YPOS="12983.5279492004" OwnPage="27" ItemID="1659666137" PTYPE="4" WIDTH="475.28" HEIGHT="111.68" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 111.68 L0 111.68 L0 0 Z" copath="M0 0 L475.28 0 L475.28 111.68 L0 111.68 L0 0 Z" gXpos="180" gYpos="12983.5279492004" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Aufgabe: Blinken in zwei Farben "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Schaffst du es auch, den Strip anstatt ein- und auszuschalten zwischen zwei Farben wechseln zu lassen? "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Oder abwechselnd in zwei Farben blinken zu lassen?"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Ein Beispiel hierfür findest du im Anhang. "/>
                <trail PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Aufgabe - Blinken in zwei Farben" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.28" YPOS="11771.1410253998" OwnPage="26" ItemID="1658370521" PTYPE="4" WIDTH="475.28" HEIGHT="460.248974600188" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 460.249 L0 460.249 L0 0 Z" copath="M0 0 L475.28 0 L475.28 460.249 L0 460.249 L0 0 Z" gXpos="735.28" gYpos="11771.1410253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1662984121" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Funktionen mit optionalen Parametern "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Sicherlich hat es dich bereits verwundert, dass bei der Funktion strip.set(), im Falle, dass alle LEDs gleichzeitig geschalten werden sollen, die Anzahl als Parameter &quot;num= ..&quot; übergeben wird. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hierbei handelt es sich um einen optionalen Parameter, auch default-Parameter genannt."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies ist sehr praktisch, da wir hierfür einen Standardwert (default-Wert) nutzen können, der bei Bedarf verändert werden kann. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wenn du nun schon etwas mit den Zeiten der Blink-Funktion herumgespielt hast und für dich optimale Zeit-Werte herausgefunden hast, kannst du diese ebenso als optionale Parameter angeben: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; def blink(color, time_on = 200, time_off = 100): "/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="strip.set(0, color, num=num_pixels"/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(time_on)"/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="strip.set(0, 0x00, num=num_pixels)"/>
                <para PARENT="code"/>
                <ITEXT CH="..."/>
                <tab/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(time_off)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu beachten ist hier lediglich, dass die default-Parameter grundsätzlich zuletzt genannt werden, da alle Variablen davor, hier lediglich &quot;color&quot; sogenannte Positionsparameter sind, sprich anhand ihrer Position identifiziert werden. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die ursprüngliche Fassung der Blink-Funktion bestand nur aus Positionsparametern. color, time_on, time_off wurden beim Funktionsaufruf in dieser Reihenfolge eingegeben und auch so zugewiesen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Da wir nun time_on und time_off als optionale Parameter definiert haben, dürfen diese genau aus diesem Grund nicht am Anfang stehen. Ansonsten würde die Gefahr bestehen,dass die Werte falsch zugeordet werden! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Würden wir nun alle drei Werte nennen, müssten wir die Schlüsselwörter (time_on / time_off) nicht zwingend mit angeben."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Falls wir aber nur den Wert für time_off ändern wollten, den für time_on aber auf default lassen würden, können wir time_on also einfach auslassen und die Funktion mit "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; blink(0x3F03F, time_off=50)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="aufrufen"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Funktionen mit optionalen Parametern" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="180" YPOS="12423.0310253998" OwnPage="27" ItemID="1662984121" PTYPE="4" WIDTH="475.28" HEIGHT="154.248974600188" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 154.249 L0 154.249 L0 0 Z" copath="M0 0 L475.28 0 L475.28 154.249 L0 154.249 L0 0 Z" gXpos="180" gYpos="12423.0310253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1658370521"/>
        <PAGEOBJECT XPOS="180" YPOS="13304.9210253998" OwnPage="29" ItemID="1663000617" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800565" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="180" gYpos="13304.9210253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1665880489" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Wenn du nun auf den neu erstellten Projektordner wieder mit der rechten Taste klickst, kannst du mit &quot;new File&quot; eine neue Datei anlegen. Alternativ geht das - bei markiertem Projektordner - mit der Taste A. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die neue Datei muss glech benannt werden (Dateiendung nicht vergessen!): Nennen wir sie &quot;animations.py&quot;"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Im rechten Bereich befindet sich dann der eigentliche Texteditor, hier werden deine geöffneten Dateien angezeigt. Und hier werden wir nun nach und nach, beginnend mit dem import-Statement, alles bisher gemachte übertragen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Sehr schnell erkennt man hier die Vorteile: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Durch farbige Hervorhebungen wirkt der Text auf Anhieb viel strukturierter. "/>
                <breakline/>
                <ITEXT CH="So werden zum Beispiel alle Schlüsselwörter (import, def, ...) lila gekennzeichnet. Alle Funktionen, Konstruktoren sind cyan, sowie Werte orange gekennzeichnet sind."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Auch die Zeilenzahlen am linken Rand werden noch von Vorteil sein, wenn es zu den ersten Syntaxfehlern (oder manchmal einfach Tippfehlern) kommt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nachdem nun alles in den Editor übertragen und gespeichert wurde, müssen wir aber doch wieder ins Terminal switchen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hier wechseln wir mit cd zu dem Ordner, in dem unser Skript gespeichert wurde (in meinem Fall chrissi/coding/microPy/RainbowWarrior) und stellen eine Verbindung zum ESP her. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Auch ich muss mir den Befehl hierfür immer wieder rauskramen. Copy &amp; Paste geht hier einfach schneller :) "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="$ rshell --buffer-size=30 -p /dev/ttyUSB0 -a -e nano"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="bevor wir aber REPL nutzen, kopieren wir das erstellte Skript auf den ESP: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="$ cp animations.py /flash"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Anschließend müssen wir den ESP neu starten. Hierzu einfach den Reset-Button (rst) auf dem Controller dücken."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun können wir den REPL-Mode starten. Also wieder einfach &quot;repl&quot; im Terminal eingeben und mit Enter bestätigen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um nun die Funktion blink zu starten, müssen wir das Skript zuerst importieren. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; import animations"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wie du sicherlich erkannt hast, funktioniert das auf die selbe Weise wie vorhandene Module eingefügt werden. Das bedeutet ja - du hast im Prinzip schon dein eigenes Mini-Modul geschrieben! Besteht zwar momentan nur aus einer Funktion, aber die wird nicht lange alleine bleiben! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Oooops! Wem ist's aufgefallen? "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Da hat sich der Fehlerteufel eingeschlichen! Aber so können wir gleich mal sehen, wozu die Zeilenanzeige im Editor gut ist! "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Bei dieser Fehlermeldung sind für uns erst einmal die letzten beiden Zeilen interessant. In der letzten steht die Art des Fehlers, der gefunden wurde - also ein Syntax-Fehler. Die Zeile darüber sagt mir, welche Datei betroffen ist und in welcher Zeile es zu einem Fehler gekommen ist - also Zeile 11. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH="utime.sleep_ms(time_on) "/>
                <ITEXT FONT="DejaVu Sans Book" CH="sieht allerdings richtig aus. Ich kann hier keinen Syntax-Fehler erkennen! In dem Fall sollte man sich auch die darüberliegende Zeile genauer ansehen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH="strip.set(0, color, num=num_pixels "/>
                <ITEXT FONT="DejaVu Sans Book" CH="- hier stimmt definitiv etwas nicht! Eine Klammer wurde geöffnet, aber nicht wieder geschlossen! So hat der Interpreter verstanden, dass die darauf folgende Zeile - Zeile 11 - also noch Bestandteil dessen ist, was in die Klammer gehört. Da dies aber so keinen Sinn macht, gab es nen Syntax-Fehler. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Also. Im Editor Klammer einfügen. Skript speichern. Im Terminal mit STRG+X REPL beenden. Skript erneut kopieren und REPL wieder starten. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Tipp für &quot;faule&quot;:  Du kannst diese Befehle auch auf eine Zeile komprimieren! In Linux ist die Systemsprache eigentlich C. Sprich Linux wurde komplett in C geschrieben. In fast allen Programmiersprachen (außer Python) werden die Zeilenenden mit einem &quot;;&quot; versehen, um dem Compiler oder dem Interpreter zu signalisieren, dass der Befehl hier nun zuende ist. Selbes können wir uns im Terminal zunutze machen, indem wir den Befehl zum kopieren und REPL starten auf eine Zeile bringen und mit einem &quot;;&quot; trennen:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="$ cp animations.py; repl"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="Auch wenn in Python keine Semikolons nötig sind um die Zeile (oder den Befehl) zu beenden, funktioniert selber Trick auch im REPL-Mode!"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Da wir wieder den Controller neu starten müssen können wir - anstatt den Button zu drücken - hier einfach eingeben: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; import machine; machine.reset()"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun sollte das Importieren problemlos funktionieren. "/>
                <trail PARENT="Default Paragraph Style"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="180" YPOS="13439.5925" OwnPage="29" ItemID="1661830153" PTYPE="2" WIDTH="475.28" HEIGHT="176.4375" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" TEXTFLOWMODE="2" LOCALSCX="0.75" LOCALSCY="0.75" LOCALX="0" LOCALY="-15.75" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" Pagenumber="0" PFILE="../../../../Schreibtisch/pics-projekt/atom- 01 blinkCode.png" IRENDER="0" EMBEDDED="0" path="M0 0 L475.28 0 L475.28 176.438 L0 176.438 L0 0 Z" copath="M0 0 L475.28 0 L475.28 176.438 L0 176.438 L0 0 Z" ImageRes="0" gXpos="180" gYpos="13439.5925" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="735.28" YPOS="13304.9210253998" OwnPage="30" ItemID="1665880489" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800565" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="735.28" gYpos="13304.9210253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1661853865" BACKITEM="1663000617"/>
        <PAGEOBJECT XPOS="180" YPOS="14186.8110253998" OwnPage="31" ItemID="1661853865" PTYPE="4" WIDTH="475.28" HEIGHT="221.03147460019" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 221.031 L0 221.031 L0 0 Z" copath="M0 0 L475.28 0 L475.28 221.031 L0 221.031 L0 0 Z" gXpos="-415.28" gYpos="14186.8110253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1665880489"/>
        <PAGEOBJECT XPOS="735.28" YPOS="14177.8110253998" OwnPage="32" ItemID="1661847577" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800565" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="735.28" gYpos="13295.9210253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1668639593" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Dateisystem auf dem ESP"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Schauen wir uns nun mal die vorhandenen Dateien auf dem ESP an. Dazu ist es am einfachsten, wenn wir REPL wieder verlassen und uns den Inhalt mittels ls ausgeben lassen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um REPL zu verlassen, muss nun wieder STRG+X gedrückt werden. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="ls /flash"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="gibt uns nun den aktuellen Inhalt des ESP aus. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Neben unserer &quot;animationen.py&quot; befindet sich nur eine Datei auf dem Controller: &quot;boot.py&quot;. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies ist die erste Datei die beim Start des ESP ausgeführt wird.  Hier sollte lediglich Code stehen, der finale Einstellungen vornimmt um den Boot-Prozess abzuschließen. Sehr viel passiert hier nicht - und wir sollten die Datei auch nicht verändern, jedoch können wir gerne mal einen Blick hinein werfen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="cp /flash/boot.py /home/chrissi/coding/microPy"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="kopiert die Datei vom Controller in dein lokales Filesystem, sodass du sie mit jedem beliebigen Editor öffnen kannst. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hier wird das Modul &quot;sys&quot; (Abkürzung für System) eingebunden, und die Funktion sys.path() aufgerufen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um herauszufinden, was dies bewirkt, lohnt es sich, einen Blick in die Dokumentation der Micropython-Implementierung zu werfen:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="https://docs.micropython.org/en/latest/pyboard/library/sys.html"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="sys.path: A mutable list of directories to search for imported modules."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Bedeutet, dass hier eine Liste an Ordnern in der Variablen gespeichert wird, in denen nach eingefügten Modulen gesucht wird. Sprich falls wir je weitere (externe) Module benötigen, die nicht in der Micropython-Implementierung vorhanden sind, müssen diese im Ordner &quot;flash/lib&quot; gespeichert werden, andererseits werden sie nicht gefunden."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nachdem boot.py ausgeführt wurde, wird standardgemäß nach einer File namens &quot;main.py&quot; gesucht. Falls diese vorhanden ist, wird sie ausgeführt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nur, was macht die main.py? "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies kommt stark darauf an, wofür der ESP eingesetzt werden soll! So können hier weitere Module eingebunden und ausgeführt werden, wie unsere Blink-Funktion."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="In unserem Fall können wir eigentlich alles übertragen, was wir zuletzt nach dem Neustart in REPL eingegeben haben, sodass die Funktion &quot;blink&quot; automatisch nach dem Boot-Prozess startet. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Im Editor erstellen wir also eine neue Datei namens &quot;main.py&quot; in unserem Projektordner. und fügen die zuletzt in REPL ausgeführten Zeilen ein. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="In Atom können nicht nur mehrere Dateien gleichzeitig geöffnet sein (Zugriff über die Reiter), es gibt auch einen sogenannten Split-Mode, mit dem wir zwei Dateien nebeneinander setzen können. Dazu klicken wir auf den Reiter &quot;animations.py&quot;, halten die Taste gedrückt und ziehen sie in den rechten Bereich des Fensters. So kann man beide Dateien nebeneinander betrachten, was gerade beim Schreiben von Funktionsaufrufen praktisch sein kann."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="In der Zwischenzeit hast du sicherlich bereits die Aufgaben erledigt! Für den weiteren Verlauf des Projekts ist es gut, wenn wir der Blink-Funktion mitteilen können, wie oft sie blinken kann. Dazu ist ein weiterer optionaler Parameter notwendig. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Vorsicht bei der Namensgebung für diesen Parameter! Hier würde sich &quot;range&quot; als treffender Name anbieten. Jedoch kann dies zu Konflikten führen. Ebenso darf man keine Schlüsselwörter als Variablennamen einsetzen. Daher nennen wir diese Variable lieber &quot;count&quot;"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <para/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Dateisystem auf dem ESP " Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="180" YPOS="15068.7010253998" OwnPage="33" ItemID="1668639593" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800565" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="-415.28" gYpos="14186.8110253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1661847577"/>
        <PAGEOBJECT XPOS="735.28" YPOS="15068.7010253998" OwnPage="34" ItemID="1668644793" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800565" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="735.28" gYpos="13304.9210253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1680750409" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Animationen Teil 2: running dots"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Eine Möglichkeit, mehr Bewegung in die Animation einzubringen, ist es, wenn man zum Beispiel nur eine LED zur gleichen Zeit - jedoch nach und nach um eine Position verschoben - leuchten lässt. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nur - wie erreichen wir das? "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="schalte LED an Pos 0 ein - warte - schalte LED an Pos 0 aus - schalte LED an Pos 1 ein - warte - schalte LED an Pos 1 aus. ....... schalte LED an letzter Pos ein - warte - schalte LED an letzter Pos aus. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Kommt dir bekannt vor? "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hierfür brauchen wir eine for-schleife, wobei die obere Grenze der range-Funkton gleich der Anzahl der LED (also num_led) ist."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wir brauchen nun keine weitere Datei mehr anzulegen. Diese Funktion können wir einfach in animations.py einfügen! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def running_dot(color = 0x00FFFF, time_on=50):"/>
                <para PARENT="code"/>
                <ITEXT CH="    while True:"/>
                <para PARENT="code"/>
                <ITEXT CH="        for i in range(num_pixels):"/>
                <para PARENT="code"/>
                <ITEXT CH="            strip.set(i +1, color)"/>
                <para PARENT="code"/>
                <ITEXT CH="            utime.sleep_ms(time_on)"/>
                <para PARENT="code"/>
                <ITEXT CH="            strip.set(i +1, 0x00)"/>
                <para PARENT="code"/>
                <ITEXT CH="            utime.sleep_ms(1)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dieses mal wollen wir wieder die Möglichkeit haben, die Farbe und die Zeit, die die LED leuchten soll, zu bestimmen, daher werden colr und time_on als optionale Parameter angegeben. color = 0x00FFFF ergibt die Farbe cyan, mit time_on = 50 ms läuft das ganze relativ zügig durch."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hier ist am Ende der Schleife ein kurzes Delay von min 1ms notwendig, da es sonst zu fehlerhaften Zuweisungen auf dem Strip kommt. Würden wir dieses Delay auslassen, würden die LEDs in allen möglichen Farben kreuz und quer leuchten!"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hier muss ich auf eine &quot;Kleinigkeit&quot; in dieser MicroPython Implementierung hinweisen, die nicht den gängigen Programmierkonventionen entspricht! "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Normalerweise fängt der Informatiker bei 0 an zu zählen. Von 0 bis 9 hat man genau 10 Ziffern. Und zwar genau die 10 Ziffern, die notwendig sind, um alle Zahlen im 10er Zahlensystem darzustellen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="DIes bedeutet, dass - normalerweise - alles, was sich an erster Stelle befindet, mit dem Index 0 angesprochen wird. Ebenso wird alles, was sich an Stelle n befindet, mit dem Index n-1 angesprochen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Auf den Strip übertragen würde dies bedeuten, die LED an 1. Stelle würde mit 0 angesprochen werden, die LED an der letzten Stelle mit (num_pixels -1)"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies ist hier aber leider anders umgesetzt! Die erste LED kann man sowohl mit 0 als auch mit 1 ansprechen, mit num_pixels-1 würde aber die vorletzte leuchten!"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Daher müssen wir in strip.set() als Index i+1 übergeben! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Gestartet wird die Animation wieder in &quot;main.py&quot;. Da alle Parameter optional sind, muss man auch beim Funktionsaufruf nicht zwingend Parameter übergeben! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="animations.running_dots() "/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="führt die Animation mit default-Werten aus. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Animationen Teil 2: running dots" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
