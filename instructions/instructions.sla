            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Aufgabe: Schleifen erstellen "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Erstelle nun anhand der vorherigen Beispiele eine Blink-Funktion, die "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="1. 10 x blinkt (einmal mit for- und einmal mit while-Schleife) "/>
                <para PARENT="numerierte Liste"/>
                <ITEXT CH="2. für immer blinken wird. "/>
                <para PARENT="numerierte Liste"/>
                <ITEXT CH="3. (Special) erweitere die Funktion (Parameterliste und Funktionsrumpf) so, dass beim Funktionsaufruf definiert werden kann, wie oft die Schleife durchlaufen werden soll. "/>
                <para PARENT="numerierte Liste"/>
