        <PAGEOBJECT XPOS="170.8" YPOS="40657.74" OwnPage="46" ItemID="1937359745" PTYPE="4" WIDTH="453.68" HEIGHT="256.260000000017" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 256.26 L0 256.26 L0 0 Z" copath="M0 0 L453.68 0 L453.68 256.26 L0 256.26 L0 0 Z" gXpos="170.8" gYpos="40657.74" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1924490113"/>
        <PAGEOBJECT XPOS="170.8" YPOS="11555.37" OwnPage="13" ItemID="1937364817" PTYPE="4" WIDTH="453.68" HEIGHT="714.390000000001" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="11555.37" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="2009798321" BACKITEM="1891820401"/>
        <PAGEOBJECT XPOS="170.8" YPOS="13319.15" OwnPage="15" ItemID="1937324129" PTYPE="4" WIDTH="453.68" HEIGHT="714.390000000001" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="13319.15" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Compiler"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Überschrift - normal-fett"/>
                <ITEXT CH="Compiler sind spezielle Übersetzer, die Programmcode aus problemorientierten Programmiersprachen (auch Hochsprachen genannt) in ausführbaren Maschinencode (also die Sprache, die die Prozessoren verstehen) übersetzen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Mit einem Blick in die Ordnerstruktur des Micropython Frameworks findet man zum Beispiel im Ordner 'MicroPython_ESP32_psRAM_LoBoNeu/MicroPython_BUILD/components/micropython/esp32' einige Dateien mit der Endung "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="*.c"/>
                <ITEXT CH=" oder "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="*.h"/>
                <ITEXT CH=". Dies sind Module, geschrieben in der Programmiersprache C - und die zugehörigen Header-Files."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Diese sind für uns Menschen noch einigermaßen lesbar. Der Prozessor, der dies allerdings ausführen muss, kann noch gar nichts damit anfangen. Dieser &quot;versteht&quot; nämlich nur den sogenannten Maschinencode, bestehend aus einer Folge von Bytes, die sowohl Befehle als auch Daten repräsentieren. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Da dieser Code für den Menschen kaum lesbar ist, werden in der Assemblersprache zum Beispiel (eine Sprache, die der Maschiensprache noch am nächsten kommt) die Befehle durch besser verständliche Abkürzungen, sogenannte Mnemonics, dargestellt."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Der Compiler scannt also während des kompiliervorgangs den Code und übersetzt ihn - so ressourcenschonend es geht - in Maschinensprache. Die dadurch entstandene binary lässt sich dann vom Computer ausführen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nachfolgend ein kleines Beispiel, wie es für den Prozessor aussehen kann, wenn man der Variablen &quot;a&quot; einen Wert zuweist oder wenn man zwei Werte, die in Variablen gespeichert sind, miteinander addiert. Wichtig zu wissen ist hierbei, dass die Variablen in sogenannten Registern gespeichert werden . Dies sind Speicherbereiche innerhalb des Prozessors, die direkt mit dem Rechenkern verbunden sind."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Für neugierige hier einige Links mit detaillierteren Informationen:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="https://de.wikipedia.org/wiki/Compiler"/>
                <para PARENT="links"/>
                <ITEXT CH="https://de.wikipedia.org/wiki/Maschinensprache"/>
                <para PARENT="links"/>
                <ITEXT CH="https://de.wikipedia.org/wiki/Register_(Computer)"/>
                <trail PARENT="links"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Compiler" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="14549.788168257" OwnPage="16" ItemID="1938893217" PTYPE="4" WIDTH="453.68" HEIGHT="365.641831742998" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 365.642 L0 365.642 L0 0 Z" copath="M0 0 L453.68 0 L453.68 365.642 L0 365.642 L0 0 Z" gXpos="170.8" gYpos="14549.788168257" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Interpreter"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Im Gegesatz zu einem Compiler, der vor Ausführung des Programms den vorhandenen Code in Maschinensprache übersetzt, macht dies ein sogenannter Interpreter erst währed der Laufzeit."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Sprich es wird eine Zeile nach der anderen zuerst eingelesen und dann in Maschinensprache übersetzt. Nach diesem Schritt erst können die Befehle dem Prozessor zur Ausführung weitergegeben werden."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Ein Vorteil hiervon ist, dass diese Programme auf verschiedenen Systemen mit verschiedenen  Prozessorarchitekturen ausführbar sind. Leider muss man aber - bedingt durch die Übersetzung während der Laufzeit, mit Performanceeinbußen rechnen. Das bedeutet, dass das Programm allgemein langsamer läuft als selbes Programm, das in einer kompilierten Sprache geschrieben wird."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies wird meist bei der Ausführung von rechenintensiven Anwendungen deutlich spürbar. "/>
                <trail/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Interpreter" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="46830.97" OwnPage="53" ItemID="1939532401" PTYPE="4" WIDTH="453.68" HEIGHT="441.530000000021" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 441.53 L0 441.53 L0 0 Z" copath="M0 0 L453.68 0 L453.68 441.53 L0 441.53 L0 0 Z" gXpos="170.8" gYpos="46830.97" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1932697905"/>
        <PAGEOBJECT XPOS="170.8" YPOS="47286.6310253998" OwnPage="53" ItemID="1939508145" PTYPE="4" WIDTH="453.68" HEIGHT="258.728974600177" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 258.729 L0 258.729 L0 0 Z" copath="M0 0 L453.68 0 L453.68 258.729 L0 258.729 L0 0 Z" gXpos="170.8" gYpos="47286.6310253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1937611601" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="In unseren Code eingefügt sieht das nun so aus: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="touch_threshold = 180 "/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="while True:"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="# read touch pin"/>
                <para PARENT="Kommentare - Zeilen"/>
                <tab/>
                <ITEXT CH="try: "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="touch_val = touch.read()"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="expect ValueError:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="# if ValueError occures print on terminal and blink 3 times in red"/>
                <para PARENT="Kommentare - Zeilen"/>
                <tab/>
                <tab/>
                <ITEXT CH="print(&quot;TouchPad Error&quot;)"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="animations.blink(color=0xff0000, count=3, time_on=50, time_off=50)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="# check if touch was registered"/>
                <para PARENT="Kommentare - Zeilen"/>
                <tab/>
                <ITEXT CH="if touch_val &lt; touch_threshold: "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="# print output and start function  handleAnimations() without"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="            # argument to start next animation"/>
                <para PARENT="Kommentare - Zeilen"/>
                <tab/>
                <tab/>
                <ITEXT CH="animations.next()"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(200)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Jetzt haben wir aber mit animations.next() eine Funktion namens next erfunden, die es in animations.py aber noch gar nicht gibt! "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Diese müssen wir noch in die File animations.py einbauen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wie der Name schon sagt, soll diese Funktion einfach die nächste Animation aufrufen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Am einfachsten erreicht man dies, wenn man alle Funktionen in eine Liste oder in einen Dictionary - schreibt, bei der man ganz einfach sagen kann: starte die Nächste! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Für den jetzigen Verwendungszweck würde eine Liste ausreichen. Aber da wir das ganze später noch ausweiten wollen, werden wir hier einen Dictionary verwenden. Spätestens wenn wir die Webseite erstellen, um bestimmte Animationen auszuwählen, werden wir nicht um einen Dictionary kommen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zur Erinnerung:"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Ein Dictionary zechnet sich dadurch aus, dass zu jedem Wert ein Schlüsselwort gehört, über den man diesen Wert auch abrufen kann."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="In der File animations.py erstellen wir also zuerst einen Dictionary, mit dem wir arbeiten können. In diesem werden alle vorhandenen Animationen, zusammen mit einem Schlüsselwort - also den Namen der Animation als String -  gespeichert. Wichtig ist, dass diese Variable erst nach den Funktionen, die eingesetzt werden sollen, deklariert wird. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Beim Import der File animations.py werden - von oben nach unten - alle Variablen und Funktionen angelegt. Würden wir den Dictionary zu Beginn der Datei anlegen, würde der Interpreter die Funktionen noch gar nicht kennen und somit einen Fehler melden. Daher wird die Variable und die zugehörige next()-Funktion am Ende positioniert. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="# saves all animations in a dictionary"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="animation_dict = [&quot;blink&quot; : blink, &quot;running_dot&quot; : running_dot, &quot;crossing_dot&quot; : crossing_dots, &quot;rainbowCycle&quot; : rainbow_cycle ]"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wie man sehen kann, werden hier nur die Bezeichner der Funktionen gespeichert. Würden wir &quot;blink()&quot; anstatt &quot;blink&quot; schreiben, würde das zur sofortigen Ausführung der Funktion führen, was wir aber nicht wollen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Anschließend überlegen wir uns, was genau in der Funktion next() geschehen soll. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Innerhalb dieser Funktion möchten wir, dass, wenn bisher keine Funktion aufgerufen wurde, die erste in der Liste gestartet wird. Falls aber im Vorfeld bereits eine gestartet wurde, soll die jeweils nächste gestartet werden."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Eine einfache, aber komplizierte Lösung wäre, dass wir zum Beispiel sagen, dass blink() die Animation Nr 0 ist, running_dot() Animation Nr 1 (...), während ein Zähler die Touch-Eingaben hochzählt."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Einfacher geht es mit einem Iterator"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="-379" YPOS="49618.3010253998" OwnPage="-1" ItemID="1903488897" PTYPE="4" WIDTH="475.28" HEIGHT="682.708974600198" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 682.709 L0 682.709 L0 0 Z" copath="M0 0 L475.28 0 L475.28 682.709 L0 682.709 L0 0 Z" gXpos="-379" gYpos="49618.3010253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Datentypen in Python"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="List"/>
                <ITEXT CH=" kann man als eine Art Stapelspeicher bezeichnen, auf den man weitere Elemente ablegen oder wieder herausnehmen kann. Diese wird gekennzeichnet durch eckige Klammern []. Man kann mittels dem Index auf die einzelnen Elemente der Liste zugreifen und diese ggf. auch verändern."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Tuple"/>
                <ITEXT CH=", oder auch Tupel sind einer Liste sehr ähnlich. Man kann ebenso mittels dem Index auf die einzelnen Elemente zugreifen. Jedoch werden Tupel in runde Klammern () gepackt und sind im Gegensatz zu Listen nicht veränderbar. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Dictionaries"/>
                <ITEXT CH=" enthalten zusätzlich zu den Werten auch sogenannte Bezeichner, über den man das Objekt im Dictionary aufrufen kann:"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="capital_dict = {&quot;Baden-Wuerttemberg&quot;:&quot;Stuttgart&quot;,  &quot;Bayern&quot;:&quot;Muenchen&quot;, ... }"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Während in anderen Sprachen der Typ der Variable explizit definiert werden muss und auch nicht verändert werden kann, geschieht das in Python mit der Zuweisung. Zum Beispiel ist die Variable a = 10 automatisch eine Integer-Zahl. Wenn man später aber a mit a = 2.3 überschreibt, ist a vom typ float. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Man kann nicht nur einzelne Zahlenwerte oder Zeichen(ketten) in Variablen speichern. Man kann ebenso eine Liste,ein Tupel oder einen Dictionary in eine Variable speichern. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Nicht nur das. Man kann ebenso eine Liste aus Listen anlegen. Oder aber ganze Funktonen in Variablen speichern! "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Diese Eigenschaft machen wir uns nun zunutze. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Mehr Informationen zu den bisher gezeigten Datentypen kann man auf folgenden Links abrufen: "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="https://www.python-kurs.eu/python3_variablen.php"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="https://www.python-kurs.eu/python3_listen.php"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="https://www.python-kurs.eu/python3_dictionaries.php"/>
                <para PARENT="erklärungen"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="47712.86" OwnPage="54" ItemID="1937611601" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="47712.86" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="2052262737" BACKITEM="1939508145"/>
        <PAGEOBJECT XPOS="170.8" YPOS="49476.64" OwnPage="56" ItemID="1937631329" PTYPE="4" WIDTH="453.68" HEIGHT="259.199999999997" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 259.2 L0 259.2 L0 0 Z" copath="M0 0 L453.68 0 L453.68 259.2 L0 259.2 L0 0 Z" gXpos="170.8" gYpos="49476.64" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Lokale und globale Variablen in Python"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Jede Variable, die man in Python in einer Funktion definiert, ist automatisch eine lokale Variable. Sprich sie existiert nur innerhalb dieser Funktion."/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Man kann zwar innerhalb der Funktion auf eine Variable, die außerhalb der Funktion definiert wurde, zugreifen, diese auch verändern. Jedoch hat diese Veränderung keinen Einfluss auf die Variable außerhalb."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Sprich wenn eine Variable x außerhalb der Funktion den Wert 10 hat, wir diesen innerhalb der Funktion durch 2 teilen, hat die Variable außerhalb der Funktion immernoch den Wert 10, während die Variable x in der Funktion den Wert 5 hat."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Weitere Infos sowie einige Beispiele sind unter anderem im Python-Forum zu finden:"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Oblique" CH="https://www.python-kurs.eu/python3_global_lokal.php"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="E: Lokale und Globale Variablen in Pythin" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="48664.5210253998" OwnPage="55" ItemID="1941548897" PTYPE="4" WIDTH="453.68" HEIGHT="391.478974600199" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 391.479 L0 391.479 L0 0 Z" copath="M0 0 L453.68 0 L453.68 391.479 L0 391.479 L0 0 Z" gXpos="170.8" gYpos="48664.5210253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Iterator"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Ein Iterator ist einfach ein Zeiger, der auf das Element einer Liste (oder ähnlichem) zeigt, das gerade an der Reihe ist. Er funktioniert ähnlich wie eine for-Schleife."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="for i in range(3):"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="print(i)"/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="könnte man auch folgendermaßen schreiben: "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="numbers = [0,1,2]"/>
                <para PARENT="code"/>
                <ITEXT CH="num_iterator = iter(numbers)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="while True: "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="try:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="num = num_iterator.__next__()"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="except StopIteration:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="break"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="print(num)"/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Im ersten Moment scheint dies zwar komplizierter, aber oftmals - wie in unserem Fall - definitiv die bessere Wahl."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Zu beachten ist hier auch der try / except - Bereich. Würden wir diesen auslassen, würde es zu einer Fehlermeldung (StopIteration) und folglich zu einer Unterbrechung des Programms kommen, sobald wir das Ende des Dictionarys erreicht, bzw. überschritten haben."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="E: Iterator" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="49062.5210253998" OwnPage="55" ItemID="1943368881" PTYPE="4" WIDTH="453.68" HEIGHT="246.618974600176" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 246.619 L0 246.619 L0 0 Z" copath="M0 0 L453.68 0 L453.68 246.619 L0 246.619 L0 0 Z" gXpos="170.8" gYpos="49062.5210253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Wir möchten aber nicht, dass die Iteration abbricht, nachdem wir die letzte Animation erreicht haben. Wir wollen, dass, wenn das letzte Element erreicht wurde, die Iteration wieder von vorn beginnt."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Also behelfen wir uns eines kleinen Tricks:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wir legen einfach eine neue Iteration an! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies führt uns aber zu einem weiteren Thema, das hier behandelt werden muss: Lokale und globale Variablen."/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="49743.37" OwnPage="56" ItemID="1942487617" PTYPE="4" WIDTH="453.68" HEIGHT="447.659999999974" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 447.66 L0 447.66 L0 0 Z" copath="M0 0 L453.68 0 L453.68 447.66 L0 447.66 L0 0 Z" gXpos="170.8" gYpos="49743.37" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1945788401" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Die Variable animation_iterator wird außerhalb der Funktion next() deklariert (=angelegt). Würden wir sie erst beim Aufruf der Funktion deklarieren, würde das bedeuten, dass, bei jedem Aufruf die Iteration bei 0 beginnen würde. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Da wir sie aber - um nach Ende der Iteration wieder bei 0 zu beginnen - im except-Block neu anlegen und somit verändern müssen, würde dies zu einer Fehlermeldung führen (Variable referenziert bevor sie deklariert wurde), da wir die Variable zuerst verwenden und anschließend verändern. Gerne darf dieser Effekt ausprobiert werden! "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Daher müssen wir in der Funktion angeben, dass es sich hier um eine globale Variable handelt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Anders verhält es sich bei animation_dict. Diese wird in der Funktion lediglich aufgerufen, nicht aber verändert. Daher muss diese nicht global sein."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="# saves all animations in a dictionary"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="animation_dict = [&quot;blink&quot; : blink, &quot;running_dot&quot; : running_dot, &quot;crossing_dots&quot; : crossing_dots, &quot;rainbowCycle&quot; : rainbow_cycle ]"/>
                <para PARENT="code"/>
                <ITEXT CH="animation_iterator = iter(animation_dict)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="def next(): "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="global animation_iterator "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="try: "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="running_function = animation_iterator.__next__()"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="running_function()"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="except StopIteration: "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="animation_iterator = iter(animation_dict)"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="next()"/>
                <tab/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="running_function = animation_iterator.__next__()"/>
                <ITEXT CH=" bewirkt, dass die nächste Funktion in die Variable running_function gespeichert wird. Mit "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="running_function()"/>
                <ITEXT CH=" wird diese dann ausgeführt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Damit nun aber bei Erreichen des except-Blocks aus Sicht des Nutzers nicht einfach 'nichts' passiert, können wir - aus der Funktion heraus - selbe noch einmal aufrufen."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wenn eine Funktion sich selbst aufruft nennt man diese eine rekursive Funktion. Da diese im Projekt nur eine untergeordnete Rolle spielt, verweise ich auf Quellen im Internet, um weitere Informationen zu erhalten: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="https://www.python-kurs.eu/rekursive_funktionen.php"/>
                <para PARENT="links"/>
                <ITEXT CH="http://python4kids.net/how2think/kap04.htm"/>
                <para PARENT="links"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Fertig für den Test!"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Also, Datei speichern, zum Terminal wechseln. Datei auf den ESP kopieren, Repl starten, ESP neu starten."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun sollte es möglich sein, die erste Animation mit dem Touch Sensor zu starten. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Bei erneuter Berührung sollte die laufende Animation stoppen und eine neue starten. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Allerdings wird dies so nicht funktionieren!"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Der Sensor reagiert nicht mehr auf Eingaben! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dader Code linear abgearbeitet wird, wird die Ausführung von main.py unterbrochen sobald die Animation gestartet wurde. Die Animation läuft ebenso in einer Endlosschleife. Das bedeutet, es ist momentan nicht möglich, den Sensor auszulesen und diese Animation von außen abzubrechen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Natürlich könnten wir jetzt einfach sagen, wir begrenzen die Ausführung der Animationen, sodass die Schleife nur noch 20 Mal ausgeführt werden kann. Aber was ist, wenn wir die Animation früher wechseln möchten?  "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um mehrere Prozesse quasi Parallel laufen lassen zu können, behilft man sich mit dem Prinzip des Multithreading."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <para/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="50358.53" OwnPage="57" ItemID="1945788401" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="50358.53" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1942487617"/>
        <PAGEOBJECT XPOS="170.8" YPOS="51240.42" OwnPage="58" ItemID="1943974993" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="51240.42" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1945856513" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Multithreading"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Multithreading wird wahrscheinlich den wenigsten ein Begriff sein. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Dafür dürfte Multitasking bekannt sein!"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Jeder PC beherrscht Multitasking. So werden mehrere Programme quasi gleichzeitig ausgeführt."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Quasi gleichzeitig bedeutet, sie laufen nicht wirklich gleichzeitig. Es bedeutet lediglich, dass das Betriebssystem dafür sorgt, dass jedem Programm eine gewisse CPU-Zeit zugesprochen wird. Sprich ein Zeitfenster, indem es die CPU (also den Prozessor) beanspruchen darf. In Wirklichkeit laufen diese Prozesse gestückelt nacheinander - aber in solch engen Zeitfenstern, dass es der User nicht warhnimmt. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Multithreading verhält sich ähnlich, nur dass hier nicht zwei Programme gemeint sind, die parallel laufen, sondern ein Programm in verschiedene Threads, also verschiedene Aufgabenbereiche gegliedert wird, die parallel abzuarbeiten sind. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Bisher läuft unser Programm in einem Thread, dem main-Thread. Weitere Threads können wir in unserem Code hinzufügen. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Hierfür nutzt man das Micropython Modul _thread. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Im Python-Forum wird nochmal das Verhalten vom Threads erklärt:"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="https://www.python-kurs.eu/threads.php"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Allerdings können wir diese Beispiele nicht anwenden, da Micropython lediglich eine Abgespeckte Version von Python ist. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Die Modul-Beschreibung zu _thread inklusive Beispielen findet man hier: "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="https://github.com/loboris/MicroPython_ESP32_psRAM_LoBo/wiki/thread"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Um eine Funktion in einem Thread zu starten, nutzt man"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="thread = _thread.start_new_thread(th_name, th_func, args [, kwargs])"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="th_name"/>
                <ITEXT CH=" ist der Name des Threads, der als String (also in doppelten Anführungszeichen) anzugeben ist."/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="th_func"/>
                <ITEXT CH=" ist die Funktion, die gestartet werden soll. Hier soll man nur den Bezeichner angeben, ohne Klammer und Argumente"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="mit "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="args"/>
                <ITEXT CH=" werden dann, falls vorhanden, die Argumente als Tupel übergeben. Falls keine Argumente übergeben werden, muss man eine leere Klammer einfügen. Falls nur ein Argument übergeben wird, muss vor der schließenden Klammer noch ein Komma stehen. Dies symbolisiert ein Tupel mit nur einem Wert. Zum Beispiel (arg1, )"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="kwargs"/>
                <ITEXT CH=" sind optional. Hier werden die optionalen Parameter mit ihren Schlüsselworten übergeben. Diese müssen als Dictionary eingetragen werden. Bsp: {&quot;arg1&quot;: value1, &quot;arg2&quot;: value2}. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Die Funktion gibt beim Aufruf die ID des Threads zurück, die dann in einer Variablen (hier: thread) gespeichert werden sollte."/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Diese ID wird benötigt, um die Ausführung des Threads auszusetzen oder ihn wieder zu stoppen."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="mit "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="_thread.suspend(thread_id)"/>
                <ITEXT CH=" und"/>
                <ITEXT FONT="DejaVu Sans Bold" CH=" _thread.resume(thread_id)"/>
                <ITEXT CH=" wird die Ausführung des Threads unterbrochen und anschließend wieder fortgesetzt. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Dies funktioniert allerdings nur, wenn in der auszuführenden Funktion die Unterbrechung mit "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="_thread.allowsuspend(True) "/>
                <ITEXT CH="erlaubt wurde. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Möchte man den Thread stoppen, kann man "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="_thread.stop(thread_id)"/>
                <ITEXT CH=" nutzen. Dadurch wird dem Thread allerdings nur eine Benachrichtigung geschickt! Damit diese auch verarbeitet werden kann, muss in dem Thread, der abgebrochen werden soll, die Funktion "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="notification = _thread.getnotification() "/>
                <ITEXT CH="ausgeführt werden. Sobald in notification der selbe Wert steht, der auch _thread.EXIT hat (EXIT ist keine normale Variable sondern eine Konstante. Wenn man hier einen Namen anstatt eines Werts nutzt trägt das zur besseren Lesbarkeit bei)"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Alternativ kann man auch "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="_thread.wait(timeout) "/>
                <ITEXT CH="nutzen. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Dies ist sehr praktisch, da man diese Funktion anstelle von utime.sleep_ms(timeout) einsetzen kann. Damit wird erreicht, dass während des Timeouts trotzdem Benarichtigungen erhalten werden können und auch währenddessen ein Abbruch möglich ist. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Durch das Schlüsselwort return wird die derzeitig laufende Schleife (egal ob endlich oder endlos) wieder verlassen wird. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="E: Multithreading" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="52550.9710253998" OwnPage="59" ItemID="1948280785" PTYPE="4" WIDTH="453.68" HEIGHT="285.728974600177" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 285.729 L0 285.729 L0 0 Z" copath="M0 0 L453.68 0 L453.68 285.729 L0 285.729 L0 0 Z" gXpos="170.8" gYpos="52550.9710253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1945841793" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Implementierung von _thread"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Überschrift - normal-fett"/>
                <ITEXT CH="Wir haben nun schon einige Animationen geschrieben. Bevor wir nun in jeder einzelnen prüfen, ob eine Benachrichtigung erhalten wurde, packen wir das ganze lieber in eine Funktion."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Es gehört zu einem guten Programmierstil, dass kein gleicher Codeblock mehrmals vorkommt. In dieser Funktion wollen wir den timeout laufen lassen (der utime.sleep_ms ersetzen soll). Also muss der Funktion mitgeteilt werden, wie lange dieser timeout sein soll. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dann wollen wir prüfen, ob eine Benachrichtigung vorliegt. Wenn die Benachrichtigung _thread.EXIT ist, wollen wir zurückgeben, dass die Abbruchbedingung gegeben ist. Also brauchen wir ein return-Statement. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies können wir so erreichen:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def waitForExitNotification(timeout):"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="notification = _thread.wait(timeout) "/>
                <para PARENT="code"/>
                <tab/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="if notification == _thread.EXIT:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="return True"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="else:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="return False"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um diese Funktion in den Animationen aufzurufen und den Return-Wert zu verarbeiten können wir utime_sleep_ms(timeout) durch folgendes ersetzen:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="exit = waitForExitNotification(timeout) "/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="if exit:"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="return"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies fügen wir jetzt anstelle von utime.sleep_ms in jede Animation ein, die in einer Endlosschleife läuft. NIcht vergessen: Den Wert von timeout angeben! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Jetzt haben wir unsere Funktionen so angepasst, dass sie mit den Benachrichtigungen umgehen können. Nun müssen wir noch dafür sorgen, dass der Thread gestartet und gestoppt werden kann. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um nicht für jede einzelne Animation _thread.start anlegen zu müssen, können wir sagen, dass animations.next() einfach im Thread gestartet werden soll. der Aufruf der Animation geschieht dann innerhalb von next(). "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Also ersetzen wir in main.py animations.next() durch "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="animation_thread = _thread.start_new_thread(&quot;animation&quot;, animations.next, () )"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Jetzt haben wir den Start der Animation geregelt, aber noch nicht den Abbruch!"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wenn wir aber bereits eine Animation am laufen haben und wir diese zuerst abbrechen müssen, müssen wir eine Eigenschaft der Variablen animation_thread prüfen bevor sie deklariert wurde! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Variablendeklarationen sollten wenn möglch immer zu Beginn einer Funktion oder eines Moduls geschehen - es sei denn, es werden dazu Angaben benötigt, die erst später im Code vorkommen (wie im Fall des animation_dict)."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Also werden wir dies in main.py auch zu Beginn - am besten nach den Vaiablen für touch -  einsetzen und den Wert 100 zuweisen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="_thread.stop(animation_thread) wird eine Zeile über der Zeile, in der der Thread gestartet wird, eingesetzt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hier nochmal die komplette main.py: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="import machine, utime, _thread"/>
                <para PARENT="code"/>
                <ITEXT CH="import animations"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="touch_pin = 27"/>
                <para PARENT="code"/>
                <ITEXT CH="touch = machine.TouchPad(machine.Pin(touch_pin))"/>
                <para PARENT="code"/>
                <ITEXT CH="touch_threshold = 180"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="animation_thread = 100"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="while True:"/>
                <para PARENT="code"/>
                <ITEXT CH=" "/>
                <tab/>
                <ITEXT CH="try:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="touch_val = touch.read()"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="except ValueError:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="print(&quot;ValueError while reading touch_pin&quot;)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="if touch_val &lt; touch_threshold:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="print(&quot;touched!!&quot;)"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="_thread.notify(animation_thread, _thread.EXIT)"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(50)"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="animation_thread = _thread.start_new_thread "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="(&quot;animation&quot;, animations.next, () )"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(200)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Implementierung von _thread" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="52122.31" OwnPage="59" ItemID="1945856513" PTYPE="4" WIDTH="453.68" HEIGHT="409.199999999997" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 409.2 L0 409.2 L0 0 Z" copath="M0 0 L453.68 0 L453.68 409.2 L0 409.2 L0 0 Z" gXpos="170.8" gYpos="52122.31" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1943974993"/>
        <PAGEOBJECT XPOS="170.8" YPOS="53004.2" OwnPage="60" ItemID="1945841793" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="53004.2" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="2053099185" BACKITEM="1948280785"/>
        <PAGEOBJECT XPOS="170.8" YPOS="54767.98" OwnPage="62" ItemID="1950413121" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="54767.98" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1953108977" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Analog Digital Converter - das ADC Modul"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Allerdings besitzt der ESP auch einen Analog-Digital-Wandler, auch ADC (=Analog Digital Converter) genannt. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Den ADC findet man - wie alles, was mit Pins zu tun hat - im machine Modul:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="https://github.com/loboris/MicroPython_ESP32_psRAM_LoBo/wiki/adc"/>
                <para PARENT="links"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die Modulbeschreibung sieht relativ komplex aus für Menschen, die mit Elektrotechnik nicht viel zu tun haben. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Aber wir setzen hier nur die nötigsten Bausteine ein, um die Umgebungshelligkeit zu messen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu beachten ist allerdings, dass es zwei Arten von ADC gibt. ADC1 und ADC2. Jeweils auf verschiedenen Pins. ADC2 kann man allerdings nur nutzen, wenn WLAN nicht aktiv ist. Da wir die WLAN Funktion später noch brauchen werden, nutzen wir ADC1."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Diesen kann man auf den Pins 32 - 38 nutzen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Eine Instanz des ADC bilden wir mit "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="ldr = machine.LDR(pin[, unit])"/>
                <ITEXT CH=", wobei unit standardgemäß 1 (für ADC1) ist. Daher muss dies nicht angegeben werden."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="ldr_val = lrd.read()"/>
                <ITEXT CH=" liest die Spannung (in mV), die am Sensor anliegt und speichert sie in ldr_val. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Aus der Modulbeschreibung (machine.ADC.vref) geht hervor, dass die Spannung einen Wert zwischen 0 und 1100 mV annehmen kann, wobei 0 als sehr dunkel und 1100 als sehr hell zu interpretieren ist."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Diesen Wert können wir also nicht einfach 1:1 nutzen, um die Helligkeit der LED zu setzen, da diese nur Werte zwischen 0 und 255 akzeptiert. Für den Anfang reicht es daher, den Wert des Sensors auf den Helligkeitswert zu &quot;mappen&quot;. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies ist ganz einfache Mathematik:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="brightness_val = ldr_val / 1100 * 255. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Würde der LDR nun einen Wert von 700 haben, würde als Ergebnis 162,272727273 herauskommen. Das Problem dabei: Neopixel.brightness kann nur mit ganzen Zahlen, sogenannten Integer umgehen! "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies ist aber leicht behoben. "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="int(brightness_val) "/>
                <ITEXT CH="sorgt dafür, dass aus der Gleitkommazahl 162,272727273 die Ganzzahl 162 wird. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hierbei wird allerdings nicht gerundet! der Wert wird einfach nach dem Komma abgetrennt, sodass wir eine ganze Zahl erhalten. Da wir keine sensiblen Berechnungen durchführen ist dies vernachlässigbar. Um Gleitkommazahlen zu runden, gibt es Funktionen im Modul math. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="brightness_val können wir nun einfach einer Funktion in animations.py übergeben, die dann die Helligkeit des Strips setzt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die Funktion, um die Helligkeit in animations.py zu setzen sieht ganz einfach aus: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def set_brightness(brightness_val):"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="strip.brightness(brightness_val)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Aufgerufen wird sie in main.py - am besten in der Endlos-Schleife: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="while True: "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="ldr_val = ldr.read()"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="brightness_val = int(ldr_val / 1100 * 255)"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="animations.set_brightness(brightness_val) "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Diese vier Zeilen lassen sich auch auf zwei komprimieren!"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="while True: "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="animation.set_brightness(int(ldr.read() / 1100 * 255)t)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Analog Digital Converter" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="55649.87" OwnPage="63" ItemID="1953108977" PTYPE="4" WIDTH="453.68" HEIGHT="261.880000000026" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 261.88 L0 261.88 L0 0 Z" copath="M0 0 L453.68 0 L453.68 261.88 L0 261.88 L0 0 Z" gXpos="170.8" gYpos="48594.75" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1950413121"/>
