        <PAGEOBJECT XPOS="170.8" YPOS="63586.88" OwnPage="72" ItemID="1997699217" PTYPE="4" WIDTH="453.68" HEIGHT="577.120000000032" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 577.12 L0 577.12 L0 0 Z" copath="M0 0 L453.68 0 L453.68 577.12 L0 577.12 L0 0 Z" gXpos="170.8" gYpos="63586.88" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" CH="HTTP - Kommunikation mit dem Server"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Bei HTTP handelt es sich um eine Art Sprache, in der ein Webserver und ein Browser miteinander kommunizieren. Diese Kommunikation erfolgt nach einem festgelegten Schema, das man auch &quot;Protokoll&quot; nennt. HTTP ist also ein Protokoll, daher auch der Name &quot;HyperText Transport Protocol&quot;."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Request und Response"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Beim Aufruf einer Seite sendet der Browser zuerst einen &quot;Request&quot; (Anfrage, Bsp: ich möchte die Seite sehen), die vom Server mit einem &quot;Response&quot; (Antwort, Bsp: ich zeige Dir die Seite) beantwortet wird. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="GET und POST"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Jeder Request wird durch die Angabe der Methode eingeleitet. Methoden bestimmen die Aktion der Anforderung. Die aktuelle HTTP-Spezifikation sieht acht Methoden vor: OPTIONS, GET, HEAD, POST, PUT, DELETE, TRACE und CONNECT."/>
                <tab/>
                <ITEXT CH=" Wir bescränken uns jetzt aber auf die Methoden GET und POST. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Die mit Abstand wichtigste Methode ist die Methode "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="GET"/>
                <ITEXT CH=". Hiermit wird - mittels einem Request - zum Beispiel ein Dokument (die aufzurufende HTML-Seite) angefordert. Als Antwort auf diesen Request erhält der Client (unser Browser) die HTML-Seite als Response"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Die "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="POST"/>
                <ITEXT CH="-Methode übermittelt in erster Linie Formulareingaben an den Webserver."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Werden durch den Request lediglich andere Daten als Antwort empfangen, so ist die GET-Methode die richtige Wahl."/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Werden durch den Request Daten auf dem Server verändert, ist die POST-Methode die richtige Wahl."/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Werden Daten für Logins, insbesondere Passwörter übermittelt, dann ist nur POST die einzig richtige Wahl."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Oblique" CH="https://de.wikipedia.org/wiki/Hypertext_Transfer_Protocol"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Oblique" CH="https://wiki.selfhtml.org/wiki/HTTP/Einsteiger-Tutorial"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Oblique" CH="https://wiki.selfhtml.org/wiki/HTTP/Anfragemethoden"/>
                <para PARENT="erklärungen"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="HTML Forulare - Kommunikation mit dem Server " Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="86516.02" OwnPage="98" ItemID="1997506945" PTYPE="4" WIDTH="453.68" HEIGHT="368.48000000004" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 368.48 L0 368.48 L0 0 Z" copath="M0 0 L453.68 0 L453.68 368.48 L0 368.48 L0 0 Z" gXpos="170.8" gYpos="86516.02" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1983191425"/>
        <PAGEOBJECT XPOS="170.8" YPOS="87765.8" OwnPage="99" ItemID="1997512017" PTYPE="4" WIDTH="453.68" HEIGHT="346.5" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 346.5 L0 346.5 L0 0 Z" copath="M0 0 L453.68 0 L453.68 346.5 L0 346.5 L0 0 Z" gXpos="170.8" gYpos="87765.8" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="2095545025" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Das Timer-Modul "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun haben wir erreicht, dass auf dem ESP die aktuelle Systemzeit eingestellt werden kann. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um den ESP aber als Wecker nutzen zu können, müssen wir zusätzlich einen Timer definieren. Hierzu gibt es das Timer-Modul: "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="https://github.com/loboris/MicroPython_ESP32_psRAM_LoBo/wiki/timer"/>
                <para PARENT="links"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Der Timer ist in folgenden Modi verfügbar:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="ONESHOT:"/>
                <ITEXT CH=" Timer läuft für die vorgegebene Zeit und stoppt dann. Wenn die vorgegebene Zeit abgelaufen ist, stoppt der Timer und ruft eine Callback-Funktion auf, wenn diese definiert ist."/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="PERIODIC: "/>
                <ITEXT CH="Timer läuft wiederholt bis er explizit gestoppt wird. Wenn die Zeit abgelaufen ist, wird die Callback-Funktion aufgerufen, sofern diese definiert ist."/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="CHRONO:"/>
                <ITEXT CH=" wird genutzt, um verstrichene Zeit in µs zu messen. Er kann gestartet, gestoppt, pausiert und wieder gestartet werden."/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="EXTBASE"/>
                <ITEXT CH=": Basis-Timer für verlängerte Timer. Hierfür kann nur Timer 0 verwendet werden. "/>
                <para PARENT="Liste"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="tm = machine.Timer(timer_no)"/>
                <ITEXT CH=" erstellt eine Timer-Instanz. timer_no ist die Nummer des verwendeten Timers. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="für Hardware-Timer müssen wir hier 0-3 setzen"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="tm.init(period, mode, callback, dbgpin)"/>
                <ITEXT CH=" initialisiert den Timer "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="period"/>
                <ITEXT CH=": legt die Zeit in ms fest (Default: 10 ms) "/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="mode"/>
                <ITEXT CH=": definiert den Modus (Default: PERIODIC)"/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="callback"/>
                <ITEXT CH=": Die Callback-Funktion, die ausgeführt werden soll, wenn die Zeit abgelaufen ist (Default: None)"/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="dbgpin"/>
                <ITEXT CH=": Debug-Pin. Wenn dieser definiert ist, wird der Wert, der an diesem Pin anliegt gewechselt (wenn ein- dann aus und andersrum)"/>
                <para PARENT="Liste"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Das Timer-Modul" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="88408.4610254" OwnPage="100" ItemID="1997772609" PTYPE="4" WIDTH="453.68" HEIGHT="585.728974599959" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 585.729 L0 585.729 L0 0 Z" copath="M0 0 L453.68 0 L453.68 585.729 L0 585.729 L0 0 Z" gXpos="170.8" gYpos="88408.4610254" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="2001915809" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Das Timer-Modul als Wecker einsetzen "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um das Timer-Modul als Wecker einsetzen zu können müssen wir - neben den bekannten Schritten (Eingabemöglichkeit auf der Webseite erstellen sowie die Daten in main.py entgegennehmen) - auch die Zeit in ms berechnen, die als period in Timer.init() gesetzt wird. Zudem muss eine Callback-Funktion erstellt werden, die aufgerufen wird, sobald die Zeit abgelaufen ist. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wir sind bei der Eingabe auf der Webseite faul und wollen lediglich die Zeit eingeben, zu der der Alarm losgehen soll. Also fügen wir in index.html unter der Form für die Systemzeit mit etwas Abstand noch eine Form zur Eingabe der Alarm-Zeit hinzu:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="       &lt;form class=&quot;form-inline&quot; method=&quot;POST&quot; action=&quot;/&quot; role=&quot;form&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="        &lt;div class=&quot;input-group&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="          &lt;label class=&quot;sr-only&quot; for=&quot;inlineFormInputTime&quot;&gt;Weckzeit&lt;/label&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="          &lt;input type=&quot;text&quot; name=&quot;alarm&quot; class=&quot;form-control mb-2&quot; "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="id=&quot;inlineFormInputTime&quot; placeholder=&quot;{hour:02d}:{minute:02d}&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="          &lt;div class=&quot;input-group-append&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="            &lt;button type=&quot;submit&quot; class=&quot;btn btn-dark mb-2&quot; name=&quot;button&quot; "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="value=&quot;alarm&quot;&gt;Weckzeit einstellen&lt;/button&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="          &lt;/div&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="        &lt;/div&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="      &lt;/form&gt;"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="In main.py initialisieren wir zuerst Timer 1:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="timer = machine.Timer(1)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="und definieren dann eine Funktion setAlarmTime(time), in der wir zuerst die Zeit in ein Tupel konvrtieren: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def setAlarmTime(time):"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="time = convertDateOrTimeToTuple(time)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Anschließend brauchen wir die derzeitige Systemzeit aus Tupel und in Millisekunden, mit denen wir dann die Millisekunden zur Zeitpunkt des Alarms kriegen können: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="curr_time_toup = utime.localtime()"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="curr_time_sec = utime.mktime(curr_time_toup)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="alarm_time_sec = utime.mktime(curr_time_toup[0:3] + time + curr_time_toup[5:])"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="curr_time_toup[0:3] bzw [5:] funktionieren ähnlich wie range. Bei [0:3] (oder [:3] werden alle Elemente von Position 0 bis &lt; 3 zurückgegeben, bei [5:] alle Elemente ab Position 5 (bis Ende)."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um period_sec zu berechnen, müssen wir nun curr_time_sec von alarm_time_sec abziehen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nehmen wir an, wir wollen jetzt (19.30 Uhr) den Wecker für morgen früh um 7.30 Uhr stellen, dann stehen wir vor einem kleinen Problem: Da wir für utime.mktime() alle Werte - bis auf die für Stunde und Minute - des heutigen Tags genutzt haben, wäre der Zeitpunkt in der Vergangenheit.  Das bedeutet alarm_time_sec wäre kleiner als curr_time_sec und somit period_ms negativ!"/>
                <breakline/>
                <ITEXT CH="Wenn dies der Fall ist, müssen wir die Anzahl der Sekunden pro Tag zu period_ms addieren! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="period_sec = alarm_time_sec - curr_time_sec"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="if period_sec &lt; 0:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="period_sec += seconds_per_day"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun können wir den Timer initialisieren und die Callback-Funktion definieren. Aber da der Timer für period den Wert in Millisekunden erwartet, wir aber bisher mit Sekunden gearbeitet haben, müssen wir period = period_sec*1000 einsetzen:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="timer.init(period = period_sec*1000, mode = timer.ONE_SHOT, callback= _timer_callback )"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="86865.6810254" OwnPage="98" ItemID="2001798865" PTYPE="4" WIDTH="453.68" HEIGHT="364.728974599959" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 364.729 L0 364.729 L0 0 Z" copath="M0 0 L453.68 0 L453.68 364.729 L0 364.729 L0 0 Z" gXpos="170.8" gYpos="86865.6810254" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="2095074289" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Fehler in der Eingabe auffangen"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Oftmals kann es bei der Eingabe von Datum und Uhrzeit zu Fehlern kommen. So kann man aus Gewohnheit anstatt 2018 einfach nur 18 eingeben. Oder man verwendet für die Uhrzeit einen Punkt anstatt des Doppelpunkts. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="In beiden Fällen würde die Initialisierung der Systemzeit fehlschlagen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um herauszufinden, ob wir split() mit einem Doppelpunkt oder einem Punkt durchführen müssen, können wir einfach den String nach dem gesuchten Zeichen (oder der gesuchten Zeichenkette durchsuchen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="String.find(char(s) [, startPos = 0, endPos = len]) "/>
                <ITEXT CH="durchsucht den String nach der Zeichenkette, die mit charakter(s) angegeben wurde. startPos und endPos sind optional und bezeichnen die Position ab der, bzw bis zu der durchsucht werden muss."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wenn die Suche erfolgreich war und die Zeichenkette gefunden wurde, gibt sie die Startposition zurück, ansonsten -1. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="date.find(&quot;:&quot;)"/>
                <ITEXT CH=" würde also bei einer korrekter Eingabe den Wert -1 zurückgeben, da wir zur Trennung zwischen Tag, Monat und Jahr in der Regel einen Punkt verwenden."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="date.find(&quot;.&quot;) würde hingegen den Wert 2 zurückgeben, da die erste Position, an der der Punkt auftritt, die Position Nr 2 ist (nicht vergessen: Informatiker fangen bei 0 an zu zählen!) "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Ebenso können wir prüfen, ob die eingegebene Ziffer für das Jahr lediglich zwei Stellen enthält (also 18 statt 2018) und für den Fall, dass lediglich zwei Ziffern vorhanden sind, 2000 dazu addieren."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Mit len() könnten wir die Anzahl der Ziffern für einen String erhalten. Dazu müssen wir für diese Prüfung wieder in einen String konvertieren. "/>
                <breakline/>
                <ITEXT CH="Dies können wir verallgemeinern und in eine Funktion schreiben, die gleichzeitig auch die Konvertierung von String in Tupel aus Integern übernimmt und dann unter anderem in setSystemTime aufrufen: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def convertDateOrTimeToTuple(s):"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="&quot;&quot;&quot; splits given String by '.' or ':'. If resulting List has len = 4"/>
                <para PARENT="Kommentare - Block"/>
                <ITEXT CH=" "/>
                <tab/>
                <ITEXT CH="it is interpreted as date and will be converted in (yyyy, mm, dd). "/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="if value for year has two digits, 2000 is added. "/>
                <para PARENT="Kommentare - Block"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="s: string in format hh:mm or dd.mm.yy(yy)"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="return: Tuple (hh, mm) or (yyyy, mm, dd)"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="&quot;&quot;&quot;"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="if s.find(&quot;.&quot;) &gt;= 0: "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="s_list = list(int(i) for i in s.split(&quot;.&quot;))"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="else:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="s_list = list(int(i) for i in  s.split(&quot;:&quot;))"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="if len(s_list) == 4:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="s_list.reverse()"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="if len(str(s_list[0])) == 2:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="s_list[0] += 2000"/>
                <para PARENT="code"/>
                <ITEXT CH="            "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="return tuple(s_list)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Fehler in der Eingabe auffangen " Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="89161.69" OwnPage="101" ItemID="2001915809" PTYPE="4" WIDTH="453.68" HEIGHT="416.060000000041" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 416.06 L0 416.06 L0 0 Z" copath="M0 0 L453.68 0 L453.68 416.06 L0 416.06 L0 0 Z" gXpos="170.8" gYpos="89161.69" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1997772609"/>
        <PAGEOBJECT XPOS="170.8" YPOS="89560.03" OwnPage="101" ItemID="2001871985" PTYPE="4" WIDTH="453.68" HEIGHT="316.050000000003" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 316.05 L0 316.05 L0 0 Z" copath="M0 0 L453.68 0 L453.68 316.05 L0 316.05 L0 0 Z" gXpos="170.8" gYpos="89560.03" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="2099792625" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Timer Callback"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun müssen wir definieren, was passieren soll, wenn der Timer abgelaufen ist. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wir können den zuletzt aufgerufenen Song abspielen und eine Animation starten. Wie aber bereits bekannt kann es bei rechenintensiven Animationen dazu kommen, dass alles nur sehr langsam abgespielt wird. Daher würde ich - wenn überhaupt eine Animation laufen soll - die Animation blink empfehlen und diese mit langen Delays ausstatten. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um den zuletzt gewählten Song abzuspielen, muss dieser erst in einer Variablen gespeichert werden. Damit es keine Probleme gibt, falls seit dem letzten Start kein Song gewählt wurde, empfiehlt es sich, einen initialen Song am Anfang von main.py zu bestimmen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die Callback-Funktion des Timers braucht als Argument den Timer, für den diese Callback-Funktion gilt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def _timer_callback(timer):"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="&quot;&quot;&quot; Timer callback function to start playing alarm_song"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="and start &quot;blink&quot; animation"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="timer: according timer"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="&quot;&quot;&quot;"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="global is_alarm_running"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="handleSoundThread(alarm_song)"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="handleAnimations(&quot;blink&quot;)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wenn nun also der Timer abgelaufen ist, wird eine Melodie sowie die Animation &quot;blink&quot; abgespielt "/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Timer Callback" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="90133.3510254" OwnPage="102" ItemID="2004150177" PTYPE="4" WIDTH="453.68" HEIGHT="624.618974599944" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 624.619 L0 624.619 L0 0 Z" copath="M0 0 L453.68 0 L453.68 624.619 L0 624.619 L0 0 Z" gXpos="170.8" gYpos="90133.3510254" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Alarm stoppen"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun benötigen wir nur noch eine Routine die es uns ermöglicht, den Alarm wieder zu stoppen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zum Anhalten der beiden Threads können wir ebenso den Touch Sensor einsetzen! Allerdings müssen wir gewährleisten, dass dieser auch nur zum Stop der Animationen fungiert, wenn auch ein Alarm läuft!"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies erreichen wir durch eine Variable. is_alarm_running wird zu Beginn des Programms auf False gesetzt."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wenn der Alarm losgeht wird sie True, sodass der Touch Sensor zum Anhalten der Animationen fungiert (und nicht etwa zum Wechsel). Wenn die Animationen angehalten wurden, muss man sie wieder auf False setzen, sodass Touch wieder dazu dienen kann, zwischen den Animationen zu wechseln. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zu Beginn der File main.py setzen wir nun also ein:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="is_alarm_running = False"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Diese wird dann auf True gesetzt, sobald der Timer abgelaufen ist. Dies geschieht also in _timer_callback: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="is_alarm_running = True"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Anschließend bearbeiten wir die Routine, in der die Touch-Eingabe verarbeitetwird. Hier wird zuerst geprüft, ob is_alarm_running wahr ist und stoppt dann die Animationen. Falls nicht, soll die nächste Animation gestartet werden: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="# if touch is registered check if alarm is running"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="if touch_val &lt; touch_threshold:"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="if is_alarm_running:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="handleSoundThread(&quot;&quot;)"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="handleAnimations(&quot;off&quot;)"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="is_alarm_running = False"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="else:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="# print output and start function handleAnimations() without"/>
                <para PARENT="Kommentare - Zeilen"/>
                <tab/>
                <tab/>
                <ITEXT CH="# argument to start next animation"/>
                <para PARENT="Kommentare - Zeilen"/>
                <tab/>
                <tab/>
                <ITEXT CH="print(&quot;touched!! value: {}&quot;.format(touch_val))"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="handleAnimations()"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Alarm stoppen" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="42421.52" OwnPage="48" ItemID="2003418641" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="42421.52" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1930388257"/>
        <PAGEOBJECT XPOS="170.8" YPOS="77697.12" OwnPage="88" ItemID="2003403553" PTYPE="4" WIDTH="453.68" HEIGHT="533.130000000034" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 533.13 L0 533.13 L0 0 Z" copath="M0 0 L453.68 0 L453.68 533.13 L0 533.13 L0 0 Z" gXpos="170.8" gYpos="77697.12" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1981944209"/>
        <PAGEOBJECT XPOS="170.8" YPOS="3718.85571428571" OwnPage="4" ItemID="2003408257" PTYPE="4" WIDTH="453.68" HEIGHT="613.89428571429" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 613.894 L0 613.894 L0 0 Z" copath="M0 0 L453.68 0 L453.68 613.894 L0 613.894 L0 0 Z" gXpos="170.8" gYpos="3718.85571428571" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="2003322177" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Das ESP32 Development Board"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Beim ESP32 Development Board handelt es sich um einen Mikrocontroller, auch gerne Ein-Platinen-Computer oder Minicomputer genannt."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Neben dem 240 MHz DualCore Prozessor ist der ESP32 noch mit 520 KiB SRAM ausgestattet (SRAM = Static Random Access Memory - also ein Arbeitsspeicher, der Werte und Dateien zwischenspeichern kann solange er mit Strom versorgt wird - wie bei einem Computer. Kib (Kibibyte, 1 KiB = 1024 Byte) und 4  MiB flash-Speicher (= flash EEEPROM, ein Speicherbereich, der nicht verloren geht, sobald kein Strom da ist) "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zum Vergleich: Heute haben die meisten PCs (teilweise auch viele Smartphones) mindestens einen Dual-Core Prozessor mit ca 2.5 GHz, das wäre also mindestens 100 Mal schneller als der ESP32. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Außerdem verfügen ebenso die meisten PCs über mindestens zwei, eher noch vier GB an Arbeitsspeicher, sprich vier bis achtmal so viel. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="DIes sollte allerdings nicht abschrecken, da bei 240 MHz immerhin durchschnittlich circa 240,000,000 Rechenoperationen pro Sekunde durchgeführt werden. Und dies ist eine beachtliche Menge - absolut ausreichend für einen Microcontroller, mit dem man lediglich &quot;kleine&quot; Aufgaben im Vergleich zum PC erledigen möchte."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Neben der Rechenleistung und dem verfügbaren Speicher bringt der ESP noch 34 GPIO Pins (General Purpose Input/Output Pins) mit sich, das sind also 34 sogenannte Pins (kleine Metallstifte), an denen man über ein angeschlossenes Jumperwire (ein Kabel mit passendem Anschluss für die Pins) zum Beispiel Werte eines Feuchtigkeits- oder Temperatursensors auslesen, Werte an einen elektrische Motor oder - wie in unserem Fall - an einen LED-Strip senden kann. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Außerdem sind bereits einige Sensoren integriert: Der Touch-Sensor beispielsweise, der auf Berührung reagiert, oder der Hall-Sensor, der Schwankungen im Erdmagnetfeld misst. Außerdem ist bereits ein Temperatursensor integriert."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zusätzlich kann dieses Board nicht nur eine Funkverbindung per WLAN bereitstellen oder sich per WLAN mit einem bestehenden (Heim-) Netzwerk verbinden, sondern ebenfalls eine Verbindung per Bluetooth herstellen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Auf dem ESP32 läuft ein Mini-Betriebssystem namens freeRTOS (free Real Time Operating System) im Hintergrund. Dadurch hat man zum Beispiel die Möglichkeit zeitgesteuerte Aufgaben zu erstellen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die Einsatzmöglichkeiten sind sehr vielfältig. So kann man mit dem ESP32 zum Beispiel die Wetterdaten oder einfache Daten im Haus zur Luftfeuchtigkeit, Luftdruch und Temperatur messen und protokollieren. Ebenso kann man damit elektrische Motoren steuern - per WLAN oder Bluetooth."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Man kann die integrierten Touch-Sensoren - oder einfach Buttons, die angeschlossen wurden - nutzen, um etwas ein- oder auszuschalten."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Oder man kann sich - wie hier geschehen - LED Strips kaufen und für diese Animationen programmieren. Einen Webserver erstellen, der eine Webseite zur Steuerung der Animationen bereitstellt. Zusätzlich kann man den integrierten Touch-Sensor verwenden, um diese Animationen zu starten. Man kann einen Piezo-Speaker nutzen, um Töne, oder gleich ganze Melodien zu spielen und einen Photowiderstand, um die Helligkeit der LEDs an die Umgebung anzupassen."/>
                <para PARENT="Default Paragraph Style"/>
                <para/>
                <ITEXT CH="Programmieren kann man den ESP32 auf drei verschiedene Arten, bzw. drei verschiedenen Frameworks programmieren: "/>
                <para/>
                <para/>
                <ITEXT CH="Arduino-IDE: "/>
                <tab/>
                <ITEXT CH="Arduino ist - nach dem Raspberry Pi - bestimmt der bekannteste Ein-Platinen-Computer. Daher wird dies wohl auch die geläufigste Sprache bzw das geläufigste Framework sein. Arduino basiert auf C++/C. Man kann, nachdem man einige Einstellungen in der Entwicklungsumgebung für Arduino vorgenommen hat - einfach mit der selben Sprache und der selben Programmierumgebung (IDE = Integrated Development Environment), die man für den Arduino nutzt, auch den ESP32 programmieren. "/>
                <para/>
                <para/>
                <ITEXT CH="MicroPython: "/>
                <tab/>
                <ITEXT CH="MicroPython - eine abgespeckte Version von Python) zeichnet sich dadurch aus, dass sie von Anfängern gerne als leicht lernbar eingestuft wird. Allerdings ist hier die Einrichtung, die man im Vorfeld vornehmen muss, etwas komplexer. "/>
                <para/>
                <para/>
                <ITEXT CH="ESP-IDF: "/>
                <tab/>
                <tab/>
                <ITEXT CH="Das ESP-IoT-Development Framework basiert ebenso auf C++ und ist das ofizielle Framework, das vom Hersteller der ESP entwickelt wurde. Dies erfordert - sowohl für die Einrichtung als auch für den Upload und die Ausführung des Codes einige Grundkenntnisse."/>
                <para/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Das ESP32 Development Board" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="4500.25" OwnPage="5" ItemID="2003322177" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="4500.25" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="2003408257"/>
        <PAGEOBJECT XPOS="170.8" YPOS="6353.23" OwnPage="7" ItemID="2003413521" PTYPE="4" WIDTH="453.68" HEIGHT="182.27" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 182.27 L0 182.27 L0 0 Z" copath="M0 0 L453.68 0 L453.68 182.27 L0 182.27 L0 0 Z" gXpos="170.8" gYpos="6353.23" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Ordnerinhalt auflisten "/>
                <para PARENT="Überschrift - normal-fett"/>
                <ITEXT FONT="DejaVu Sans Mono Bold" CH="ls "/>
                <ITEXT CH="(= list)"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Mit dem Befehl "/>
                <ITEXT FONT="DejaVu Sans Mono Bold" CH="ls"/>
                <ITEXT CH=" gibt man den Inhalt eines Ordners aus. Nur "/>
                <ITEXT FONT="DejaVu Sans Mono Bold" CH="ls"/>
                <ITEXT CH=", ohne Optionen, listet den Inhalt des aktuellen Arbeitsverzeichnisses, während man mit "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH="$ ls /home/$USER/Bilder "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH=" - unabhängig vom derzeitigen Arbeitsverzeichnis - zum Beispiel den Inhat des Ordners &quot;Bilder&quot; im home-Verzeichnis des aktuellen Benutzers (=$USER) ausgeben. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Terminalbefehle: Ordnerinhalt aufliste" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="6532.75" OwnPage="7" ItemID="2008329121" PTYPE="4" WIDTH="453.68" HEIGHT="445.67" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 445.67 L0 445.67 L0 0 Z" copath="M0 0 L453.68 0 L453.68 445.67 L0 445.67 L0 0 Z" gXpos="170.8" gYpos="6532.75" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Verzeichnis wechseln "/>
                <para PARENT="Überschrift - normal-fett"/>
                <ITEXT FONT="DejaVu Sans Mono Bold" CH="cd"/>
                <ITEXT CH=" (=change directory)"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH=" "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um in ein Unterverzeichnis zu wechseln, gibst du einfach den befehl cd ein, gefolgt durch den Namen des Unterverzeichnisses. Leerzeichen zwischen diesen beiden nicht vergessen! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="$ cd Bilder "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="wechselt also in einen Unterordner namens &quot;Bilder&quot;, der sich im akuellen Arbeitsverzeichnis befinden muss. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Tipp für Schreibfaule: Man kann auch einfach die ersten Buchstaben des Verzeichnisses eingeben. Mit einem Klick auf die TAB taste erledigt die Autovervollständigung den Rest der Schreibarbeit – es sei denn es gibt mehrere Dateien oder Verzichnisse mit dieser Zeichenfolge am Anfang, oder es befindet sich ein Schreibfehler darin. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Möchte man in ein ganz anderes Verzeichnis wechseln, gibt man einfach cd  ein, gefolgt vom kompletten Verzeichnispfad ein. Dieses Mal muss allerdings ein"/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH=" /"/>
                <ITEXT CH=" vor den Pfad, um zu signlaisieren, dass es sich nicht um ein Unterverzeichnis handelt "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="$ cd /tmp"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="wechselt nun in das temporäre Verzeichnis namens &quot;tmp&quot;, dass sich im Root-Verzeichnis (Unix-Äquivalent zu Windows' &quot;C:\&quot;-Laufwerk."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um wieder ins Übergeordnete Verzeichnis zu wechseln, einfach"/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH=" cd .."/>
                <ITEXT CH=" eingeben."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="$ cd .. "/>
                <para PARENT="code"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Terminalbefehle: Verzeichnis wechseln" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
