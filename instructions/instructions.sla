            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="12" CH="und einmal, nachdem der Controller eingesteckt wurde.  Der markierte Bereich zeigt alle Meldungen bezüglich dem zuletzt eingesteckten USB-Geräts. Zum Beispiel idVendor (= Hersteller-ID) und idProduct, verwendeter Treiber (Product: CP2102 USB to UART Bridge Controller)."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Für uns ist lediglich die letzte Zeile interessant. Hier steht &quot;"/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="cp210x converter now attached to ttyUSB0"/>
                <ITEXT FONTSIZE="12" CH="&quot;, was bedeutet, dass unser Gerät, das tatsächlich mit dem cp210x-Treiber läuft, am Port "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="ttyUSB0"/>
                <ITEXT FONTSIZE="12" CH=" angeschlossen wurde."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Für Windows gibt es leider kein Terminal-Äquivalent zu dmesg. Jedoch kann man hier im Hardwaremanager (einfach in die Suche eingeben und mit Enter bestätigen) unter &quot;USB-Controller&quot; oder &quot;Anschlüsse (COM &amp; LTP)&quot; bei eingestecktem Gerät sehen, unter welchem Port das Gerät zu erreichen ist. Unter Windows sind USB-Geräte an den sogenannten &quot;COM-Port&quot; gebunden. "/>
                <trail PARENT="Default Paragraph Style"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="9991.36102539981" OwnPage="11" ItemID="1895607825" PTYPE="2" WIDTH="310.95" HEIGHT="123.375" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" TEXTFLOWMODE="2" LOCALSCX="0.75" LOCALSCY="0.75" LOCALX="0" LOCALY="-82.491966133585" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" Pagenumber="0" PFILE="../../../../Schreibtisch/pics-projekt/dmesg2marked.png" IRENDER="0" EMBEDDED="0" path="M0 0 L310.95 0 L310.95 123.375 L0 123.375 L0 0 Z" copath="M0 0 L310.95 0 L310.95 123.375 L0 123.375 L0 0 Z" ImageRes="0" gXpos="-9.19999999999999" gYpos="32" gWidth="475.28" gHeight="267.743974600189" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="170.8" YPOS="16846.71" OwnPage="19" ItemID="1895609153" PTYPE="4" WIDTH="453.68" HEIGHT="350.790000000005" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 350.79 L0 350.79 L0 0 Z" copath="M0 0 L453.68 0 L453.68 350.79 L0 350.79 L0 0 Z" gXpos="170.8" gYpos="16846.71" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Bauteile verbinden"/>
                <para PARENT="Überschrift Mittel"/>
                <para PARENT="Überschrift Mittel"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Neugierig? Erster Test?? "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="LED Strip an ESP32 anschließen"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Überschrift - normal-fett"/>
                <ITEXT CH="Also. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Du hast den LED-Strip, dieser hat an jedem Ende drei Anschlüsse, einen für 5V (rot), einen Data-Pin (meistens gelb) und einen für Ground, oder GND (schwarz oder weiß).  "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Schließe hier das rote und das schwarze (oder weiße) Kabel an eine externe Stromquelle (falls es nur wenige LED sind, kannst du auch den 5V Ausgang und GND vom ESP nutzen. Allerdings kann dies bei Überspannung deinen USB-Port am Rechner zumindest vorübergehend außer Gefecht setzen). Wichtig: um den Stromkreis zu schließen muss der GND der externen Stromquelle mit dem GND des ESP verbunden sein. Anschließend verbindest Du den Data-Pin des LED-Strips (DIN oder DI) mit Pin 14 am ESP. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Der Controller wird - falls noch nicht geschehen - per USB-Kabel mit dem Computer verbunden."/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Bauteile verbinden" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
                <ItemAttribute Name="Inhalt" Type="none" Value="LED Strip an ESP anschließen" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="480.98" YPOS="17019.43" OwnPage="19" ItemID="1896408881" PTYPE="2" WIDTH="143" HEIGHT="175.5" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR="White" PCOLOR2="Black" PLINEART="1" TEXTFLOWMODE="2" LOCALSCX="0.266666666666667" LOCALSCY="0.266666666666667" LOCALX="19.7999999999999" LOCALY="45.9375" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" Pagenumber="0" PFILE="../../RainbowWarrior_schaltung/esp32_led_direkt.png" IRENDER="0" EMBEDDED="0" path="M0 0 L143 0 L143 175.5 L0 175.5 L0 0 Z" copath="M0 0 L143 0 L143 175.5 L0 175.5 L0 0 Z" ImageRes="0" gXpos="480.98" gYpos="17019.43" gWidth="0" gHeight="0" GRExtM="3" GRTYPM="1" GRSTARTXM="0" GRSTARTYM="0" GRENDXM="143" GRENDYM="0" GRFOCALXM="0" GRFOCALYM="0" GRSCALEM="1" GRSKEWM="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <M_CSTOP RAMP="0" NAME="Black" SHADE="100" TRANS="1"/>
