            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" CH="HTML - Formulare "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Formulare eignen sich hervorragend um Nutzereingaben in Form von Text (Bsp. Name, Nachrichten ...) oder einem Auswahlmenü  an den Server zu senden. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
