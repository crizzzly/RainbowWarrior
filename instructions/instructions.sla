                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="$ git clone https://github.com/loboris/MicroPython_ESP32_psRAM_LoBo.git"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Oder aber man besucht die Seite "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="https://github.com/loboris/MicroPython_ESP32_psRAM_LoBo"/>
                <para PARENT="links"/>
                <para PARENT="links"/>
                <ITEXT CH="klickt auf den Button „clone or download“ und lädt sich manuell die Zip-Datei herunter."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Diese musst allerdings zuerst in ein geeignetes Verzeichnis entpackt werden."/>
                <trail/>
