        <PAGEOBJECT XPOS="180" YPOS="31072.5" OwnPage="69" ItemID="1831037897" PTYPE="4" WIDTH="475.28" HEIGHT="542.397949200364" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 542.398 L0 542.398 L0 0 Z" copath="M0 0 L475.28 0 L475.28 542.398 L0 542.398 L0 0 Z" gXpos="180" gYpos="31072.5" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" CH="HTTP - Kommunikation mit dem Server"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Bei HTTP handelt es sich um eine Art Sprache, in der ein Webserver und ein Browser miteinander kommunizieren. Diese Kommunikation erfolgt nach einem festgelegten Schema, das man auch &quot;Protokoll&quot; nennt. HTTP ist also ein Protokoll, daher auch der Name &quot;HyperText Transport Protocol&quot;."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Request und Response"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Beim Aufruf einer Seite sendet der Browser zuerst einen &quot;Request&quot; (Anfrage, Bsp: ich möchte die Seite sehen), die vom Server mit einem &quot;Response&quot; (Antwort, Bsp: ich zeige Dir die Seite) beantwortet wird. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="GET und POST"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Jeder Request wird durch die Angabe der Methode eingeleitet. Methoden bestimmen die Aktion der Anforderung. Die aktuelle HTTP-Spezifikation sieht acht Methoden vor: OPTIONS, GET, HEAD, POST, PUT, DELETE, TRACE und CONNECT."/>
                <tab/>
                <ITEXT CH=" Wir bescränken uns jetzt aber auf die Methoden GET und POST. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Die mit Abstand wichtigste Methode ist die Methode "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="GET"/>
                <ITEXT CH=". Hiermit wird - mittels einem Request - zum Beispiel ein Dokument (die aufzurufende HTML-Seite) angefordert. Als Antwort auf diesen Request erhält der Client (unser Browser) die HTML-Seite als Response"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Die "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="POST"/>
                <ITEXT CH="-Methode übermittelt in erster Linie Formulareingaben an den Webserver."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Werden durch den Request lediglich andere Daten als Antwort empfangen, so ist die GET-Methode die richtige Wahl."/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Werden durch den Request Daten auf dem Server verändert, ist die POST-Methode die richtige Wahl."/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Werden Daten für Logins, insbesondere Passwörter übermittelt, dann ist nur POST die einzig richtige Wahl."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="https://de.wikipedia.org/wiki/Hypertext_Transfer_Protocol"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="https://wiki.selfhtml.org/wiki/HTTP/Einsteiger-Tutorial"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="https://wiki.selfhtml.org/wiki/HTTP/Anfragemethoden"/>
                <para PARENT="erklärungen"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="HTML Forulare - Kommunikation mit dem Server " Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="180" YPOS="44171.0710253998" OwnPage="99" ItemID="1837973433" PTYPE="4" WIDTH="475.28" HEIGHT="611.42897460021" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 611.429 L0 611.429 L0 0 Z" copath="M0 0 L475.28 0 L475.28 611.429 L0 611.429 L0 0 Z" gXpos="180" gYpos="44171.0710253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1735366265"/>
        <PAGEOBJECT XPOS="180" YPOS="45052.9610253998" OwnPage="101" ItemID="1827460617" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800565" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="180" gYpos="45052.9610253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Das Timer-Modul "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun haben wir erreicht, dass auf dem ESP die aktuelle Systemzeit eingestellt werden kann. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um den ESP aber als Wecker nutzen zu können, müssen wir zusätzlich einen Timer definieren. Hierzu gibt es das Timer-Modul: "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="https://github.com/loboris/MicroPython_ESP32_psRAM_LoBo/wiki/timer"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Der Timer ist in folgenden Modi verfügbar:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="ONESHOT:"/>
                <ITEXT CH=" Timer läuft für die vorgegebene Zeit und stoppt dann. Wenn die vorgegebene Zeit abgelaufen ist, stoppt der Timer und ruft eine Callback-Funktion auf, wenn diese definiert ist."/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="PERIODIC: "/>
                <ITEXT CH="Timer läuft wiederholt bis er explizit gestoppt wird. Wenn die Zeit abgelaufen ist, wird die Callback-Funktion aufgerufen, sofern diese definiert ist."/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="CHRONO:"/>
                <ITEXT CH=" wird genutzt, um verstrichene Zeit in µs zu messen. Er kann gestartet, gestoppt, pausiert und wieder gestartet werden."/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="EXTBASE"/>
                <ITEXT CH=": Basis-Timer für verlängerte Timer. Hierfür kann nur Timer 0 verwendet werden. "/>
                <para PARENT="Liste"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="tm = machine.Timer(timer_no)"/>
                <ITEXT CH=" erstellt eine Timer-Instanz. timer_no ist die Nummer des verwendeten Timers. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="für Hardware-Timer müssen wir hier 0-3 setzen"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="tm.init(period, mode, callback, dbgpin)"/>
                <ITEXT CH=" initialisiert den Timer "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="period"/>
                <ITEXT CH=": legt die Zeit in ms fest (Default: 10 ms) "/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="mode"/>
                <ITEXT CH=": definiert den Modus (Default: PERIODIC)"/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="callback"/>
                <ITEXT CH=": Die Callback-Funktion, die ausgeführt werden soll, wenn die Zeit abgelaufen ist (Default: None)"/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="dbgpin"/>
                <ITEXT CH=": Debug-Pin. Wenn dieser definiert ist, wird der Wert, der an diesem Pin anliegt gewechselt (wenn ein- dann aus und andersrum)"/>
                <para PARENT="Liste"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Das Timer-Modul" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.28" YPOS="45052.9610253998" OwnPage="102" ItemID="1733531497" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800565" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="735.28" gYpos="45052.9610253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1840568377" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Das Timer-Modul als Wecker einsetzen "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um das Timer-Modul als Wecker einsetzen zu können müssen wir - neben den bekannten Schritten (Eingabemöglichkeit auf der Webseite erstellen sowie die Daten in main.py entgegennehmen) - auch die Zeit in ms berechnen, die als period in Timer.init() gesetzt wird. Zudem muss eine Callback-Funktion erstellt werden, die aufgerufen wird, sobald die Zeit abgelaufen ist. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wir sind bei der Eingabe auf der Webseite faul und wollen lediglich die Zeit eingeben, zu der der Alarm losgehen soll. Also fügen wir in index.html unter der Form für die Systemzeit mit etwas Abstand noch eine Form zur Eingabe der Alarm-Zeit hinzu:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="       &lt;form class=&quot;form-inline&quot; method=&quot;POST&quot; action=&quot;/&quot; role=&quot;form&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="        &lt;div class=&quot;input-group&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="          &lt;label class=&quot;sr-only&quot; for=&quot;inlineFormInputTime&quot;&gt;Weckzeit&lt;/label&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="          &lt;input type=&quot;text&quot; name=&quot;alarm&quot; class=&quot;form-control mb-2&quot; "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="id=&quot;inlineFormInputTime&quot; placeholder=&quot;{hour:02d}:{minute:02d}&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="          &lt;div class=&quot;input-group-append&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="            &lt;button type=&quot;submit&quot; class=&quot;btn btn-dark mb-2&quot; name=&quot;button&quot; "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="value=&quot;alarm&quot;&gt;Weckzeit einstellen&lt;/button&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="          &lt;/div&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="        &lt;/div&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="      &lt;/form&gt;"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="In main.py initialisieren wir zuerst Timer 1:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="timer = machine.Timer(1)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="und definieren dann eine Funktion setAlarmTime(time), in der wir zuerst die Zeit in ein Tupel konvrtieren: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def setAlarmTime(time):"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="time = convertDateOrTimeToTuple(time)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Anschließend brauchen wir die derzeitige Systemzeit aus Tupel und in Millisekunden, mit denen wir dann die Millisekunden zur Zeitpunkt des Alarms kriegen können: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="curr_time_toup = utime.localtime()"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="curr_time_sec = utime.mktime(curr_time_toup)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="alarm_time_sec = utime.mktime(curr_time_toup[0:3] + time + curr_time_toup[5:])"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="curr_time_toup[0:3] bzw [5:] funktionieren ähnlich wie range. Bei [0:3] (oder [:3] werden alle Elemente von Position 0 bis &lt; 3 zurückgegeben, bei [5:] alle Elemente ab Position 5 (bis Ende)."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um period_sec zu berechnen, müssen wir nun curr_time_sec von alarm_time_sec abziehen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nehmen wir an, wir wollen jetzt (19.30 Uhr) den Wecker für morgen früh um 7.30 Uhr stellen, dann stehen wir vor einem kleinen Problem: Da wir für utime.mktime() alle Werte - bis auf die für Stunde und Minute - des heutigen Tags genutzt haben, wäre der Zeitpunkt in der Vergangenheit.  Das bedeutet alarm_time_sec wäre kleiner als curr_time_sec und somit period_ms negativ!"/>
                <breakline/>
                <ITEXT CH="Wenn dies der Fall ist, müssen wir die Anzahl der Sekunden pro Tag zu period_ms addieren! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="period_sec = alarm_time_sec - curr_time_sec"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="if period_sec &lt; 0:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="period_sec += seconds_per_day"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun können wir den Timer initialisieren und die Callback-Funktion definieren. Aber da der Timer für period den Wert in Millisekunden erwartet, wir aber bisher mit Sekunden gearbeitet haben, müssen wir period = period_sec*1000 einsetzen:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="timer.init(period = period_sec*1000, mode = timer.ONE_SHOT, callback= _timer_callback )"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.28" YPOS="44171.0710253998" OwnPage="100" ItemID="1837295609" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800565" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="735.28" gYpos="44171.0710253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Fehler in der Eingabe auffangen"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Oftmals kann es bei der Eingabe von Datum und Uhrzeit zu Fehlern kommen. So kann man aus Gewohnheit anstatt 2018 einfach nur 18 eingeben. Oder man verwendet für die Uhrzeit einen Punkt anstatt des Doppelpunkts. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="In beiden Fällen würde die Initialisierung der Systemzeit fehlschlagen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um herauszufinden, ob wir split() mit einem Doppelpunkt oder einem Punkt durchführen müssen, können wir einfach den String nach dem gesuchten Zeichen (oder der gesuchten Zeichenkette durchsuchen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="String.find(char(s) [, startPos = 0, endPos = len]) "/>
                <ITEXT CH="durchsucht den String nach der Zeichenkette, die mit charakter(s) angegeben wurde. startPos und endPos sind optional und bezeichnen die Position ab der, bzw bis zu der durchsucht werden muss."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wenn die Suche erfolgreich war und die Zeichenkette gefunden wurde, gibt sie die Startposition zurück, ansonsten -1. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="date.find(&quot;:&quot;)"/>
                <ITEXT CH=" würde also bei einer korrekter Eingabe den Wert -1 zurückgeben, da wir zur Trennung zwischen Tag, Monat und Jahr in der Regel einen Punkt verwenden."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="date.find(&quot;.&quot;) würde hingegen den Wert 2 zurückgeben, da die erste Position, an der der Punkt auftritt, die Position Nr 2 ist (nicht vergessen: Informatiker fangen bei 0 an zu zählen!) "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Ebenso können wir prüfen, ob die eingegebene Ziffer für das Jahr lediglich zwei Stellen enthält (also 18 statt 2018) und für den Fall, dass lediglich zwei Ziffern vorhanden sind, 2000 dazu addieren."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Mit len() könnten wir die Anzahl der Ziffern für einen String erhalten. Dazu müssen wir für diese Prüfung wieder in einen String konvertieren. "/>
                <breakline/>
                <ITEXT CH="Dies können wir verallgemeinern und in eine Funktion schreiben, die gleichzeitig auch die Konvertierung von String in Tupel aus Integern übernimmt und dann unter anderem in setSystemTime aufrufen: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def convertDateOrTimeToTuple(s):"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="&quot;&quot;&quot; splits given String by '.' or ':'. If resulting List has len = 4 it "/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="is interpreted as date and will be converted in (yyyy, mm, dd). if value "/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="for year has two digits, 2000 is added. "/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="s: string in format hh:mm or dd.mm.yy(yy)"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="return: Tuple (hh, mm) or (yyyy, mm, dd)"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="&quot;&quot;&quot;"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="if s.find(&quot;.&quot;) &gt;= 0: "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="s_list = list(int i for s.split(&quot;.&quot;))"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="else:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="s_list = list(int i for s.split(&quot;:&quot;))"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="if len(s_list) == 4:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="s_list.reverse()"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="if s_list[0] == 2:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="s_list[0] += 2000"/>
                <para PARENT="code"/>
                <ITEXT CH="            "/>
                <para PARENT="code"/>
                <tab/>
