            <PAGEOBJECT XPOS="330.48" YPOS="35476.7479492004" OwnPage="81" ItemID="1714348185" PTYPE="2" WIDTH="324.8" HEIGHT="289.6" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="0.266666666666667" LOCALSCY="0.266666666666667" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" Pagenumber="0" PFILE="../../RainbowWarrior_schaltung/esp32_led_ldr_piezo_Steckplatine.png" IRENDER="0" EMBEDDED="0" path="M0 0 L324.8 0 L324.8 289.6 L0 289.6 L0 0 Z" copath="M0 0 L324.8 0 L324.8 289.6 L0 289.6 L0 0 Z" gXpos="150.48" gYpos="124.576923800567" gWidth="475.28" gHeight="414.176923800565" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
