        <PAGEOBJECT XPOS="170.8" YPOS="17235.43" OwnPage="19" ItemID="1899131873" PTYPE="4" WIDTH="453.68" HEIGHT="325.669999999995" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 325.67 L0 325.67 L0 0 Z" copath="M0 0 L453.68 0 L453.68 325.67 L0 325.67 L0 0 Z" gXpos="170.8" gYpos="17235.43" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Eine Verbindung zum ESP herstellen - rshell und REPL"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hierfür verwenden wir das Tool &quot;"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="rshell"/>
                <ITEXT CH="&quot; (= remote shell), das es uns ermöglicht, eine Verbindung zum Controller herzustellen um Dateien und Ordner hochzuladen, auszulesen, zu verschieben oder zu löschen, oder REPL zu starten. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Rshell benötigt einige Angaben, wie zum Beispiel die Puffergröße (--buffer-size=30) und den Port (-p /dev/ttyUSB0), wobei hier der vorher ermittelte Port (/dev/ttyUSB0) anzugeben ist."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Beentet werden kann rshell mit der Tastenkombination STRG+C"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="repl"/>
                <ITEXT CH=" (= Read–eval–print loop) wiederum ist dazu da, um per Kommandozeile Befehle auf dem Board auszuführen, kleine Funktionen zu erstellen und diese, oder andere vorhandene Skripte auszuführen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="$ rshell --buffer-size=30 -p /dev/ttyUSB0 -a -e nano "/>
                <para PARENT="code"/>
                <ITEXT CH="$ repl"/>
                <para PARENT="code"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Eine Verbindung zum ESP32 herstellen -rshell" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="17910.3710253998" OwnPage="20" ItemID="1897904337" PTYPE="4" WIDTH="453.68" HEIGHT="303.238974600201" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 303.239 L0 303.239 L0 0 Z" copath="M0 0 L453.68 0 L453.68 303.239 L0 303.239 L0 0 Z" gXpos="170.8" gYpos="17910.3710253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Micropython "/>
                <para PARENT="Überschrift Mittel"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="MicroPython ist - wie bereits erwähnt - eine etwas &quot;abgespeckte&quot; Version von Python (Version 3). Daher kann man sich auf die Beschreibungen, die sich auf Python beziehen, nur teilweise verlassen. Jedoch bieten sie gute Beschreibungen in Sachen Aufbau, Verwendung der einzelnen Module und Funktionen sowie der gängigen Programmier-Routinen, weshalb öfter auf Seiten verwiesen wird, die eigentlich Tutorials / Anleitungen für Python 3 bereitstellen:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="links"/>
                <ITEXT CH="Python4Kids, sowie deren Unterseite &quot;Denken wie ein Informatiker&quot; bietet sich speziell für jüngere Generationen an:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="python4kids.net "/>
                <para PARENT="links"/>
                <ITEXT CH="http://python4kids.net/how2think/index.htm"/>
                <para PARENT="links"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Auch auf der deutschen Seite python-kurs.eu findet sich ein sehr schön beschriebenes und duch simple Grafiken unterstütztes Tutorial für Python 3:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="https://www.python-kurs.eu/python3_kurs.php"/>
                <para PARENT="links"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Micropython" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="18957.0691936568" OwnPage="21" ItemID="1899150913" PTYPE="4" WIDTH="453.68" HEIGHT="367.810806343194" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 367.811 L0 367.811 L0 0 Z" copath="M0 0 L453.68 0 L453.68 367.811 L0 367.811 L0 0 Z" gXpos="170.8" gYpos="18957.0691936568" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1900782657" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Neopixel"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Bevor wir den Strip leuchten lassen können, müssen wir also zuerst das Modul namens machine einbinden und damit den Pin definieren, an den wir den LED-Strip angeschlossen haben. Anschließend muss ein Objekt von &quot;Neopixel&quot; erzeugt werden. Im Fachjargon nennt man das &quot;eine Instanz einer Klasse&quot; erstellen. Aber da das Prinzip der Klassen für Anfänger oft schwer nachvollziehbar ist, lassen wir dieses Detail einmal aus. Gerne darf aber bei Interesse nachgelesen werden. Das deutschsprachige Python-Tutorial bietet dazu eine einfach geschriebene und detaillierte Erklärung: "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="https://www.python-kurs.eu/python3_klassen.php. "/>
                <para PARENT="links"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="mit dem &quot;"/>
                <ITEXT FONT="DejaVu Sans Mono Bold" CH="import"/>
                <ITEXT CH="&quot; - Befehl sind wir in der Lage, Module einzubinden. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Auch wichtig für den Umgang mit der Neopixel-Library ist die Modulbeschreibung des Entwicklers:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="https://github.com/loboris/MicroPython_ESP32_psRAM_LoBo/wiki/neopixel"/>
                <para PARENT="links"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Mono Bold" CH="machine.Pin([pin_number], [input / output]) "/>
                <ITEXT CH="definiert den Pin, den wir in [pin_number] angeben, sowie ob hier Signale empfangen (input) oder gesendet (output) werden. "/>
                <ITEXT FONT="DejaVu Sans Mono Bold" CH="machine.Pin.OUT "/>
                <ITEXT CH="gibt zum Beispiel an, dass hier Signale vom Controller aus (an den LED-Strip) gesendet werden. Gib nun folgende Befehle nacheinander in REPL ein und bestätige sie jeweils mit Enter:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; import machine"/>
                <para PARENT="code"/>
                <ITEXT CH="&gt;&gt;&gt; led_pin = machine.Pin(14, machine.Pin.OUT)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um ein Objekt &quot;Neopixel&quot; zu erstellen, müssen ebenso einige Angaben gemacht werden. So muss der Pin angegeben werden, an dem der Strip angeschlossen ist, sowie die Anzahl der Pixel auf den Strip (in meinem Fall sind das 14 Pixel). Auch der verwendete LED-Typ sollte angegeben werden, sofern du auch den Strip vom Typ &quot;SK6812&quot;. Wenn man dies auslässt ist als Standard-Typ TYPE_RGB angegeben, also LEDs mit jeweils drei Farben: rot, blau grün. Der SK6812 besteht sogenannten &quot;RGBW&quot; LED, sprich ein Pixel besteht aus vier Farben. Neben RGB auch weiß, so sind die LEDs auch einzeln dimmbar. So setzt sich dieser Schritt nun zusammen: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Mono Bold" CH="machine.Neopixel(pin, num_pixels[, type]) "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Den Pin haben wir ja bereits im letzten Schritt in einer Variablen namens &quot;led_pin&quot; gespeichert. Es bietet sich an, auch die Pixelanzahl (hier: 14) in einer Variablen zu speichern.  Warum dies sinnvoll ist, wirst du in einem späteren Kapitel erfahren. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Gib nun folgende Befehle ein, um die Anzahl der Pixel in der Variablen &quot;num_pixels&quot; zu speichern und eine Instanz der Klasse Neopixel in der Variablen &quot;strip&quot; zu speichern:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; num_pixels = 14"/>
                <para PARENT="code"/>
                <ITEXT CH="&gt;&gt;&gt; led_pin = machine.Pin(14, machine.Pin.OUT)"/>
                <para PARENT="code"/>
                <ITEXT CH="&gt;&gt;&gt; strip = machine.Neopixel(led_pin, num_pixels, machine.Neopixel.TYPE_RGBW)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Selbstverständlich kannn man dies auch auf zwei Zeilen, bzw. zwei Befehle komprimieren, allerdings wirst du gleich sehen, dass dies vergleichsweise sehr unübersichtlich ist: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; strip = machine.Neopixel(machine.Pin(14, machine.Pin.OUT), 11, machine.Neopixel.TYPE_RGBW)"/>
                <para PARENT="code"/>
                <para/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Micropython: Neopixel" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="17728.6" OwnPage="20" ItemID="1903187057" PTYPE="4" WIDTH="453.68" HEIGHT="225.91897460019" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 225.919 L0 225.919 L0 0 Z" copath="M0 0 L453.68 0 L453.68 225.919 L0 225.919 L0 0 Z" gXpos="-9.19999999999999" gYpos="9715.01897460019" gWidth="475.28" gHeight="150.698974600187" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="12" CH="Ab hier verändert sich die Darstellung der Zeilen! "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Im Python-REPL werden die Zeilenanfäge mit den Zeichen &quot;&gt;&gt;&gt;&quot; signalisiert. So weiß man sofort, dass man sich nicht in der systemeigenen Shell, sondern in der Python-Shell befindet, hier also alle Befehle in Python geschrieben werden."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Beendet wird REPL mit der Tastenkombination STRG+X"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="17728.6" OwnPage="20" ItemID="1900697361" PTYPE="2" WIDTH="300.75" HEIGHT="107.25" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" TEXTFLOWMODE="2" LOCALSCX="0.75" LOCALSCY="0.75" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" Pagenumber="0" PFILE="../../../../Schreibtisch/pics-projekt/repl-1.png" IRENDER="0" EMBEDDED="0" path="M0 0 L300.75 0 L300.75 107.25 L0 107.25 L0 0 Z" copath="M0 0 L300.75 0 L300.75 107.25 L0 107.25 L0 0 Z" ImageRes="0" gXpos="-9.19999999999999" gYpos="9715.0189746002" gWidth="475.28" gHeight="150.698974600187" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="170.8" YPOS="19492.38" OwnPage="22" ItemID="1900782657" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="19492.38" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1899150913"/>
        <PAGEOBJECT XPOS="170.8" YPOS="20374.27" OwnPage="23" ItemID="1900776673" PTYPE="4" WIDTH="453.68" HEIGHT="499.200000000001" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 499.2 L0 499.2 L0 0 Z" copath="M0 0 L453.68 0 L453.68 499.2 L0 499.2 L0 0 Z" gXpos="170.8" gYpos="20374.27" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="LEDs leuchten lassen "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="np.set( pos, color [, white, num, update] ) "/>
                <ITEXT FONT="DejaVu Sans Book" CH="ist dafür zuständig, die Pixels leuchten zu lassen. Auch hier sind zwei Angaben zwingend erforderlich, alle anderen (in der eckigen Klammer) optional."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="pos"/>
                <ITEXT CH=" bezeichnet den Index des Pixels, dessen Farbwert gesetzt werden soll. "/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="color"/>
                <ITEXT CH=" gibt - wie der Name schon sagt - an, in welcher Farbe der Pixel leuchten soll. Hier kann man entweder vordefinierte Farben verwenden (wie im folgenden Beispiel), oder Farbwerte als Hexadezimalwert (wird später noch erklärt)"/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="white"/>
                <ITEXT CH=" (optional) gibt - bei RGBW-Strips - an, wie hell die Farbe weiß in diesem Pixel leuchten soll. Wird hier nichts angegeben, wird als Standardwert 0 gesetzt. Hier sind Werte von 0-255 möglich. Je höher, desto mehr weiß wird in die Farbe gemischt. "/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="num"/>
                <ITEXT CH=" (optional) definiert, wieviele Pixel in diesem Schritt gesetzt werden sollen, ausgehend vom Pixel, der in pos angegeben wurde. Standardwert ist her 1, also wird standardgemäß nur der eine Pixel gesteuert, der in num angegeben wurde. "/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="update"/>
                <ITEXT CH=" (optional) bestimmt, ob der oder die Pixel sofort in der jeweiligen Farbe angezeigt werden soll, bzw sollen (was auch Standardwert ist), oder ob mit der Anzeige gewartet werden soll. Falls abgewartet werden sollte, ist update=False anzugeben."/>
                <para PARENT="Liste"/>
                <ITEXT CH="Um dann allerdings die Pixel in der vorher gesetzten Farbe leuchten zu lassen, ist zusätzlich noch der Befehl strip.show() nötig."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; strip.set(0, strip.NAVY) "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="lässt also den ersten Pixel des Strips in der Farve &quot;Navy&quot; leuchten, während "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; strip.set(0, strip.NAVY, num=num_pixels) "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="alle Pixel in der Farbe &quot;Navy&quot; leuchten lässt"/>
                <trail PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Micropython Neopixel: LEDs leuchten lassen" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="20870.4691936568" OwnPage="23" ItemID="1904824785" PTYPE="4" WIDTH="453.68" HEIGHT="218.190806343191" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 218.191 L0 218.191 L0 0 Z" copath="M0 0 L453.68 0 L453.68 218.191 L0 218.191 L0 0 Z" gXpos="170.8" gYpos="20870.4691936568" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1880302513" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="12" CH="Farben "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Wie bereits erwähnt kann man die Farbe aus vordefinierten Farben oder als Hexadezimalwert angeben."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Um herauszufinden, welche Farben verfügbar sind, gibt es einen kleinen Trick: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Gib einfach &quot;strip.&quot; in REPL ein und drücke einmal auf TAB."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="So erscheint eine Auflistung aller vordefinierten Werte und sogar auch der vorhandenen Funktionen.  Die vordefinierten Werte sind konstante Werte und werden in Großbuchstaben angezeigt. So sind folgende Farbwerte bereits definiert: "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="BLACK"/>
                <ITEXT FONTSIZE="12" CH=", "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="WHITE"/>
                <ITEXT FONTSIZE="12" CH=", "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="RED"/>
                <ITEXT FONTSIZE="12" CH=", "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="LIME"/>
                <ITEXT FONTSIZE="12" CH=", "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="BLUE"/>
                <ITEXT FONTSIZE="12" CH=", "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="YELLOW"/>
                <ITEXT FONTSIZE="12" CH=", "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="CYAN"/>
                <ITEXT FONTSIZE="12" CH=", "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="MAGENTA"/>
                <ITEXT FONTSIZE="12" CH=", "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="SILVER"/>
                <ITEXT FONTSIZE="12" CH=", "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="GRAY"/>
                <ITEXT FONTSIZE="12" CH=", "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="MAROON"/>
                <ITEXT FONTSIZE="12" CH=", "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="OLIVE"/>
                <ITEXT FONTSIZE="12" CH=", "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="GREEN"/>
                <ITEXT FONTSIZE="12" CH=", "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="PURPLE"/>
                <ITEXT FONTSIZE="12" CH=", "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="TEAL"/>
                <ITEXT FONTSIZE="12" CH=", "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="NAVY"/>
                <ITEXT FONTSIZE="12" CH=". "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Alternativ hat man die Möglichkeit, die Farbe anhand des Hexadezimalwerts anzugeben. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="RGB-Farben werden definiert durch einen Wert für rot (=r), einen für grün (=g) und einen für blau (=b). Dieser Wert darf zwischen 0 und 255 liegen. Für die Farbe rot muss also für r=255, sprich höchster Wert, für g=0, und b=0 gesetzt werden. Anders geschrieben: (255, 0, 0)."/>
                <para PARENT="erklärungen"/>
                <ITEXT FONTSIZE="12" CH="Dies ist der RGB-Wert als Tupel dargestellt."/>
                <para PARENT="erklärungen"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Der Hexadezimalwert wäre für rot &quot;#FF0000&quot;, bzw. 0xFF0000, wobei die Zeichen &quot;#&quot; oder &quot;0x&quot; nur vorangestellt werden, um zu zeigen, dass es sich hierbei um einen Hexadezimalwert handelt. "/>
                <ITEXT CH="Da &quot;#FF0000&quot; mit der '#' als initiales Zeichen, eine Zeichenkette darstellt, 0xff0000 hingegen einen (hexadezimalen) Zahlenwert und sich mit Zahlen besser rechnen lässt, verwenden wir hier die Darstellung 0xff0000."/>
                <para PARENT="erklärungen"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Wandeln wir die Dezimalzahl &quot;255&quot; in eine Hexadezimalzahl erhalten wir &quot;FF&quot;. Der Hexadezimalwert einer Farbe ist also nichts weiter, als die jeweiligen Farbwerte für r, g und b, einzeln konvertiert in eine Hexadezimalzahl und anschließend aneinandergereiht. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Die Farbe &quot;aquamarin&quot; hat zum Beispiel den RGB-Wert (127,255,212). Konvertieren wir die einzelnen Werte in Hexadezimalwerte erhalten wir 0x7FFFD4 (also 127 = 0x7F, 255 = 0xFF, 212 = 0xD4. Konvertieren wir wiederum den kompletten Hexadezimalwert in einen Dezimalwert erhalten wir 8388564. Beide Werte sind hier als Farbangabe zulässig. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die Konvertierung von Dezimal- in Hexadezimalwerte übernimmt Python für uns. Hierfür gibt es Funktionen, die bestimmte Werte in Werte eines anderen Typs (wie zum Beispiel Dezimalzahl in Hexadezimalzahl: hex(decimal_value) und andersrum: int(hex_value) konvertiert. Dies kann gleich in REPL getestet werden: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; int(0xff0000)"/>
                <para PARENT="code"/>
                <ITEXT CH="16711680"/>
                <para PARENT="code"/>
                <ITEXT CH="&gt;&gt;&gt; int(0xff)"/>
                <para PARENT="code"/>
                <ITEXT CH="255"/>
                <para PARENT="code"/>
                <ITEXT CH="&gt;&gt;&gt; hex(128)"/>
                <para PARENT="code"/>
                <ITEXT CH="'0x80'"/>
                <para PARENT="code"/>
                <ITEXT CH="&gt;&gt;&gt;"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Möchten wir jedoch ein RGB-Tupel, also hier (127, 255, 212) als Angabe verwenden, müssen wir dieses erst in den entsprechenden Hexadezimalwert konvertieren."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Wollen wir die Farbe Schwarz, was im additiven Farbsystem, also dem Farbsystem unseres sichtbaren Lichts soviel wie &quot;kein Licht&quot; bedeutet, müssen wir einfach alle Farben auf 0 setzen. Dies geht indem man für color den Wert &quot;0x00&quot; angibt. also 0."/>
                <para/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Micropython Neopixel: Farben" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="179" YPOS="24783.72" OwnPage="28" ItemID="1906648273" PTYPE="4" WIDTH="445.48" HEIGHT="326.28000000001" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L445.48 0 L445.48 326.28 L0 326.28 L0 0 Z" copath="M0 0 L445.48 0 L445.48 326.28 L0 326.28 L0 0 Z" gXpos="179" gYpos="24783.72" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Animationen Teil 1: Blinken"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para/>
                <ITEXT CH="Bevor die LEDs allerdings blinken können, muss man sich Gedanken darüber machen, was hier eigentlich genau geschehen soll: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die LEDs werden alle zur gleichen Zeit eingeschalten. Sie leuchten eine Weile, anschließend werden sie ausgeschalten. Nach einer Weile gehen sie wieder an, und so weiter. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wenn die LEDs also zweimal blinken sollen, könnte ich einfach schreiben: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="mach alle an – warte – mach alle aus – warte – mach alle an – warte – mach alle aus. "/>
                <para PARENT="Default Paragraph Style" ALIGN="1"/>
                <para PARENT="Default Paragraph Style" ALIGN="1"/>
                <ITEXT CH="Nur wäre diese Herangehensweise auf Dauer sehr umständlich. Wollten wir die LEDs auf diese Weise 1000x blinken lassen, hätten wir mittels copy + paste einiges zu tun! Außerdem würde das Programm viel zu groß und unübersichtlich werden! "/>
                <para PARENT="Default Paragraph Style" ALIGN="0"/>
                <ITEXT CH="Viel besser wäre es doch wenn wir sagen könnten:"/>
                <para/>
                <para/>
                <ITEXT CH=" mach bitte 5x: "/>
                <para ALIGN="1"/>
                <ITEXT CH="mach alle an – warte – mach alle aus – warte. "/>
                <trail PARENT="Default Paragraph Style" ALIGN="1"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Animationen Teil 1: Blinken" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="25128.0558984007" OwnPage="28" ItemID="1901379985" PTYPE="4" WIDTH="453.68" HEIGHT="370.054101599289" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 370.054 L0 370.054 L0 0 Z" copath="M0 0 L453.68 0 L453.68 370.054 L0 370.054 L0 0 Z" gXpos="170.8" gYpos="25128.0558984007" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="12" CH="Schleifen"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Aus diesem Grund gibt es Schleifen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Mit Schleifen kann man zum Beispiel festlegen, dass bestimmte Codezeilen genau 5, 10, 1000 Mal ausgeführt werden sollen ("/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="for-Schleife"/>
                <ITEXT FONTSIZE="12" CH="). Außerdem kann man auch sagen, dass ein Code-Block, wie bei uns das Blinken, nur ausgeführt werden soll, während es draußen dunkel ist ("/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="while-Schleife"/>
                <ITEXT FONTSIZE="12" CH=")."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Zum Beispiel: während es draußen dunkel ist: blinke, checke obs immernoch dunkel ist. wenn dunkel: nochmal das ganze! Wenn hell, abbrechen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Auch könnte man mit einer while-Schleife sagen: mache das einfach immer wieder! Eine sogenannte Endlos-Schleife. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Grundlegend braucht eine Schleife für jeden Schleifendurchlauf eine Bedingung, deren Wahrheitsgehalt geprüft werden muss. Trifft die Bedingung zu, wird von dieser Bedingung der Boolsche Wert True zurück gegeben, was bedeutet, die Schleifenbedingung trifft zu und sie wird ein weiteres mal ausgeführt. Trifft die Bedingung nicht zu, wird der Wert False zurück gegeben. Die Schleifenbedingung trifft nicht zu und die Ausführung des Codes geht in der ersten Zeile nach der Schleife weiter. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Schleifen" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="25665.61" OwnPage="29" ItemID="1907810465" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="-564.48" gYpos="16770.1389746002" gWidth="475.28" gHeight="672.176923800567" LAYER="0" NEXTITEM="1880406961" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="12" CH="For-Schleife"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Überschrift - normal-fett"/>
                <ITEXT CH="https://www.python-kurs.eu/python3_for-schleife.php"/>
                <para PARENT="links"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Um zu sagen: führe diesen Codeblock 5x aus schreibt man in Python: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="10" CH="&gt;&gt;&gt; for i in range(5): "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="range(5) ist eine Funktion, die  nacheinander alle ganzen Zahlen von 0 bis 4(!!!) ausgibt. Genauer sollte man eigentlich schreiben: range(0, 5). Warum 4 und nicht 5? 5 gibt hier das obere Limit der Zahlenfolge, sowie die Anzahl der Zahlen an. Also bedeutet range(5) (bzw range(0,5)), dass eine Zahlenreihe, bestehend aus 5 ganzen Zahlen, beginnend bei 0, endend bei &lt; 5 - also 4, zurückgegeben wird. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Bedeutet wiederum: dieser Codeblock wird 5x ausgeführt. Für jedes i von 0 bis &lt; 5. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Aber -  was befindet sich in diesem Codeblock - was außerhalb?"/>
                <breakline/>
                <ITEXT FONTSIZE="12" CH="Dies wird in Python durch Einrückungen definiert."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Sprich alles, was hier 5x ausgeführt werden soll, muss nun im Vergleich Zeilenanfang der for-Schleife einmal mit Tab eingerückt sein."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="range(5) ist eine Funktion, die  nacheinander alle ganzen Zahlen von 0 bis 4(!!!) ausgibt. Genauer sollte man eigentlich schreiben: range(0, 5). Warum 4 und nicht 5? 5 gibt hier das obere Limit der Zahlenfolge, sowie die Anzahl der Zahlen an. Also bedeutet range(5) (bzw range(0,5)), dass eine Zahlenreihe, bestehend aus 5 ganzen Zahlen, beginnend bei 0, endend bei &lt; 5 - also 4, zurückgegeben wird. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Bedeutet wiederum: dieser Codeblock wird 5x ausgeführt. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Ein mal für jedes i von 0 bis &lt; 5. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Aber -  was befindet sich in diesem Codeblock, der nun fünf mal ausgeführt werden soll - und was außerhalb?"/>
                <breakline/>
                <ITEXT FONTSIZE="12" CH="Dies wird in Python durch Einrückungen definiert."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Sprich alles, was hier 5x ausgeführt werden soll, muss nun im Vergleich Zeilenanfang der for-Schleife einmal mit TAB eingerückt werden."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="REPL erledigt das für uns, sobald man die Zeile &quot;for i in range(5):&quot; mit einem Doppelpunkt beendet und einmal Enter gedrückt hat. Der Cursor ist nun eingerückt, zudem hat sich wieder der Zeilenanfang verändert. Hier stehen nun &quot;"/>
                <ITEXT FONT="DejaVu Sans Mono Bold" FONTSIZE="11.8" CH="..."/>
                <ITEXT FONTSIZE="11.8" CH="&quot;:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="Auch wenn wir mit Enter in die nächste Zeile wechseln, bleibt dieser noch auf selber Position. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="Um die For-Schleife mit der Range-Funktion mal zu demonstrieren, sagen wir: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="für jedes i von 0 bis &lt;5 : "/>
                <para PARENT="Default Paragraph Style" ALIGN="1"/>
                <ITEXT FONTSIZE="11.8" CH="gib einmal den Wert von i aus. "/>
                <para PARENT="Default Paragraph Style" ALIGN="1"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Um Werte (oder Texte) im Terminal auszugeben nutzt man die Funktion print([variable / Text (in Anführungszeichen)])"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="Zweimaliges Drücken der Enter-Taste lässt den Cursor wieder an den Anfang der Zeile stellen, sprich, falls hier noch etwas gemacht werden sollte, dann wird das erst ausgeführt, sobald die Schleife verlassen, also 5x ausgeführt wurde. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="Da wir nach der Schleife nichts weiter ausführen wollen, können wir nochmals mit Enter bestätigen, sodass die Schleife ausgeführt werden kann. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="Dies ging nun allerdings sehr sehr schnell! Wenn wir in diesem Tempo unsere LEDs blinken lassen würden, würden wir das Blinken sehr wahrscheinlich überhaupt nicht mehr wirklich wahrnehmen! "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="Um diesen Prozess zu verlangsamen, müssen wir sogenannte Delays einbauen. "/>
                <breakline/>
                <ITEXT FONTSIZE="11.8" CH="Dies erledigt man am einfachsten mit dem Modul &quot;"/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="11.8" CH="utime"/>
                <ITEXT FONTSIZE="11.8" CH="&quot;"/>
                <ITEXT FONTSIZE="12" CH=", bzw. dessen Funktion "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="sleep_ms()"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Mono Bold" FONTSIZE="11.8" CH="utime.sleep_ms(200)"/>
                <ITEXT FONTSIZE="11.8" CH=" sorgt dafür, dass die nächste Codezeile (oder das nächste Mal, dass die Schleifenbedingung geprüft wird) erst 200 ms später stattfindet. Wenn nun folgende Zeilen ins Terminal eingegeben und - wie eben schonmal - 3x mit Enter bestätigt werden"/>
                <ITEXT CH=" ... "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="10" CH="&gt;&gt;&gt; import utime"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="&gt;&gt;&gt; for i in range(5):"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="... "/>
                <tab FONTSIZE="10"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="print(i)"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="... "/>
                <tab FONTSIZE="10"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="utime.sleep_ms(200)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="... erscheinen nun die Ergebnisse der einzelnen Schleifendurchläufe in zeitlichen Abständen auf dem Terminal. Selbes Verhalten wollen wir für unsere blinkenden LEDs! "/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
