            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Das meiste des hier gezeigten HTML-Codes ist lediglich für die Darstellung der Objekte und die Animationen im Hintergrund zuständig.  "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Für uns sind lediglich folgende Zeilen wichtig, in denen ein Dropdownmenü zur Auswahl der Animationen erstellt wird: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="10" CH="&lt;form class=&quot;form-group&quot; method=&quot;POST&quot; action=&quot;/&quot; role=&quot;form&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="   &lt;h3 class=&quot;mb-3&quot; id=&quot;id1&quot;&gt;LED Pattern ausw&amp;auml;hlen:&lt;/h3&gt;"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="   &lt;div class=&quot;input-group&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="      &lt;select class=&quot;custom-select&quot; id=&quot;inputGroupSelect04&quot; name=&quot;light&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="         &lt;option selected&gt;Animation ausw&amp;auml;hlen ...&lt;/option&gt;"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="         &lt;option value=&quot;myValue&quot;&gt;MyName&lt;/option&gt;"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="      &lt;/select&gt;"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="      &lt;div class=&quot;input-group-append&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="         &lt;button class=&quot;btn btn-dark&quot; type=&quot;submit&quot;&gt;ausw&amp;auml;hlen&lt;/button&gt;"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="      &lt;/div&gt;"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="   &lt;/div&gt;"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="&lt;/form&gt;"/>
                <para PARENT="code"/>
                <tab/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Eine HTML-Form erfüllt den Zweck, User-Eingaben entgegenzunehmen und an den Server zur Weiterverarbeitung zu senden. Dazu muss mit &quot;method&quot; angegeben werden, mit welcher Methode (hier: Post) gearbeitet wird und mit &quot;action&quot;, welcher Route-Handler verwendet werden soll. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die Form beginnt mit dem offnenden HTML-Tag: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH="&lt;form class=&quot;form-group&quot; method=&quot;POST&quot; action=&quot;/&quot;&gt; "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="und endet mit dem schließenden Tag:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH="&lt;/form&gt;"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Direkt unter dem öffnenden form-Tag befindet sich das h3-Tag. Hier wird eine Überschrift angegeben. Dies ist allerdings optional. Die Formatierung von h3 wird in der Datei style.css festgelegt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="10" CH="&lt;h3 class=&quot;mb-3&quot; id=&quot;id1&quot;&gt;LED Pattern ausw&amp;auml;hlen:&lt;/h3&gt;"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="h3 steht für &quot;header 3&quot; - also Überschrift Nr. 3. Wobei mit der Numerierung die Rangfolge angegeben wird. h1 ist in der Regel sehr groß. Die Größe ändert sich mit der Rangfolge. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um die Daten an den Server zu senden, wird ein Button verwendet: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="10" CH="&lt;div class=&quot;input-group-append&quot;&gt;"/>
                <para PARENT="code"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="&lt;button class=&quot;btn btn-dark&quot; type=&quot;submit&quot;  &gt;ausw&amp;auml;hlen&lt;/button&gt;"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="&lt;/div&gt;"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Welche Werte an den Server gesendet werden, werden durch die Optionen "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="name"/>
                <ITEXT CH=" und "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="value"/>
                <ITEXT CH=" definiert. Diese werden dann als Dictionary { name:value} an den Server übergeben"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="10" CH="&lt;select class=&quot;custom-select&quot; id=&quot;inputGroupSelect04&quot; name=&quot;light&quot;&gt;"/>
                <para PARENT="code"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="&lt;option selected&gt;Animation ausw&amp;auml;hlen ...&lt;/option&gt;"/>
                <para PARENT="code"/>
                <tab FONTSIZE="10"/>
                <ITEXT FONTSIZE="10" CH="&lt;option value=&quot;animationName&quot;&gt;MyName&lt;/option&gt;"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="10" CH="&lt;/select&gt;"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="So wird ein Dropdown-Menü erstellt. Im öffnenden select-Tag wird der name (light) des zu übergebenden Werts bestimmt. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Innerhalb des select-Tags werden die options angegeben, also die Werte, die dann mittels dem Dropdown-Menüs ausgewählt werden können. Hier wird auch der zu übergebede value definiert. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun müssen wir nur noch unsere Animationen einfügen."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Für jede unserer Animationen müssen wir nun zwischen &lt;option selected&gt; und &lt;/select&gt; einen eigenen option value eintragen: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="10" CH="&lt;option value=&quot;running_dot&quot;&gt;Running Dot&lt;/option&gt;"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="value=&quot;running_dot&quot;"/>
                <ITEXT CH="  ist der Wert, der nach dem Absenden vom Server weiter verarbeitet wird. Ihn werden wir nutzen, um die richtige Animation zu starten. Hier ist es wichtig, dass der Eintrag in value derselbe ist, den wir im Dictionary animation_dict in animations.py angegeben haben! andernfalls werden wir die Animation nicht starten können. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Running Dot"/>
                <ITEXT CH=" - was vor dem schließenden"/>
                <ITEXT FONT="DejaVu Sans Bold" CH=" &lt;/option&gt;"/>
                <ITEXT CH="-Tag steht ist lediglich der Text, der im Dropdown Menü angezeigt wird.. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die &quot;Hieroglyphen&quot; "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="&amp;auml;"/>
                <ITEXT CH=" werden zur Darstellung von Umlauten benötigt, da nicht jeder Browser Umlaute in Form von 'ä', 'ö', 'ü' darstellen kann. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hier nutzt man stattdessen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH=" &amp;auml;"/>
                <ITEXT CH=" (für ä), "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="&amp;ouml;"/>
                <ITEXT CH=" (für ö) und "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="&amp;uuml;"/>
                <ITEXT CH=" (für ü)."/>
                <para PARENT="Default Paragraph Style"/>
                <para/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="HTML-Form" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="66232.55" OwnPage="75" ItemID="1956261857" PTYPE="4" WIDTH="453.68" HEIGHT="490.450000000026" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 490.45 L0 490.45 L0 0 Z" copath="M0 0 L453.68 0 L453.68 490.45 L0 490.45 L0 0 Z" gXpos="170.8" gYpos="66232.55" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1963545521"/>
        <PAGEOBJECT XPOS="170.8" YPOS="67114.44" OwnPage="76" ItemID="1956265137" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="67114.44" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1969575905" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Eingaben auf dem Server verarbeiten "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun wissen wir, dass für die erste Darstellung der Seite die GET-Methode, für das Absenden der Formulardaten die POST_Methode zum Einsatz kommen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Das bedeutet wir brauchen zwei Rout-Handler-Methoden in webSrv.py."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Der Route-Handler für GET muss also vor der Definition folgende Zeile enthalten: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="@MicroWebSrv.route('/')"/>
                <para PARENT="code"/>
                <ITEXT CH=" "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Der Route-Handler für POST wird mit folgender Zeile eingeleitet:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="@MicroWebSrv.route('/', 'POST') "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="für jeden Request brauchen wir eine eigene Funktion, die entweder nur die Seite als Antwort sendet, oder die Eingabedaten weiter verarbeitet bevor die Seite als Antwort gesendet wird. Leider können wir - obwohl beide Funktionen teilweise selben Inhalt haben, diese beiden nicht kombinieren. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Für den GET-Request reicht es also aus, die Seite als Antwort zu senden: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="@MicroWebSrv.route('/')"/>
                <para PARENT="code"/>
                <ITEXT CH="def _routeHandlerGet(httpClient, httpResponse): "/>
                <para PARENT="code"/>
                <tab/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="httpResponse.WriteResponseFile(filepath = 'www/index.html', "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="contentType= &quot;text/html&quot;, "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="headers = None)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Beim POST-Request muss der Server zusätzlich die eingehenden Daten abgerufen werden."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Ein Blick in die Modulbeschreibung verrät uns, wie wir an die FormData herankommen können: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="@MicroWebSrv.route('/', 'POST')"/>
                <para PARENT="code"/>
                <ITEXT CH="def _routeHandlerGet(httpClient, httpResponse): "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="formData = httpClient.ReadRequestPostedFormData()"/>
                <para PARENT="code"/>
                <tab/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="httpResponse.WriteResponseFile(filepath = 'www/index.html', "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="contentType= &quot;text/html&quot;, "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="headers = None)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun müssen wir diese empfangene Daten an main.py senden."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wie bereits erwähnt, läuft der WebServer in einem eigenen Thread. So wie auch der MainThread ein eigener Thread ist. Auch unsere Animatione laufen in einem eigenen Thread. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wir könnten jetzt - wie bei animations.set_brightness auch sagen: wir schreiben eine Funktion namens get_values in webSrv.py, die wir von main.py aus aufrufen."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies würde uns allerdings immense Probleme bereiten, wenn main.py auf die Variable zugreift, während sie zur gleichen Zeit von webSrv.py geändert wird (und dies kommt häufiger vor als man denkt!)"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Daher ist hier der Einsatz von Thread-Messages die sicherere Wahl. Sobald neue Daten vorliegen, kriegt main.py eine Benachrichtigung und holt sie dann erst ab:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="if &quot;light&quot; in formData:"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="_thread.sendmsg(_thread.getReplID(), &quot;light:{}&quot;.format(formData[&quot;light&quot;]))"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH=" "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="if &quot;light&quot; in formData"/>
                <ITEXT CH=" prüft, ob die Zeichenfolge &quot;light&quot; in formData zu finden ist, um fehlerhafte Verarbeitung auszuschließen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="_thread.sendmsg( thread_id, message)"/>
                <ITEXT CH=" sendet eine Nachricht als Zeichenfolge an den Thread, der mit thread_id angegeben wurde. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="_thread.getReplID"/>
                <ITEXT CH=" gibt die ID des MainThreads zurück. Achtung! Import des _thread-Moduls nicht vergessen!"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="&quot;light:{}&quot;.format(value)"/>
                <ITEXT CH=" konvertiert den Wert in value in einen String und setzt ihn in den Platzhalter {} ein. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Der Doppelpunkt hinter &quot;light&quot; wird später bei der Verarbeitung in main.py noch wichtig sein! Ebenso ist wichtig, dass sich weder vor noch nach dem Doppelpunkt leerzeichen befinden! "/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Eingaben auf dem Server verarbeiten" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="68344.9910253998" OwnPage="77" ItemID="1967844417" PTYPE="4" WIDTH="453.68" HEIGHT="365.728974600162" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 365.729 L0 365.729 L0 0 Z" copath="M0 0 L453.68 0 L453.68 365.729 L0 365.729 L0 0 Z" gXpos="766.08" gYpos="67463.1010253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1970951249" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Thread-Messages vom Server in main.py empfangen und verarbeiten"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um im MainThread überhaupt Messages empfangen zu können, müssen wir dies in main.py zuerst explizit mit "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="_thread.ReplAcceptMsg(True)"/>
                <ITEXT CH=" erlauben!"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="msg = _thread.getmsg()"/>
                <ITEXT CH=" speichert - wenn vorhanden - empfangene Nachrichten in msg. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="msg enthält dann ein Tupel (message_type, sender_id, message), wobei "/>
                <ITEXT FONT="DejaVu Sans Bold" CH="message_type"/>
                <ITEXT CH="  0 (=none), 1 (=integer) oder 2 (=String) sein kann. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="sender_id"/>
                <ITEXT CH=" ist die Thread-ID des Senders (wobei hier nur der Server infrage kommt) "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="message"/>
                <ITEXT CH=" ist die Nachricht, die wir vom Server aus geschickt haben, also &quot;light:formData[&quot;light&quot;]."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um an den Wert von formData[&quot;light&quot;] zu kommen, müssen wir also msg[2] an der Stelle &quot;:&quot; splitten."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="values = msg[2].split(&quot;:&quot;) "/>
                <ITEXT CH="erledigt das für uns, sodass values dann aus (&quot;light&quot;, formData[&quot;light&quot;] besteht. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Doch sollten wir - bevor wir mit dem Wert in formData[&quot;light&quot;] die Animation starten noch prüfen, ob wir mit dem Wert wirklich die Animationen steuern. Jedoch ist hier vorsicht geboten: viel zu oft kommt es aus Gewohnheit vor, dass sich vor oder nach dem Doppelpunkt leerzeichen einschleichen. Diese kann man aber einfach abtrennen."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="values[0].strip() entfernt Leerzeichen, die zu Beginn oder am Ende des Strings vorkommen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die Stringverarbeitung in Python ist ein sehr komplexes Thema, weshalb ich mich hier auf die Beschreibung der notwendigsten Funktionen beschränke. Weitere Infos kann man zum Beispiel hier finden:"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="https://docs.python.org/2/library/string.html"/>
                <para PARENT="links"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Auch hier sollten wir vor dem Start einer neuen Animation prüfen, ob bereits eine Animation im Thread läuft. Außerdem ist es zwingend notwendig, vor Start des neuen Threads dem WebServer einige Zeit (2000 ms) zu geben, dass dieser die Response-Seite dem Client (also unserem Computer) zur Verfügung stellen kann, andernfalls führt dies zu kritischen Fehlern. Das bedeutet, das System stürzt ab und startet neu - die Animation kann also nicht gestartet werden"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zusammengefasst sieht das dann so aus: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="_thread.ReplAcceptMsg(True)"/>
                <para PARENT="code"/>
                <ITEXT CH="wait_before_start_thread = 2000"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="# ..."/>
                <para PARENT="Kommentare - Zeilen"/>
                <para PARENT="code"/>
                <ITEXT CH="while True:"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="# ..."/>
                <para PARENT="Kommentare - Zeilen"/>
                <tab/>
                <ITEXT CH="msg = _thread.getmsg()"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="if msg[0] == 2: "/>
                <tab/>
                <ITEXT CH=" "/>
                <ITEXT FCOLOR="middle grey" FSHADE="100" CH="#true if msg is a String"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="values = msg[2].split(&quot;:&quot;)"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="if values[0].strip() == &quot;light&quot;:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="if _thread.status(animation_thread) != _thread.TERMINATED:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="_thread.notify(animation_thread, _thread.EXIT)"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(wait_before_start_thread)"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <tab/>
                <ITEXT CH="_thread.start_new_thread(&quot;animation&quot;, animations.start, (values[1].strip(), ))"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Thread-Messages vom Server in main empfangen und verarbeiten" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="67996.33" OwnPage="77" ItemID="1969575905" PTYPE="4" WIDTH="453.68" HEIGHT="336.920000000027" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 336.92 L0 336.92 L0 0 Z" copath="M0 0 L453.68 0 L453.68 336.92 L0 336.92 L0 0 Z" gXpos="170.8" gYpos="67996.33" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1956265137"/>
        <PAGEOBJECT XPOS="170.8" YPOS="69365.8810253998" OwnPage="78" ItemID="1965993201" PTYPE="4" WIDTH="453.68" HEIGHT="236.53897460016" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 236.539 L0 236.539 L0 0 Z" copath="M0 0 L453.68 0 L453.68 236.539 L0 236.539 L0 0 Z" gXpos="170.8" gYpos="69365.8810253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Animationen mit einem String starten"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
