        <PAGEOBJECT XPOS="735.28" YPOS="14522.5775" OwnPage="32" ItemID="1670996681" PTYPE="2" WIDTH="475.28" HEIGHT="62.8125" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" TEXTFLOWMODE="2" LOCALSCX="0.75" LOCALSCY="0.75" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" Pagenumber="0" PFILE="../../../../Schreibtisch/pics-projekt/groß/boot-py.png" IRENDER="0" EMBEDDED="0" path="M0 0 L475.28 0 L475.28 62.8125 L0 62.8125 L0 0 Z" copath="M0 0 L475.28 0 L475.28 62.8125 L0 62.8125 L0 0 Z" ImageRes="0" gXpos="735.28" gYpos="14522.5775" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="735.28" YPOS="12893.7025" OwnPage="28" ItemID="1671223129" PTYPE="2" WIDTH="475.28" HEIGHT="201.505449200376" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="0.75" LOCALSCY="0.75" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" Pagenumber="0" PFILE="../../../../Schreibtisch/pics-projekt/groß/atom-start.png" IRENDER="0" EMBEDDED="0" path="M0 0 L475.28 0 L475.28 201.505 L0 201.505 L0 0 Z" copath="M0 0 L475.28 0 L475.28 201.505 L0 201.505 L0 0 Z" ImageRes="0" gXpos="735.28" gYpos="12893.7025" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="180" YPOS="15394.3425" OwnPage="33" ItemID="1671387785" PTYPE="2" WIDTH="475.28" HEIGHT="196.787499999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" TEXTFLOWMODE="2" LOCALSCX="0.75" LOCALSCY="0.75" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" Pagenumber="0" PFILE="../../../../Schreibtisch/pics-projekt/atom-split.png" IRENDER="0" EMBEDDED="0" path="M0 0 L475.28 0 L475.28 196.787 L0 196.787 L0 0 Z" copath="M0 0 L475.28 0 L475.28 196.787 L0 196.787 L0 0 Z" ImageRes="0" gXpos="180" gYpos="15394.3425" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="180" YPOS="16859.4810253998" OwnPage="37" ItemID="1670990681" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800575" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="180" gYpos="16859.4810253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="running dots mit Schweif - white level"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wollen wir nun erreichen, dass nicht nur eine, sondern 3 aneinandergereihte LEDs leuchten, diese aber mit jeder LED an Farbintensität abnimmt, müssen wir das white-level einsetzen (steuerbar mit dem optionalen Parameter &quot;white&quot; - in strip.set() beschrieben). "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die zweite LED, die also etwas weniger farbintensiv ist, können wir ganz einfach in der Schleife mit pos=i-1 ansprechen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="strip.set(i-1, color, white=100) "/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="würde also die LED ansprechen, die sich direkt hinter der aktuell leuchtenden befindet."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="strip.set(i-2, color, white = 150) "/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="würde dann die LED ansprechen, die sich zwei Positionen dahinter befindet! "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Aber: im Fall i=1 wäre i-2 = -1. LEDs, die mit negativen Werten angesprochen werden, gibt es nicht! Auch würde dies beim Ausführungsversuch zu Fehlermeldungen führen (Index Error - List Index out of range)! "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Daher müssen wir garantieren, dass der Wert nie kleiner als 1 und auch nie größer als num_pixels ist. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Würde der Index des Strips von 0 bis "/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH="num_pixels"/>
                <ITEXT CH="-1 gehen, könnte man dies ganz einfach mit dem Modulo-Operator (einfache Restdivision, die als Ergebnis den Restwert zurückgibt) lösen. Da dies aber nicht der Fall ist, können wir so lediglich ausschließen, dass der Wert für i nie größer wird als "/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH="num_pixels"/>
                <ITEXT CH=" +1 : "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="pos1 = (i -1) % "/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH="num_pixels"/>
                <ITEXT CH=" "/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="Da bei der Modulodivison - genau wie bei der normalen Division die Regel &quot;Punkt vor Strich&quot; gilt, muss der Ausdruck (i - 1) zwingend in Klammern. Somit haben wir garantiert, dass pos1 immer kleiner bleibt als "/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH="num_pixels"/>
                <ITEXT CH=", da zum Beispiel 6 % 5 =1 (1 Rest 1).  "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Aber 5 % 5 = 1 Rest 0. Wenn wir Pixel 0 in cyan leuchten lassen, im nächsten Schritt aber Pixel 1, dann würde jeweils die selbe LED leuchten. Dafür würde aber die letzte LED (an Stelle "/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH="num_pixels"/>
                <ITEXT CH=" ausbleiben. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Also müssen wir jedes Mal, wenn der Wert für pos1 = 0 ist, diesen auf "/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH="num_pixels"/>
                <ITEXT CH=" setzen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hierfür nutzt man in der Programmierung sogenannte Verzweigungen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="##" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.28" YPOS="15950.5910253998" OwnPage="36" ItemID="1674563689" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800565" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="735.28" gYpos="15950.5910253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Kommentare in (Micro)Python "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Du möchtest nun, dass lediglich die Animation running_dots() ausgeführt wird, nicht aber die Blink-Animation? Dann kannst du selbstverständlich die entsprechende Zeile in main.py löschen - oder einfach dafür sorgen, dass diese Zeile &quot;überlesen&quot; wird."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dies kann man durch Kommentare erreichen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Üblicherweise werden Kommentare eingesetzt um zu beschreiben, was im nachfolgenden Codeblock geschieht. Aber man kann diese während der Entwicklung ebenso einsetzen, um Codezeilen vorübergehend nicht ausgeführt werden. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Aus Performancegründen sollten diese Kommentare allerdings gelöscht werden, sobald das Programm fertig ist."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Man unterscheidet zwischen zwei verschiedenen Arten: dem Zeilenkommentar und dem Blockkommentar. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zeilenkommentare"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zeilenkommentare gehen - wie der Name schon sagt - nur über eine Zeile und werden gekennzeichnet durch eine # zu Beginn der Zeile / des Kommentars. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Man kann Zeilenkommentare auf die komplette Zeile anwenden, oder eben nur auf einen Teil der Zeile. Alles, was sich hinter der # befindet, wird also vom Interpreter übersprungen und nicht ausgeführt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="# Dies ist ein Zeilenkommentar. "/>
                <para PARENT="Kommentare - Zeilen"/>
                <para PARENT="Kommentare - Zeilen"/>
                <ITEXT CH="i = x % 5 "/>
                <ITEXT FCOLOR="middle grey" FSHADE="100" CH="# hier kann man kurz beschreiben, was passiert"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Blockkommentare / Docstrings"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Blockkommentare können über mehrere Zeilen gehen. Dazu müssen zu Beginn und am Ende des Blockkommentars drei Anführungszeichen in Folge stehen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Kommentare - Block"/>
                <ITEXT CH="&quot;&quot;&quot; Dies ist ein Blockkommentar. "/>
                <para PARENT="Kommentare - Block"/>
                <ITEXT CH="Dieser darf auch mehrzeilig sein und wird auch häufig eingesetzt, um Module, bzw. Funktionen zu beschreiben. &quot;&quot;&quot;"/>
                <para PARENT="Kommentare - Block"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <para/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Kommentare in (Micro)Python" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.28" YPOS="18596.2610253998" OwnPage="42" ItemID="1675507641" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800565" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="735.28" gYpos="18596.2610253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1682335945" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Bedingte Anweisungen / Verzweigungen"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="In der Programmierung versteht man unter einer bedingten Anwesung (oder Verzweigung) Codeteile, die nur unter bestimmten Bedingungen ausgeführt werden. Liegt diese Bedingung nicht vor, werden diese nicht ausgeführt. Alternativ kann (aber muss nicht) statt dessen ein anderer Codeteil ausgeführt werden. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Anders ausgedrückt: Eine Verzweigung legt fest, welcher von zwei (oder auch mehr) Programmteilen (Alternativen) in Abhängigkeit von einer (oder mehreren) Bedingungen ausgeführt wird. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="So könnte man ganz einfach sagen: "/>
                <para PARENT="erklärungen"/>
                <tab/>
                <ITEXT CH="Wenn dies: mache das. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Dazu könnte man noch definieren: "/>
                <tab/>
                <para PARENT="erklärungen"/>
                <tab/>
                <ITEXT CH="Wenn dies nicht: mache was anderes. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Oder wenn man meherere Bedingungen in Frage kommen könnten:"/>
                <para PARENT="erklärungen"/>
                <tab/>
                <ITEXT CH="Wenn dies:   mache das"/>
                <para PARENT="erklärungen"/>
                <tab/>
                <ITEXT CH="Wenn dies nicht sondern das:  mache jenes"/>
                <para PARENT="erklärungen"/>
                <tab/>
                <ITEXT CH="wenn dies und das nicht:  mache was anderes. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Hierzu nutzt man die if-Anweisung. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Nehmen wir an, wir haben den derzeitigen Wochentag in der Variable &quot;weekday&quot; gespeichert. Wollen wir zum Beispiel erreichen, dass nur am Montag eine Anzeige im Terminal erscheint, die einen an diesen (meist ungeliebten) Tag erinnert schreiben wir: "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="if weekday == &quot;monday&quot;: "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="print(&quot;today is monday. &quot;) "/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="der Operator &quot;==&quot; ist ein sogenannte Gleichheitsoperator. Dieser prüft, ob die beinden Werte (davor und danach) gleich sind. Ist diese Bedingung erfüllt, wird True zurück geliefert, die Bedingung wird somit wahr und der nachfolgende Block darf ausgeführt werden. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Wollen wir, dass die Ausgabe nur erfolgt, wenn der Wochentag nicht der Montag ist, so können wir auch das Gegenteilige mit einem Gleichheitsoperator prüfen. Also ob eine Bedingung nicht stimmt. Hierzu nutzt man &quot;!=&quot; (=ungleich):"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="if weekday != &quot;monday&quot;:"/>
                <para PARENT="erklärungen"/>
                <tab/>
                <ITEXT CH="print(&quot;today's not monday&quot;)"/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Falls wir nur den Samstag oder Sonntag als Tag ausgeben, die restlichen Tage unter der Woche aber nur, dass kein Wochenende ist:"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="if weekday == &quot;saturday&quot;: "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="print(&quot;today is saturday&quot;) "/>
                <para PARENT="code"/>
                <ITEXT CH="elif weekday == &quot;sunday&quot;: "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="print(&quot;today is sunday&quot;)"/>
                <para PARENT="code"/>
                <ITEXT CH="else: "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="print(&quot;no weekend today&quot;) "/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="elif (Abkürzung von else if) wird nur ausgeführt, wenn die erste Bedingung (if) nicht zutrifft."/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Else wird nur ausgeführt, wenn sowohl if als auch elif nicht zutreffen. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Man kann beliebig oft verzweigen. "/>
                <trail PARENT="erklärungen"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Bedingte Anweisungen / Verzweigungen" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="180" YPOS="17888.1825367012" OwnPage="39" ItemID="1670478649" PTYPE="4" WIDTH="475.28" HEIGHT="225.48746329875" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 225.487 L0 225.487 L0 0 Z" copath="M0 0 L475.28 0 L475.28 225.487 L0 225.487 L0 0 Z" gXpos="0" gYpos="1055.7015113014" gWidth="475.28" gHeight="672.176923800575" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="12" CH="Der Touch Sensor"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Unser ESP32 hat praktischerweise einige Sensoren an Board, wie zum Beispiel den Touch Sensor (oder Berührungssensor). "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Genauer gesagt haben wir insgesamt 10 Touch Sensoren. Diese befinden sich an den Pins  0, 2, 4, 13, 12, 14, 15, 27, 33 und 32. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Genauer gesagt sind diese Touch-Sensoren sogenannte Kapazitive Näherungssensoren. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Die Funktion dieses Sensors beruht auf der Änderung des elektrischen Feldes in der Umgebung vor seiner Sensorelektrode (aktive Zone)."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Im Prinzip handelt es sich hierbei um eine Art Kondensator. Da sich die Kapazität eines Kondensators mit dem Abstand seiner Elektroden verändert, kann diese messbare Größe zur Erkennung von Berührung eingesetzt werden. "/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Der Touch Sensor" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="180" YPOS="17714.3710253998" OwnPage="39" ItemID="1677523017" PTYPE="4" WIDTH="475.28" HEIGHT="189.918974600187" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 189.919 L0 189.919 L0 0 Z" copath="M0 0 L475.28 0 L475.28 189.919 L0 189.919 L0 0 Z" gXpos="0" gYpos="881.889999999999" gWidth="475.28" gHeight="672.176923800575" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="20" CH="Animationen steuern"/>
                <para PARENT="Überschrift Mittel"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Nun haben wir einige Animationen gebastelt, zwischen denen wir im Idealfall auch wechseln wollen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Natürlich könnten wir jetzt sagen, wir lassen diese einfach nacheinander abspielen, sodass zum Beispiel Animation Nr. 1 für einige Zeit läuft, anschließend Animation Nr. 2"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Besser wäre es doch, wenn wir selbst bestimmen könnten, wann die nächste Animation laufen soll. "/>
                <trail PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Animationen steuern " Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="180" YPOS="18110.67" OwnPage="39" ItemID="1677810857" PTYPE="4" WIDTH="475.28" HEIGHT="275.877949200374" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 275.878 L0 275.878 L0 0 Z" copath="M0 0 L475.28 0 L475.28 275.878 L0 275.878 L0 0 Z" gXpos="0" gYpos="1278.1889746002" gWidth="475.28" gHeight="672.176923800575" LAYER="0" NEXTITEM="1680372777" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="12" CH="Wir wollen uns momentan nur auf den Touch Sensor konzentrieren und lassen den Teil mit den Animationen vorerst aus. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Alle Arten von Pins am ESP werden über das machine-Modul eingebunden. Wollen wir also den Touch Sensor an Pin 27 nutzen, schreiben wir: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="import machine"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="12" CH="touchPin = 27"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="12" CH="touch = machine.TouchPad(machine.Pin(touchPin))"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Somit haben wir eine Instanz des Touchpads in der Variable touch gespeichert."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Wenn wir nun den Wert des Touch Pins auslesen wollen, schreiben wir: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="touch.read() "/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Da es hin und wieder beim Auslesen zu Fehlern (ValueError) kommen kann und diese dazu führen, dass die Ausführung des kompletten Programms anhält (was nur durch Neustart behoben werden kann), müssen wir diesen Fehler abfangen."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Dieser Fehler tauchte bei meinen Tests anfangs überhaupt nicht auf, später dafür immer wieder. Falls dieser Fehler zu häufig auftreten sollte, empfiehlt es sich, einen anderen Pin als Touch-Sensor zu konfigurieren, weshalb ich die Pin-Nummer auch in eine separate Variable gespeichert habe. Dies wird, sobald das Programm eine gewisse Größe erreicht hat, zur Übersicht  und besseren Wartbarkeit beitragen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Um den Fehler abzufangen müssen wir dem Programm sagen, dass es versuchen soll, diesen Pin auszulesen. Falls dies nicht möglich sein sollte, kann (muss aber nicht) eine Ausgabe im Terminal geschehen."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Dies regelt man mit einer Aushanhmebehandlung (exception handling). "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Der Code, der das Risiko für eine Ausnahme beherbergt, wird in ein try-Block eingebettet. Abgefangen werden diese Ausnahmen im except-Block."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Wie bereits erwähnt wird die Ausführung des Programms linear vorgenommen. Sprich es wird Zeile für Zeile ausgeführt. Wenn eine Funktion (auch aus anderen Modulen) aufgerufen wird, geht die Ausführung des Programms - wieder Zeile für Zeile, in der aufgerufenen Funktion weiter, bis diese komplett ausgeführt wurde. Anschließend geht die Ausführung weiter in der Zeile nach der aufgerufenen Funktion. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Da wir den Wert des Sensors mehr als nur einmal auslesen wollen um die Werte zu vergleichen, müssen wir den Codeblock wieder in eine Endlosschleife packen. Es reicht, den Wert 5 mal pro Sekunde auszulesen, also nehmen wir einen sleep-Wert von 200 ms."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="import machine, utime"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="12" CH="touch_pin = 27"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="12" CH="touch = machine.TouchPad(machine.Pin(touch_pin))"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="12" CH="while True: "/>
                <para PARENT="code"/>
                <tab FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="try:"/>
                <para PARENT="code"/>
                <tab FONTSIZE="12"/>
                <tab FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="touch_val = touch.read()"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <tab FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="except ValueError:"/>
                <para PARENT="code"/>
                <tab FONTSIZE="12"/>
                <tab FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="print(&quot;ValueError while reading touch_pin&quot;)"/>
                <para PARENT="code"/>
                <tab FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="print(touch_val)"/>
                <para PARENT="code"/>
                <tab FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="utime.sleep_ms(200)"/>
                <tab FONTSIZE="12"/>
                <para PARENT="code"/>
                <para/>
                <ITEXT FONTSIZE="12" CH="Bevor wir aber den Touch Sensor testen können, müssen wir noch ein Kabel an den Touch Pin (IO27) stecken. Nehmt dazu am besten ein JumperWire (male - female), also eins, das man mit der einen Seite über den Pin stülpen kann und bei dem dann auf der anderen Seite eine Spitze herausragt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Main.py Speichern, Datei auf den ESP kopieren, Repl starten und Controller neustarten. Schon sehen wir im Terminal, welche Werte der Sensor hat. Ohne Berührung bewegt sich der Wert zwischen 400 und 500, mit Berührung grob zwischen 30 und 130. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Setzen wir also den Schwellwert, ab dem der Sensor als &quot;berührt&quot; gelten soll, auf 150. Falls es im weiteren Verlauf zu Schwierigkeiten kommen sollte, wenn der Code eine Berührung registriert hat obwohl keine stattgefunden hat, oder keine Reaktion erfolgen sollte obwhol der Sensor berührt wurde, kann man diesen Schritt einfach wiederholen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Wir wollen uns momentan nur auf den Touch Sensor konzentrieren und lassen den Teil mit den Animationen vorerst aus. "/>
                <para PARENT="Default Paragraph Style"/>
                <para/>
                <ITEXT FONTSIZE="12" CH="Bevor wir aber den Touch Sensor testen können, müssen wir noch ein Kabel an den Touch Pin (IO27) stecken. Nehmt dazu am besten ein JumperWire (male - female), also eins, das man mit der einen Seite über den Pin stülpen kann und bei dem dann auf der anderen Seite eine Spitze herausragt. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Main.py Speichern, Datei auf den ESP kopieren, Repl starten und Controller neustarten. Schon sehen wir im Terminal, welche Werte der Sensor hat. Ohne Berührung bewegt sich der Wert zwischen 400 und 600, mit Berührung grob zwischen 30 und 140. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.28" YPOS="17714.3710253998" OwnPage="40" ItemID="1680372777" PTYPE="4" WIDTH="475.28" HEIGHT="591.5189746002" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 591.519 L0 591.519 L0 0 Z" copath="M0 0 L475.28 0 L475.28 591.519 L0 591.519 L0 0 Z" gXpos="735.28" gYpos="11541.1410253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1680366457" BACKITEM="1677810857"/>
        <PAGEOBJECT XPOS="180" YPOS="18596.2610253998" OwnPage="41" ItemID="1680366457" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800565" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="180" gYpos="12423.0310253998" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1680372777"/>
