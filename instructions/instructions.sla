            <ITEXT FONT="DejaVu Sans Bold" CH="Ordnerinhalt auflisten "/>
            <para/>
            <ITEXT FONT="DejaVu Sans Mono Bold" CH="ls"/>
            <ITEXT CH=" (=list)"/>
            <para/>
            <para/>
            <ITEXT CH="Mit dem Befehl "/>
            <ITEXT FONT="DejaVu Sans Mono Bold" CH="ls"/>
            <ITEXT CH=" gibt man den Inhalt eines Ordners aus. Nur "/>
            <ITEXT FONT="DejaVu Sans Mono Bold" CH="ls"/>
            <ITEXT CH=", ohne Optionen, listet den Inhalt des aktuellen Arbeitsverzeichnisses, mit "/>
            <para/>
            <para/>
            <tab FONT="DejaVu Sans Mono Bold"/>
            <ITEXT FONT="DejaVu Sans Mono Bold" CH="$ ls /home/$USER/Bilder"/>
            <ITEXT CH=" "/>
            <para/>
            <para/>
            <ITEXT CH="kann man, unabhängig vom derzeitigen Arbeitsverzeichnis, zum Beispiel den Inhat des Ordners &quot;Bilder&quot; im home-Verzeichnis des aktuellen Benutzers (=$USER) ausgeben. "/>
            <para/>
            <para/>
