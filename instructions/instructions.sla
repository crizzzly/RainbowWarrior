        <PAGEOBJECT XPOS="735.28" YPOS="7500.64" OwnPage="16" ItemID="1645755001" PTYPE="4" WIDTH="475.28" HEIGHT="303.227949200376" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 303.228 L0 303.228 L0 0 Z" copath="M0 0 L475.28 0 L475.28 303.228 L0 303.228 L0 0 Z" gXpos="735.28" gYpos="7500.64" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Auf den ESP zugreifen"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Hierfür verwenden wir das Tool &quot;"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="rshell"/>
                <ITEXT CH="&quot; (= remote shell), das es uns ermöglicht, eine Verbindung zum Controller herzustellen um Dateien und Ordner hochzuladen, auszulesen, zu verschieben oder löschen, und um REPL zu starten. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Rshell benötigt einige Angaben, wie zum Beispiel die Puffergröße (--buffer-size=30) und den Port (-p /dev/ttyUSB0), wobei hier der vorher ermittelte Port (/dev/ttyUSB0) anzugeben ist. Bitte hier den richtigen Port für dein System verwenden!"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="repl"/>
                <ITEXT CH=" (= Read–eval–print loop) wiederum ist dazu da, um per Kommandozeile Befehle auf dem Board auszuführen, kleine Funktionen zu erstellen und diese, oder andere vorhandene Skripte auszuführen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="$ rshell --buffer-size=30 -p /dev/ttyUSB0 -a -e nano "/>
                <para PARENT="code"/>
                <ITEXT CH="$ repl"/>
                <para PARENT="code"/>
                <para/>
                <para/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Auf den ESP zugreifen" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="180" YPOS="8180.58102539981" OwnPage="17" ItemID="1645703081" PTYPE="4" WIDTH="475.28" HEIGHT="383.698974600187" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 383.699 L0 383.699 L0 0 Z" copath="M0 0 L475.28 0 L475.28 383.699 L0 383.699 L0 0 Z" gXpos="180" gYpos="8180.58102539981" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Micropython "/>
                <para PARENT="Überschrift Mittel"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Module"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Modulare Programmierung ist eine Software-Design-Technik, die auf dem allgemeinen Prinzip des modularen Designs beruht. Unter modularem Design versteht man, dass man ein komplexes System in kleinere selbständige Einheiten oder Komponenten zerlegt. Diese Komponenten bezeichnet man üblicherweise als Module. Ein Modul kann unabhängig vom Gesamtsystem erzeugt und separat getestet werden. In den meisten Fällen kann man ein Modul auch in anderen Systemen verwenden. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Modul: machine"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um in Micropython unter anderem die Pins anzusteuern benötigt man das Modul &quot;"/>
                <ITEXT FONT="DejaVu Sans Book" CH="machine"/>
                <ITEXT CH="&quot;.  Eigentlich handelt es sich bei &quot;machine&quot; um ein Modul, das sogar weitere Module, beziehungsweise sogenannte &quot;Klassen&quot; beinhaltet. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="mit dem machine-Modul kann man zum Beispiel die Prozessorgeschwindigkeit in MHz auslesen oder einstellen ("/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH="machine.freq([speed in MHz])"/>
                <ITEXT CH=") oder den Controller neu starten ("/>
                <ITEXT FONT="DejaVu Sans Mono Book" CH="machine.reset()"/>
                <ITEXT CH=")."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Außerdem beinhaltet es das für uns wichtige Modul, bzw. die Klasse &quot;Neopixel&quot;, mit dem wir den Strip konfigurieren und ansteuern, also zum leuchten bringen können. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Micropython" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="153" YPOS="12696.7" OwnPage="27" ItemID="1648004857" PTYPE="12" WIDTH="511.28" HEIGHT="229.139999999999" FRTYPE="0" CLIPEDIT="0" ANNAME="Gruppe7" groupWidth="511.28" groupHeight="229.139999999999" groupClips="1" path="M0 0 L511.28 0 L511.28 229.14 L0 229.14 L0 0 Z" copath="M0 0 L511.28 0 L511.28 229.14 L0 229.14 L0 0 Z" gXpos="153" gYpos="12696.7" gWidth="0" gHeight="0" LAYER="0">
            <PAGEOBJECT XPOS="153" YPOS="12696.7" OwnPage="27" ItemID="1648006873" PTYPE="4" WIDTH="475.28" HEIGHT="211.5" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 211.5 L0 211.5 L0 0 Z" copath="M0 0 L475.28 0 L475.28 211.5 L0 211.5 L0 0 Z" gXpos="0" gYpos="0" gWidth="511.28" gHeight="229.139999999999" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
                <StoryText>
                    <DefaultStyle/>
                    <ITEXT CH="https://www.python-kurs.eu/kurs.php"/>
                    <para PARENT="Default Paragraph Style"/>
                    <ITEXT CH="https://www.python-kurs.eu/klassen.php"/>
                    <para PARENT="Default Paragraph Style"/>
                    <ITEXT CH="https://github.com/loboris/MicroPython_ESP32_psRAM_LoBo/wiki/machine"/>
