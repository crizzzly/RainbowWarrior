        <PAGEOBJECT XPOS="170.8" YPOS="82988.46" OwnPage="94" ItemID="1988509713" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="82988.46" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1988515105" BACKITEM="1985688177"/>
        <PAGEOBJECT XPOS="170.8" YPOS="83870.35" OwnPage="95" ItemID="1988515105" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="83870.35" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1988509713"/>
        <PAGEOBJECT XPOS="170.8" YPOS="84752.24" OwnPage="96" ItemID="1992483665" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="84752.24" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1983191425" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Systemzeit und Datum über die Webseite einstellen "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Um die Systemzeit einstellen zu können, brauchen wir neben der Webform, über die die Zeit und das Datum eingegeben werden kann, in main.py eine Routine, die die Werte engegennimmt und RTC initialisiert. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Also nehmen wir uns wieder die HTML-Seite. Die Webform platzieren wir direkt unter dem Abschnitt mit der Systemzeit. Hier sollen zwei Textfelder (für Datum und Uhrzeit) sowie ein Button erscheinen. Aus optischen Gründen sollen diese aber - ähnlich wie beim Dropdown-Menü und dem zugehörigen Button - in einer Reihe platziert werden. Daher ist bei class &quot;form-inline&quot; anzugeben. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&lt;form class=&quot;form-inline&quot; method=&quot;POST&quot; action=&quot;/&quot; role=&quot;form&quot;&gt;"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="&lt;/form&gt;"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die zwei Textfelder und der Button in der Form werden in eine &lt;div&gt;-Tag zusammengefasst"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="  &lt;div class=&quot;input-group&quot;&gt;"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <ITEXT CH="  &lt;/div&gt;"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Innherhalb dieses &lt;div&gt;-Tags fügen wir nun zuerst unsere Textfelder ein. Textfelder werden durch &lt;input&gt;-Tags erstellt. Zu jedem &lt;input&gt;-Tag gehört noch ein &lt;label&gt;, das vor &lt;input&gt; platziert werden soll. Dies ist wichtig, da manche Browser sonst Probleme mit der Darstellung haben."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wie auch beim Sound und den Animationen sind die wichtigsten Schlüsselwörter &quot;name&quot; und &quot;value&quot;. Im erste &lt;input&gt;-Tag geben wir also bei name &quot;date&quot; ein,  im zweiten &quot;time&quot;. Als Value wird beim Absenden der Form die Nutzereingabe an der Stelle gesetzt. Optional können wir einen placeholder, also Platzhalter definieren. Hier können wir einfach die Platzhalter nutzen, die wir zur Anzeige der Systemzeit genutzt haben."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&lt;label class=&quot;sr-only&quot; for=&quot;inlineFormInputDate&quot;&gt;Datum&lt;/label&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="    &lt;input type=&quot;text&quot; name=&quot;date&quot; class=&quot;form-control mb-2&quot; id=&quot;inlineFormInputDate&quot; placeholder=&quot;{mday:02.02d}.{month:02.02d}.{year:02.02d}&quot; name=&quot;date&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="    &lt;label class=&quot;sr-only&quot; for=&quot;inlineFormInputTime&quot;&gt;Uhrzeit&lt;/label&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="    &lt;input type=&quot;text&quot; name=&quot;time&quot; class=&quot;form-control mb-2&quot; id=&quot;inlineFormInputTime&quot; placeholder=&quot;{hour:02.02d}:{minute:02.02d}&quot;&gt;"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Fehlt nur noch der Button. Dieser muss zwischen den Textfeldern und dem schließenden &lt;div&gt;-Tag platziert und in einem eigenen &lt;div&gt;-Tag liegen. Auch hier sind die Schlüsselworte &quot;name&quot; und &quot;value für die Weiterverarbeitung wichtig."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wie auch bei Sound und Animationen werden wir zuerst prüfen, welcher Button gedrückt wird. Daher müssen wir bei name wieder &quot;button&quot; angeben, als value setzen wir &quot;datetime&quot;"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="    &lt;div class=&quot;input-group-append&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="      &lt;button type=&quot;submit&quot; class=&quot;btn btn-dark mb-2&quot; name=&quot;button&quot; value=&quot;datetime&quot;&gt;Zeit einstellen&lt;/button&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="    &lt;/div&gt;"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Zusammengesetzt sieht das dann so aus:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&lt;form class=&quot;form-inline&quot; method=&quot;POST&quot; action=&quot;/&quot; role=&quot;form&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="  &lt;div class=&quot;input-group&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="    &lt;label class=&quot;sr-only&quot; for=&quot;inlineFormInputDate&quot;&gt;Datum&lt;/label&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="    &lt;input type=&quot;text&quot; name=&quot;date&quot; class=&quot;form-control mb-2&quot; id=&quot;inlineFormInputDate&quot; placeholder=&quot;{mday:02.02d}.{month:02.02d}.{year:02.02d}&quot; name=&quot;date&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="    &lt;label class=&quot;sr-only&quot; for=&quot;inlineFormInputTime&quot;&gt;Uhrzeit&lt;/label&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="    &lt;input type=&quot;text&quot; name=&quot;time&quot; class=&quot;form-control mb-2&quot; id=&quot;inlineFormInputTime&quot; placeholder=&quot;{hour:02.02d}:{minute:02.02d}&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="    &lt;div class=&quot;input-group-append&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="      &lt;button type=&quot;submit&quot; class=&quot;btn btn-dark mb-2&quot; name=&quot;button&quot; value=&quot;datetime&quot;&gt;Zeit einstellen&lt;/button&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="    &lt;/div&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="  &lt;/div&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="&lt;/form&gt;"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="In main.py müssen wir nun zuerst eine Instanz von RTC erstellen und der Variable rtc zuweisen. Dies geschieht wieder am Anfang der File: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="rtc = machne.RTC()"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Anschließend gehen wir an die Stelle, an der die Server-Messages emfangen werden und setzen eine weitere Bedingung ein, die prüft, ob der Wert an der Stelle 'button' &quot;datetime&quot; entspricht. Wenn dies zutrifft, soll rtc initialisiert werden. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="elif values_dict[&quot;button&quot;] == &quot;datetime&quot;:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="#Systemzeit initialisieren"/>
                <para PARENT="Kommentare - Zeilen"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="RTC erwartet zur Initialisierung höchstens 6 Werte (year, month, day [,hour [,minute [, second ]]]), wir haben aber in values_dict zwei Strings mit Datum (dd.mm.yyy) und Uhrzeit (hh:mm). Diese müssen vor der Initialisierung angepasst werden. Um die Endlsschleife übersichtlich zu halten, lagern wir dies in eine Funktion aus: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Innerhalb dieser Funktion müssen wir zuerst Datum und Uhrzeit in die einzelnen Werte splitten. Damit erhalten wir eine Liste mit den Werten, die allerdings immernoch als String vorliegen. Damit diese von RTC.init() verarbeitet werden können, müssen wir sie noch in Integer konvertieren. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def setSystemTime(date, time): "/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="&quot;&quot;&quot;converts date and time (String) in integer values and sets system time"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="date: date as String (dd.mm.yy)"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="time: time as String (hh:mm)"/>
                <tab/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="&quot;&quot;&quot;"/>
                <para PARENT="Kommentare - Block"/>
                <tab/>
                <ITEXT CH="date_list = list(int(i) for i in date.split(&quot;.&quot;))"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="date = tuple(date_list.reverse())"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="time = tuple(int(i) for i in time.split(&quot;:&quot;))"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="rtc.init(date+time)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="date_list = int(i) for i in date.split(&quot;.&quot;)"/>
                <ITEXT CH="  kombiniert drei Schritte: Teilt den String an jeder Stelle, an der &quot;.&quot; vorkommt und konvertiert jeden dieser Teile in einen Integer-Wert. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Da dieser Schritt aber einen Generator zurückgibt, wird diese Rückgabe mit list() in eine Liste konvertiert. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="date = tuple(date_list.reverse())"/>
                <ITEXT CH=" kombiniert zwei Schritte: date_list.reverse() kehrt die Reihenfolge der Elemente in der Liste um (von dd, mm,yyyy in yyyy, mm, dd) und wandelt die Liste in ein Tupel (reverse kann man leider nicht auf Tupel anwenden) "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="time = tuple(int(i) for i in time.split(&quot;:&quot;))"/>
                <ITEXT CH=" kombiniert ebenso drei Schritte: Teilt den String an jeder Stelle, an der &quot;:&quot; vorkommt und konvertiert jeden dieser Teile in einen Integer-Wert. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="tuple() konvertiert diese Werte in ein Tupel."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="rtc.init(date+time)"/>
                <ITEXT CH="  kombiniert auch zwei Schritte: date+time bewirkt, dass die beiden Tupel date und time aneinandergehängt werden, die dann die RTC initialisieren. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Diese Funktion wird dann in der Endlosschleife folgendermaßen aufgerufen: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <tab/>
                <ITEXT CH="elif values_dict[&quot;button&quot;] == &quot;datetime&quot;:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="#initialize system time"/>
                <para PARENT="Kommentare - Zeilen"/>
                <tab/>
                <tab/>
                <ITEXT CH="setSystemTime(values_dict[&quot;date&quot;], values_dict[&quot;time&quot;]"/>
                <para PARENT="code"/>
                <para PARENT="Kommentare - Zeilen"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Systemzeit über die Webseite einstellen" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="170.8" YPOS="85634.13" OwnPage="97" ItemID="1983191425" PTYPE="4" WIDTH="453.68" HEIGHT="714.389999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" copath="M0 0 L453.68 0 L453.68 714.39 L0 714.39 L0 0 Z" gXpos="170.8" gYpos="85634.13" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1997506945" BACKITEM="1992483665"/>
        <PAGEOBJECT XPOS="170.8" YPOS="62331.75" OwnPage="70" ItemID="1983194705" PTYPE="4" WIDTH="453.68" HEIGHT="205.739999999969" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L453.68 0 L453.68 205.74 L0 205.74 L0 0 Z" copath="M0 0 L453.68 0 L453.68 205.74 L0 205.74 L0 0 Z" gXpos="170.8" gYpos="62331.75" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="2066904561" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Aufbau einer HTML-Seite "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="HTML ist eine „Strukturierungssprache“, mit der dem Browser „gesagt“ wird, wie die Inhalte strukturiert sind: welche Bereiche (Buchstaben, Wörter, Sätze) z.B. Überschriften, was Absätze, was Aufzählungen, was Tabellen usw. sind. "/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="HTML ist keine Programmiersprache – man beschreibt, welche logische Struktur ein Inhalt hat (nicht mehr, nicht weniger)."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Jeder einzelne Bereich wird mit einem Tag definiert. Der Beginn des Bereichs (am Beispiel Absatz - (engl. paragraph)) wird mit einem öffnenden Tag &lt;p&gt;, das Ende des Bereichs mit einem schließenden Tag &lt;/p&gt; gekennzeichnet. "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Die verschiedenen Bereiche"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="1. DOCTYPE "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="&lt;! DOCTYPE html&gt; "/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Im Bereich DOCTYPE wird dem Internet-Browser mitgeteilt, was er an Befehlen erwarten kann und an welchem Standard man sich bei der Erstellung der Seite gehalten hat."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="2. HEAD "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="&lt;head&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="  &lt;title&gt;Titel der Webseite&lt;/title&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="  &lt;link rel=&quot;stylesheet&quot; href=&quot;style.css&quot;&gt;"/>
                <para PARENT="code"/>
                <ITEXT CH="&lt;/head&gt;"/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Im Bereich HEAD stecken die Metainformationen über die Seite, also beispielsweise der Titel der Seite, der im Browserfensterkopf angezeigt wird sowie Links zur CSS-File."/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="CSS (Cascading Style Sheet) ist für das Aussehen der der Webseite verantwortlich"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="3. BODY"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="&lt;body&gt;"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="..."/>
                <para PARENT="code"/>
                <ITEXT CH="&lt;/body&gt; "/>
                <para PARENT="code"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Im Bereich BODY steckt der eigentliche Inhalt der Seite und die HTML-TAGs."/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <ITEXT CH="Mehr Informationen hierzu inklusive einem Tutorial findet man unter anderem hier: "/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Oblique" CH="https://www.html-seminar.de/einsteiger.htm"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Oblique" CH="https://www.html-seminar.de/html-grundlagen.htm"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Oblique" CH="https://www.html-seminar.de/html-seitenaufbau.htm"/>
                <para PARENT="erklärungen"/>
                <ITEXT FONT="DejaVu Sans Oblique" CH="https://www.html-seminar.de/css-lernen.htm "/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
                <para PARENT="erklärungen"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Aufbau einer HTML-Seite" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
