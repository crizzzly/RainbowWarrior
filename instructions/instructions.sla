        <PAGEOBJECT XPOS="735.28" YPOS="8013.58102539981" OwnPage="18" ItemID="1648498649" PTYPE="4" WIDTH="475.28" HEIGHT="672.176923800564" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" copath="M0 0 L475.28 0 L475.28 672.177 L0 672.177 L0 0 Z" gXpos="735.28" gYpos="8013.58102539981" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="1642931513"/>
        <PAGEOBJECT XPOS="180" YPOS="8895.47102539981" OwnPage="19" ItemID="1650486345" PTYPE="4" WIDTH="475.28" HEIGHT="479.308974600187" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 479.309 L0 479.309 L0 0 Z" copath="M0 0 L475.28 0 L475.28 479.309 L0 479.309 L0 0 Z" gXpos="180" gYpos="8895.47102539981" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="LEDs leuchten lassen "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Mono Bold" CH="np.set( pos, color [, white, num, update] )  "/>
                <ITEXT FONT="DejaVu Sans Book" CH="ist dafür zuständig, die Pixels leuchten zu lassen. Auch hier sind zwei Angaben zwingend erforderlich, alle anderen (in der eckigen Klammer) optional."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="pos"/>
                <ITEXT CH=" gibt an, welcher Pixel genau gesetzt werden soll"/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="color"/>
                <ITEXT CH=" gibt - wie der Name schon sagt - an, in welcher Farbe der Pixel leuchten soll. Hier kann man entweder vordefinierte Farben verwenden (wie im folgenden Beispiel), oder Farbwerte als Hexadezimalwert (wird später noch erklärt)"/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="white"/>
                <ITEXT CH=" (optional) gibt - bei RGBW-Strips - an, wie hell die Farbe weiß in diesem Pixel leuchten soll. Wird hier nichts angegeben, wird als Standardwert 0 gesetzt. Hier sind Werte von 0-255 möglich. Je höher, desto mehr weiß wird in die Farbe gemischt. "/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="num"/>
                <ITEXT CH=" (optional) definiert, wieviele Pixel in diesem Schritt gesetzt werden sollen, ausgehend vom Pixel, der in pos angegeben wurde. Standardwert ist her 1, also wird standardgemäß nur der eine Pixel gesteuert, der in num angegeben wurde. "/>
                <para PARENT="Liste"/>
                <ITEXT FONT="DejaVu Sans Bold" CH="update"/>
                <ITEXT CH=" (optional) bestimmt, ob der oder die Pixel sofort in der jeweiligen Farbe angezeigt werden soll, bzw sollen (was auch Standardwert ist), oder ob mit der Anzeige gewartet werden soll. Falls abgewartet werden sollte, ist update=False anzugeben."/>
                <para PARENT="Liste"/>
                <ITEXT CH="Um dann allerdings die Pixel in der vorher gesetzten Farbe leuchten zu lassen, ist zusätzlich noch der Befehl strip.show() nötig."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; strip.set(0, strip.NAVY) "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="lässt also den ersten Pixel des Strips in der Farve &quot;Navy&quot; leuchten, während "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="&gt;&gt;&gt; strip.set(0, strip.NAVY, num=num_pixels) "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="alle Pixel in der Farbe &quot;Navy&quot; leuchten lässt"/>
                <trail PARENT="Default Paragraph Style"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="180" YPOS="9373.9" OwnPage="19" ItemID="1648083785" PTYPE="4" WIDTH="475.28" HEIGHT="193.747949200376" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 193.748 L0 193.748 L0 0 Z" copath="M0 0 L475.28 0 L475.28 193.748 L0 193.748 L0 0 Z" gXpos="180" gYpos="9373.9" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1627426409" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="12" CH="Farben "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Wie bereits erwähnt kann man die Farbe aus vordefinierten Farben, oder als Hexadezimalwert angeben."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Um herauszufinden, welche Farben verfügbar sind, gibt es einen kleinen Trick: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Gib einfach &quot;strip.&quot; in REPL ein und drücke einmal auf TAB."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="So erscheint eine Auflistung aller vordefinierten Werte und sogar auch der vorhandenen Funktionen.  Die vordefinierten Werte werden in Großbuchstaben angezeigt. So sind folgende Farbwerte bereits definiert: BLACK, WHITE, RED, LIME, BLUE, YELLOW, CYAN, MAGENTA, SILVER, GRAY, MAROON, OLIVE, GREEN, PURPLE, TEAL, NAVY. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Alternativ hat man die Möglichkeit, die Farbe anhand des Hexadezimalwerts anzugeben. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="RGB-Farben werden angegeben durch einen Wert für rot (=r), einen für grün (=g) und einen für blau (=b). Dieser Wert darf zwischen 0 und 255 liegen. Für die Farbe rot muss also r=255, sprich höchster Wert, g=0, b=0. Anders geschrieben: (255, 0, 0)."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Dies ist der sogenannte RGB-Wert als Tupel dargestellt."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Der Hexadezimalwert wäre für rot #FF0000, bzw. 0xFF0000, wobei die Zeichen &quot;#&quot; oder &quot;0x&quot; nur vorangestellt werden, um zu zeigen, dass es sich hierbei um einen Hexadezimalwert handelt. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Wandeln wir die Dezimalzahl &quot;255&quot; in eine Hexadezimalzahl erhalten wir &quot;FF&quot;. Der Hexadezimalwert einer Farbe ist also nichts weiter, als die jeweiligen Farbwerte für r, g und b, konvertiert in eine Hexadezimalzahl, aneinandergereiht. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Die Farbe &quot;aquamarin&quot; hat zum Beispiel folgenden RGB-Wert: (127,255,212). Konvertieren wir die einzelnen Werte in Hexadezimalwerte erhalten wir 0x7FFFD4 (also 127 = 0x7F, 255 = 0xFF, 212 = 0xD4. Konvertieren wir wiederum den kompletten Hexadezimalwert in einen Dezimalwert erhalten wir 8388564. Beide Werte sind hier als Farbangabe zulässig. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Möchten wir jedoch ein RGB-Tupel, also hier (127, 255, 212) als Angabe verwenden, müssen wir dieses erst in den entsprechenden Hexadezimalwert konvertieren."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Wollen wir die Farbe Schwarz, was im additiven Farbsystem, also dem Farbsystem unseres sichtbaren Lichts soviel wie &quot;kein Licht&quot; bedeutet, müssen wir einfach alle Farben auf 0 setzen. Dies geht indem man für color den Wert &quot;0x00&quot; angibt. also 0."/>
                <para/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Farben" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="180" YPOS="9777.36102539981" OwnPage="21" ItemID="1653258873" PTYPE="4" WIDTH="475.28" HEIGHT="279.91897460019" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 279.919 L0 279.919 L0 0 Z" copath="M0 0 L475.28 0 L475.28 279.919 L0 279.919 L0 0 Z" gXpos="180" gYpos="9777.36102539981" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Animationen Teil 1: Blinken"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para/>
                <ITEXT CH="Bevor wir die LEDs blinken lassen können, müssen wir uns überlegen, was hier eigentlich genau geschieht: "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Die LEDs werden alle zur gleichen Zeit angeschalten, leuchten eine Weile, anschließend werden sie ausgeschalten. Nach einer Weile gehen sie wieder an, und so weiter. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wenn ich die LEDs also zweimal blinken lassen möchte, könnte ich einfach schreiben: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="mach an – warte – mach aus – warte – mach an – warte – mach aus. "/>
                <para PARENT="Default Paragraph Style" ALIGN="1"/>
                <para PARENT="Default Paragraph Style" ALIGN="1"/>
                <ITEXT CH="Nur. Das wäre ja ganz schön doof. Wollten wir die LEDs auf diese Weise 1000x blinken lassen, hätten wir mittels copy + paste einiges zu tun! Außerdem würde das Programm unübersichtlich werden! "/>
                <para PARENT="Default Paragraph Style" ALIGN="0"/>
                <ITEXT CH="Viel besser wäre es doch wenn wir sagen könnten:"/>
                <para/>
                <para/>
                <ITEXT CH=" mach bitte 5x: "/>
                <para ALIGN="1"/>
                <ITEXT CH="mach an – warte – mach aus – warte. "/>
                <trail PARENT="Default Paragraph Style" ALIGN="1"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Animationen Teil 1: Blinken" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="180" YPOS="10069.2879492004" OwnPage="21" ItemID="1651862329" PTYPE="4" WIDTH="475.28" HEIGHT="380.249999999996" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 380.25 L0 380.25 L0 0 Z" copath="M0 0 L475.28 0 L475.28 380.25 L0 380.25 L0 0 Z" gXpos="180" gYpos="10069.2879492004" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1656430761" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="12" CH="Schleifen"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Aus diesem Grund gibt es Schleifen. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Mit Schleifen kann man zum Beispiel festlegen, dass bestimmte Codezeilen genau 5, 10, 1000 Mal ausgeführt werden sollen ("/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="for-Schleife"/>
                <ITEXT FONTSIZE="12" CH="). Außerdem kann man auch sagen, dass ein Code-Block, wie bei uns das Blinken, nur ausgeführt werden soll, während es draußen dunkel ist ("/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="while-Schleife"/>
                <ITEXT FONTSIZE="12" CH=")."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Zum Beispiel: während es draußen dunkel ist: blinke, checke obs immernoch dunkel ist. wenn dunkel: nochmal das ganze! Wenn hell, abbrechen. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Auch könnte man mit einer while-Schleife sagen: mache das einfach immer wieder! Eine sogenannte Endlos-Schleife. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="For-Schleife "/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Um zu sagen: führe diesen Codeblock 5x aus schreibt man in Python: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="&gt;&gt;&gt; for i in range(5): "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="range(5) ist eine Funktion, die  nacheinander alle ganzen Zahlen von 0 bis 4(!!!) ausgibt. Genauer sollte man eigentlich schreiben: range(0, 5). Warum 4 und nicht 5? 5 gibt hier das obere Limit der Zahlenfolge, sowie die Anzahl der Zahlen an. Also bedeutet range(5) (bzw range(0,5)), dass eine Zahlenreihe, bestehend aus 5 ganzen Zahlen, beginnend bei 0, endend bei &lt; 5 - also 4, zurückgegeben wird. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Bedeutet wiederum: dieser Codeblock wird 5x ausgeführt. Für jedes i von 0 bis &lt; 5. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Aber -  was befindet sich in diesem Codeblock - was außerhalb?"/>
                <breakline/>
                <ITEXT FONTSIZE="12" CH="Dies wird in Python durch Einrückungen definiert."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Sprich alles, was hier 5x ausgeführt werden soll, muss nun im Vergleich Zeilenanfang der for-Schleife einmal mit Tab eingerückt sein."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Schleifen" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
                <ItemAttribute Name="Inhalt" Type="none" Value="For-Schleife" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.28" YPOS="9874.39000000021" OwnPage="22" ItemID="1654741849" PTYPE="4" WIDTH="475.28" HEIGHT="575.147949200168" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="11" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L475.28 0 L475.28 575.148 L0 575.148 L0 0 Z" copath="M0 0 L475.28 0 L475.28 575.148 L0 575.148 L0 0 Z" gXpos="0" gYpos="978.918974600399" gWidth="475.28" gHeight="672.176923800567" LAYER="0" NEXTITEM="1656709081" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="11.8" CH="REPL erledigt das für uns, sobald man die Zeile &quot;for i in range(5): &quot; mit einem Doppelpunkt beendet und einmal Enter gedrückt hat. Der Cursor ist nun eingerückt, zudem hat sich wieder der Zeilenanfang verändert. Hier stehen nun &quot;"/>
                <ITEXT FONT="DejaVu Sans Mono Bold" FONTSIZE="11.8" CH="..."/>
                <ITEXT FONTSIZE="11.8" CH="&quot;:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="Auch wenn wir mit Enter in die nächste Zeile wechseln, bleibt dieser noch auf selber Position. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="Um die For-Schleife mit der Range-Funktion mal zu demonstrieren, sagen wir: für jedes i von 0 bis &lt;5 : gib einmal den Wert von i aus. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="12" CH="Um Werte (oder Texte) im Terminal auszugeben nutzt man die Funktion print([variable / Text (in Anführungszeichen)])"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="&gt;&gt;&gt; for i in range(5): "/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="11.8" CH="... "/>
                <tab FONTSIZE="11.8"/>
                <tab FONTSIZE="11.8"/>
                <ITEXT FONTSIZE="11.8" CH="print(i)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="Zweimaliges Drücken der Enter-Taste lässt den Cursor wieder an den Anfang der Zeile stellen, sprich, falls hier noch etwas gemacht werden sollte, dann wird das erst ausgeführt, sobald die Schleife verlassen, also 5x ausgeführt wurde. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="Da wir nach der Schleife nichts weiter ausführen lassen wollen, können wir nochmals mit Enter bestätigen, sodass die Schleife ausgeführt werden kann. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="Das ging nun allerdings sehr sehr schnell! Wenn wir in diesem Tempo unsere LEDs blinken lassen würden, würden wir das Blinken sehr wahrscheinlich gar nicht mehr wirklich wahrnehmen! "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="Um diesen Prozess zu verlangsamen, müssen wir sogenannte delays einbauen. "/>
                <breakline/>
                <ITEXT FONTSIZE="11.8" CH="Dies macht man mit dem Modul &quot;"/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="11.8" CH="utime"/>
                <ITEXT FONTSIZE="11.8" CH="&quot;"/>
                <ITEXT FONTSIZE="12" CH=", bzw. dessen Funktion "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="sleep_ms()"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONT="DejaVu Sans Mono Bold" FONTSIZE="11.8" CH="utime.sleep_ms(200)"/>
                <ITEXT FONTSIZE="11.8" CH=" sorgt dafür, dass die nächste Codezeile (oder das nächste Mal, dass die Schleifenbedingung geprüft wird) erst 200 ms später stattfindet. Gib nun folgende Zeilen ins Terminal und bestätige - wie eben schonmal - 3x mit Enter."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="&gt;&gt;&gt; import utime"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="11.8" CH="&gt;&gt;&gt; for i in range(5):"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="11.8" CH="... "/>
                <tab FONTSIZE="11.8"/>
                <tab FONTSIZE="11.8"/>
                <ITEXT FONTSIZE="11.8" CH="print(i)"/>
                <para PARENT="code"/>
                <ITEXT FONTSIZE="11.8" CH="... "/>
                <tab FONTSIZE="11.8"/>
                <tab FONTSIZE="11.8"/>
                <ITEXT FONTSIZE="11.8" CH="utime.sleep_ms(200)"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="Wie du siehst erscheinen nun die Ergebnisse der einzelnen Schleifendurchläufe in zeitlichen Abständen auf dem Terminal. Selbes Verhalten wollen wir für unsere blinkenden LEDs! "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT FONTSIZE="11.8" CH="REPL erledigt das für uns, sobald man die Zeile &quot;for i in range(5): &quot; mit einem Doppelpunkt beendet und einmal Enter gedrückt hat. Der Cursor ist nun eingerückt, zudem hat sich wieder der Zeilenanfang verändert. Hier stehen nun &quot;"/>
                <ITEXT FONT="DejaVu Sans Mono Bold" FONTSIZE="11.8" CH="..."/>
                <ITEXT FONTSIZE="11.8" CH="&quot;:"/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.28" YPOS="9935.94774159019" OwnPage="22" ItemID="1653811401" PTYPE="2" WIDTH="475.28" HEIGHT="52.1254121858553" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" TEXTFLOWMODE="2" LOCALSCX="0.75" LOCALSCY="0.725980671112211" LOCALX="0.706666666666621" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" Pagenumber="0" PFILE="../../../../Schreibtisch/pics-projekt/groß/for-schleife1.png" IRENDER="0" EMBEDDED="0" path="M0 0 L475.28 0 L475.28 52.1254 L0 52.1254 L0 0 Z" copath="M0 0 L475.28 0 L475.28 52.1254 L0 52.1254 L0 0 Z" ImageRes="0" gXpos="0" gYpos="1040.47671619038" gWidth="475.28" gHeight="672.176923800567" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
