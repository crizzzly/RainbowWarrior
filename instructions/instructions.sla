            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Zwei ähnliche Routinen in einer Funktion zusammenfassen"/>
                <para PARENT="Überschrift - normal-fett"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun haben wir zwei Routinen in main.py, die im Prinzip ähnliches tun: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Wenn eine Animation im Thread läuft wird diese gestoppt, nach einem kurzen Delay dann eine neue im Thread gestartet. Dies geschieht entweder mit animations.next oder mit animations.start."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Diese sollten wir in einer Funktion - handleAnimations - zusammenfassen."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nachdem der animation_thread gestoppt wurde, müssen wir zunächst prüfen, ob der Funktion ein Animations-name übergeben wurde (falls die Animation auf der Website ausgewählt wurde) oder nicht (falls sie durch den Touch-Sensor gewechselt wurde). "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="In Abhängigkeit dessen müssen wir nun den Wert von sleep_ms wählen und die Animation mit der richtigen Funktion in animations.py (next oder start) starten."/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="def handleAnimations(animation_name=None):"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="if _thread.status(animation_thread) != _thread.TERMINATED:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH=" _thread.notify(animation_thread, _thread.EXIT)"/>
                <para PARENT="code"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="if animation_name == None:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(50)"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="animation_thread = _thread.start_new_thread(&quot;animation&quot;, "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="animations.next, () )"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="else:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="print(&quot;starting animation {}&quot;.format(animation_name))"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="utime.sleep_ms(wait_before_start_thread)"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="animation_thread = _thread.start_new_thread(&quot;animation&quot;, "/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH="animations.start, (animation_name,)) "/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun stehen wir aber vor einem Problem: Diese Funktion wird nur fehlerfrei ausgeführt, wenn bereits eine Animation im Thread gestartet und die Thread-ID in animation_thread gespeichert wird. Beim ersten Aufruf allerdings wird es sofort einen Fehler geben, da wir eine Prüfung von animation_thread vornehmen, bevor die Variable deklariert wurde!"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Dieser Fehler lässt sich durch einen try / except-Block abfangen, wobei im except-Block keine weitere Anweisung ausgeführt werden muss. Ein leerer except-Block wird allerdings erneut zu einem Fehler führen. Dies kann man verhindern, indem man das Schlüsselwort &quot;pass&quot; einsetzt:"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="try:"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="if _thread.status(animation_thread) != _thread.TERMINATED:"/>
                <para PARENT="code"/>
                <tab/>
                <tab/>
                <ITEXT CH=" _thread.notify(animation_thread, _thread.EXIT)"/>
                <para PARENT="code"/>
                <ITEXT CH="except NameError:"/>
                <para PARENT="code"/>
                <tab/>
                <ITEXT CH="pass"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Nun können wir entsprechende Zeilen in der Touch Sensor-Verarbeitung ersetzen durch: "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="handleAnimations()"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="in der Verarbeitung der Server-Messages durch"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="handleAnimations(values[1])"/>
                <para PARENT="code"/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH=" "/>
                <trail PARENT="Default Paragraph Style"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="Inhalt" Type="none" Value="Zwei ähnliche Routinen in einer Funktion zusammenfassen" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
